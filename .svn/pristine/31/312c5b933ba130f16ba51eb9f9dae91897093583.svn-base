    <script type="text/javascript">
    var _sBaseAddress="";
    var _oDUClaimOrders=[];
    var _sContractorIds="";
    var _sMenuID=0;
    var _nBUID = 0;
    var _oDUOrderSetups=[];
    var _oClaimOrderTypes=[];
   var _oAuthorizationRolesMapping=[];
  
    $(document).ready(function () {
        _sBaseAddress = @Html.Raw(new System.Web.Script.Serialization.JavaScriptSerializer().Serialize(HttpContext.Current.Session[SessionInfo.BaseAddress]));
        _oDUClaimOrders =@Html.Raw( new System.Web.Script.Serialization.JavaScriptSerializer().Serialize(Model));
        _sMenuID =@Html.Raw( new System.Web.Script.Serialization.JavaScriptSerializer().Serialize(HttpContext.Current.Session[SessionInfo.MenuID]));
        _nBUID = @Html.Raw(new System.Web.Script.Serialization.JavaScriptSerializer().Serialize(ViewBag.BUID));
        _oCompareOperators = @Html.Raw(new System.Web.Script.Serialization.JavaScriptSerializer().Serialize(ViewBag.CompareOperators));
        _oDUOrderSetups =@Html.Raw(new System.Web.Script.Serialization.JavaScriptSerializer().Serialize(@ViewBag.DUOrderSetups));
        _oClaimOrderTypes =@Html.Raw(new System.Web.Script.Serialization.JavaScriptSerializer().Serialize(@ViewBag.ClaimOrderTypes));
        _oAuthorizationRolesMapping = @Html.Raw(new System.Web.Script.Serialization.JavaScriptSerializer().Serialize(HttpContext.Current.Session[SessionInfo.AuthorizationRolesMapping]));
        debugger;
        if(sessionStorage.getItem("DUClaimOrders")!=null && sessionStorage.getItem("DUClaimOrders").length>0){
            _oDUClaimOrders= jQuery.parseJSON(sessionStorage.getItem('DUClaimOrders'));
            var nIndex= sessionStorage.getItem('SelectedRowIndex');
            DynamicRefreshList(_oDUClaimOrders, 'tblDUClaimOrder');
            if(nIndex>-1){
                $('#tblDUClaimOrder').datagrid('selectRow',nIndex);
            }
        }
        else{
            DynamicRefreshList(_oDUClaimOrders, 'tblDUClaimOrder');
        }

        $("#progressbar").progressbar({ value: 0 });
        $("#progressbarParent").hide();
        $(".lblLoadingMessage").hide();
        LoadComboAdv();
        RefreshControlLayout();
    });

    $(document).keydown(function (e) { if (e.keyCode == 27) { $('#winDUClaimOrderOrder').icsWindow('close'); } });

    $('#tblDUClaimOrder').datagrid({ onSelect: function (rowIndex, rowData) { OperationPerforms(rowIndex, rowData); } });

    function OperationPerforms(rowIndex, rowData)
    {
        if (rowData != null && rowData.DUClaimOrderID > 0)
        {
            debugger;
            if (rowData.ApproveBy!=0  && rowData.CheckedBy!=0)  //Initialized = 1,
            {
                $('#btnEdit,#btnDelete,#btnChecked,#btnApprove,#btnRevise').hide();
                $('#btnCreateDO').show();
                if (PermissionChecker('Revise', 'DUClaimOrder',_oAuthorizationRolesMapping)) {$("#btnRevise").show();}
            }
            else if (rowData.ApproveBy==0 && rowData.CheckedBy==0 )
            {
                $('#btnEdit,#btnDelete,#btnApprove,#btnRevise,#btnCreateDO,#btnChecked').hide();
                //$('#btnApprove').show();
                if (PermissionChecker('Verify', 'DUClaimOrder',_oAuthorizationRolesMapping)) {$("#btnChecked").show();}
               
                $('#btnEdit').show();
            }
            else if (rowData.ApproveBy==0 && rowData.CheckedBy!=0 )
            {
                $('#btnEdit,#btnDelete,#btnApprove,#btnRevise,#btnCreateDO').hide();
                //$('#btnApprove').show();
                if (PermissionChecker('Approved', 'DUClaimOrder',_oAuthorizationRolesMapping)) {$("#btnApprove").show();}
                $('#btnRevise').show();
            }
            else
            {
                $('#btnApprove,#btnRevise,#btnCreateDO').hide();
                $('#btnEdit,#btnDelete').show();
            }
            
            
        }
    }

    function RefreshControlLayout()
    {
       
        $("#btnAdd,#btnEdit,#btnView,#btnDelete,#btnApprove,#btnIsClose,#btnCancel,#btnPrintListExcel,#btnPrintList").hide();

        if (PermissionChecker('Add', 'DUClaimOrder',_oAuthorizationRolesMapping)) {$("#btnAdd").show();}
        if (PermissionChecker('Edit', 'DUClaimOrder',_oAuthorizationRolesMapping)) {$("#btnEdit").show();}
        if (PermissionChecker('View', 'DUClaimOrder',_oAuthorizationRolesMapping)) {$("#btnView").show();}
        if (PermissionChecker('Delete', 'DUClaimOrder',_oAuthorizationRolesMapping)) {$("#btnDelete").show();}
        if (PermissionChecker('Issued', 'DUClaimOrder',_oAuthorizationRolesMapping)) {$("#btnChecked").show();}
        if (PermissionChecker('Approved', 'DUClaimOrder',_oAuthorizationRolesMapping)) {$("#btnApprove").show();}
        if (PermissionChecker('Revise', 'DUClaimOrder',_oAuthorizationRolesMapping)) {$("#btnRevise").show();}
        if (PermissionChecker('Close', 'DUClaimOrder',_oAuthorizationRolesMapping)) {$("#btnIsClose").show();}
        if (PermissionChecker('Cancel', 'DUClaimOrder',_oAuthorizationRolesMapping)) {$("#btnCancel").show();}
        if (PermissionChecker('PrintList', 'DUClaimOrder',_oAuthorizationRolesMapping)) {$("#btnPrintList").show();}
        if (PermissionChecker('PrintList', 'DUClaimOrder',_oAuthorizationRolesMapping)) {$("#btnPrintListExcel").show();}
    }
    /*------------------------------------*/

    $("#btnAdd").click(function(){
        debugger;
        sessionStorage.setItem("Operation", "New");
        sessionStorage.setItem("SelectedRowIndex", -1);
        sessionStorage.setItem("DOHeader", "Add Order");
        sessionStorage.setItem("DUClaimOrders", JSON.stringify($('#tblDUClaimOrder').datagrid('getRows')));
        sessionStorage.setItem("BackLink", window.location.href);
        var tsv=((new Date()).getTime())/1000;
        window.location.href = _sBaseAddress+ "/DUClaimOrder/ViewDUClaimOrder?nId=0&ts="+tsv;
    });

    $('#btnEdit').click(function (e)
    {

        var oDUClaimOrder = $('#tblDUClaimOrder').datagrid('getSelected');
        if (oDUClaimOrder ==null || oDUClaimOrder.DUClaimOrderID <=0 ) { alert("Please select an item from list."); return ; }
        var nIndex=$('#tblDUClaimOrder').datagrid('getRowIndex',oDUClaimOrder);
        if (oDUClaimOrder.ApproveBy!=0 )
        { alert("Already Approve,You can't Change it."); return ; }
        sessionStorage.setItem("Operation", "Edit");
        sessionStorage.setItem("SelectedRowIndex", nIndex);
        sessionStorage.setItem("DOHeader", "Edit Order");
        sessionStorage.setItem("DUClaimOrders", JSON.stringify($('#tblDUClaimOrder').datagrid('getRows')));
        sessionStorage.setItem("BackLink", window.location.href);
        var tsv=((new Date()).getTime())/1000;
        window.location.href = _sBaseAddress+ "/DUClaimOrder/ViewDUClaimOrder?nId="+oDUClaimOrder.DUClaimOrderID+"&ts="+tsv;

    });

    $('#btnRevise').click(function (e)
    {

        var oDUClaimOrder = $('#tblDUClaimOrder').datagrid('getSelected');
        if (oDUClaimOrder ==null || oDUClaimOrder.DUClaimOrderID <=0 ) { alert("Please select an item from list."); return ; }
        var nIndex=$('#tblDUClaimOrder').datagrid('getRowIndex',oDUClaimOrder);

        sessionStorage.setItem("Operation", "Revise");
        sessionStorage.setItem("SelectedRowIndex", nIndex);
        sessionStorage.setItem("DOHeader", "Revise Production");
        sessionStorage.setItem("DUClaimOrders", JSON.stringify($('#tblDUClaimOrder').datagrid('getRows')));
        sessionStorage.setItem("BackLink", window.location.href);
        var tsv=((new Date()).getTime())/1000;
        window.location.href = _sBaseAddress+ "/DUClaimOrder/ViewDUClaimOrder?nId="+oDUClaimOrder.DUClaimOrderID+"&ts="+tsv;

    });

    $('#btnDelete').click(function(e){
        var oDUClaimOrder = $('#tblDUClaimOrder').datagrid('getSelected');
        if (oDUClaimOrder ==null || oDUClaimOrder.DUClaimOrderID <=0 ) { alert("Please select an item from list."); return ; }
        if (oDUClaimOrder.ApproveBy!=0 )
        { alert("Already Approve,You can't Delete it."); return ; }
        if (!confirm("Confirm to delete?")) return;
        sessionStorage.clear();
        var obj =
        {
            BaseAddress: _sBaseAddress,
            Object: oDUClaimOrder,
            ControllerName: "DUClaimOrder",
            ActionName: "Delete",
            TableId: "tblDUClaimOrder",
            IsWinClose: false
        };
        $.icsDelete(obj);

    });

    $('#btnView').click(function (e)
    {
        var oDUClaimOrder = $('#tblDUClaimOrder').datagrid('getSelected');
        if (oDUClaimOrder ==null || oDUClaimOrder.DUClaimOrderID <=0 ) { alert("Please select an item from list."); return ; }

        var nIndex=$('#tblDUClaimOrder').datagrid('getRowIndex',oDUClaimOrder);

        sessionStorage.clear();
        sessionStorage.setItem("Operation", "View");
        sessionStorage.setItem("SelectedRowIndex", nIndex);
        sessionStorage.setItem("DOHeader", "View Order");
        sessionStorage.setItem("DUClaimOrders", JSON.stringify($('#tblDUClaimOrder').datagrid('getRows')));
        sessionStorage.setItem("BackLink", window.location.href);
        var tsv=((new Date()).getTime())/1000;
        window.location.href = _sBaseAddress+ "/DUClaimOrder/ViewDUClaimOrder?nId="+oDUClaimOrder.DUClaimOrderID+"&ts="+tsv;;
    });

    $('#btnApprove').click(function(e){
        debugger;
        var oDUClaimOrder = $('#tblDUClaimOrder').datagrid('getSelected');
        if (oDUClaimOrder ==null || oDUClaimOrder.DUClaimOrderID <=0 ) { alert("Please select an item from list."); return ; }

        var nIndex=$('#tblDUClaimOrder').datagrid('getRowIndex',oDUClaimOrder);
        sessionStorage.clear();
        sessionStorage.setItem("Operation", "Approve");
        sessionStorage.setItem("SelectedRowIndex", nIndex);
        sessionStorage.setItem("DOHeader", "Approve");
        sessionStorage.setItem("DUClaimOrders", JSON.stringify($('#tblDUClaimOrder').datagrid('getRows')));
        sessionStorage.setItem("BackLink", window.location.href);
        var tsv=((new Date()).getTime())/1000;
        window.location.href = _sBaseAddress+ "/DUClaimOrder/ViewDUClaimOrder?nId="+oDUClaimOrder.DUClaimOrderID+"&ts="+tsv;;

    });
    $('#btnChecked').click(function(e){
        debugger;
        var oDUClaimOrder = $('#tblDUClaimOrder').datagrid('getSelected');
        if (oDUClaimOrder ==null || oDUClaimOrder.DUClaimOrderID <=0 ) { alert("Please select an item from list."); return ; }

        var nIndex=$('#tblDUClaimOrder').datagrid('getRowIndex',oDUClaimOrder);
        sessionStorage.clear();
        sessionStorage.setItem("Operation", "Checked");
        sessionStorage.setItem("SelectedRowIndex", nIndex);
        sessionStorage.setItem("DOHeader", "Checked");
        sessionStorage.setItem("DUClaimOrders", JSON.stringify($('#tblDUClaimOrder').datagrid('getRows')));
        sessionStorage.setItem("BackLink", window.location.href);
        var tsv=((new Date()).getTime())/1000;
        window.location.href = _sBaseAddress+ "/DUClaimOrder/ViewDUClaimOrder?nId="+oDUClaimOrder.DUClaimOrderID+"&ts="+tsv;;

    });
    $('#btnCreateDO').click(function(e){
        debugger;


        var oDUClaimOrder = $('#tblDUClaimOrder').datagrid('getSelected');
        if (oDUClaimOrder ==null || oDUClaimOrder.DUClaimOrderID <=0 ) { alert("Please select an item from list."); return ; }
        if (oDUClaimOrder.ApproveBy==0 )
        { alert("Yet Not Approve,You can't create DO."); return ; }
        var  nSelectedIndex = $('#tblDUClaimOrder').datagrid('getRowIndex', oDUClaimOrder);

        if(!confirm("Confirm To Create Delivery Order(DO)")) return false;

        $.ajax({
            type: "POST",
            dataType: "json",
            url : _sBaseAddress+"/DUClaimOrder/DOSave_Auto",
            traditional: true,
            data:  JSON.stringify(oDUClaimOrder),
            contentType: "application/json; charset=utf-8",
            success: function (data) {
                var  oDUClaimOrder = jQuery.parseJSON(data);
                debugger;
                if (oDUClaimOrder.ErrorMessage == '' || oDUClaimOrder.ErrorMessage == null)
                {
                    debugger;

                    $('#tblDUClaimOrder').datagrid('updateRow', { index: nSelectedIndex, row: oDUClaimOrder });
                    alert("DO Create Successfully");
                }
                else
                {
                    alert(oDUClaimOrder.ErrorMessage);
                }
            },
            error: function (xhr, status, error)
            {
                alert(xhr+'~' +status+'~'+error);
            }
        });
    });


    $('#btnPrint').click(function (e)
    {
        var oDUClaimOrder = $('#tblDUClaimOrder').datagrid('getSelected');
        if (oDUClaimOrder ==null || oDUClaimOrder.DUClaimOrderID <=0 ) { alert("Please select an item from list."); return ; }
        var tsv=((new Date()).getTime())/1000;
        window.open(_sBaseAddress+ "/DUClaimOrder/PrintDUClaimOrder?nId="+oDUClaimOrder.DUClaimOrderID+"&nts="+tsv, "_blank");
    });

    $('#btnPrintList').click(function(){
      
        var oDUClaimOrders= $('#tblDUClaimOrder').datagrid('getRows');
        if(oDUClaimOrders.length<=0)
        {
            alert("Data not found ");
            return;
        }
        var sDUClaimOrderIDs = "";
        for(var i = 0;i<oDUClaimOrders.length;i++)
        {
            sDUClaimOrderIDs+= oDUClaimOrders[i].DUClaimOrderID+",";
        }
        sDUClaimOrderIDs = sDUClaimOrderIDs.substring(0, sDUClaimOrderIDs.length-1);
        var oDUClaimOrder = {ErrorMessage:sDUClaimOrderIDs,BUID:_nBUID};
        $.ajax({
            type: "POST",
            dataType: "json",
            url : _sBaseAddress+  "/DUClaimOrder/SetDUClaimOrderListData",
            traditional: true,
            data:  JSON.stringify(oDUClaimOrder),
            contentType: "application/json; charset=utf-8",
            success: function (data) {
                var sFeedBackMessage = jQuery.parseJSON(data);
                if (sFeedBackMessage==="Successful")
                {
                    window.open (_sBaseAddress+ "/DUClaimOrder/PrintDUClaimOrderDetails");
                }
            },
            error: function (xhr, status, error) {
                alert(error);
            }
        });

    });
    

    $('#btnPrintListExcel').click(function(){
      
        var oDUClaimOrders= $('#tblDUClaimOrder').datagrid('getRows');
        if(oDUClaimOrders.length<=0)
        {
            alert("Data not found ");
            return;
        }
        var sDUClaimOrderIDs = "";
        for(var i = 0;i<oDUClaimOrders.length;i++)
        {
            sDUClaimOrderIDs+= oDUClaimOrders[i].DUClaimOrderID+",";
        }
        sDUClaimOrderIDs = sDUClaimOrderIDs.substring(0, sDUClaimOrderIDs.length-1);
        var oDUClaimOrder = {ErrorMessage:sDUClaimOrderIDs,BUID:_nBUID};
        $.ajax({
            type: "POST",
            dataType: "json",
            url : _sBaseAddress+  "/DUClaimOrder/SetDUClaimOrderListData",
            traditional: true,
            data:  JSON.stringify(oDUClaimOrder),
            contentType: "application/json; charset=utf-8",
            success: function (data) {
                var sFeedBackMessage = jQuery.parseJSON(data);
                if (sFeedBackMessage==="Successful")
                {
                    window.open (_sBaseAddress+ "/DUClaimOrder/ExportToExcel");
                }
            },
            error: function (xhr, status, error) {
                alert(error);
            }
        });

    });

    $("#btnReload").click(function() {
        window.location.href = _sBaseAddress+ '/DUClaimOrder/ViewDUClaimOrders?'+"buid="+_nBUID+"&menuid="+_sMenuID; ;
    });



    ////Start adv Searching

    function updateProgress() {
        var value =$('#progressbar').progressbar('getValue');
        if (value < 96){
            value += Math.floor(Math.random() * 10);
            $('#progressbar').progressbar('setValue', value);
        }
    }
    function hideShow(miliseconds) {
        $("#progressbarParent").hide();
    }

    $("#btnResetAdvSearch").click(function () {
        ResetAdvSearchWindow();
        DateActionsOrderDateAdvSearch();
    });

    $("#btnSearchAdvSearch").click(function () {
        debugger;

        $(".lblLoadingMessage").show();

        var checkDate = CheckFromAndToDateValidation("cboDate", "txtFromDateAdvSearch", "txtToDateAdvSearch");
        if (!checkDate) {
            alert("Start date must be greater than end date.");
            return false;
        }


        var ncboDate = parseInt($("#cboDate").val());
        var dFromDate = $('#txtFromDateAdvSearch').datebox('getValue');
        var dToDate = $('#txtToDateAdvSearch').datebox('getValue');
        var sClaimNo = $('#txtClaimNo').val();
        var sPINo = $('#txtPINo_adv').val();
        var sOrderNo = $('#txtOrderNo').val();
        var ncboOrderType = parseInt($("#cboOrderType").val());
        var ncboClaimType = parseInt($("#cboClaimType").val());

        var sParams = _sContractorIds + "~" +
                      ncboDate + "~" +
                      dFromDate + "~" +
                      dToDate + "~" +
                      sClaimNo + "~" +
                      sPINo+"~"+
                      sOrderNo+"~"+
                      ncboOrderType+"~"+
                       ncboClaimType+"~"+
                      _nBUID;
        var oDUClaimOrder = {
            Note : sParams
        };

        $.ajax({
            type: "POST",
            dataType: "json",
            url: _sBaseAddress + "/DUClaimOrder/AdvSearch",
            traditional: true,
            data: JSON.stringify(oDUClaimOrder),
            contentType: "application/json; charset=utf-8",
            success: function (data) {
                var oDUClaimOrders = jQuery.parseJSON(data);
                debugger;
                if (oDUClaimOrders != null) {
                    if (oDUClaimOrders.length > 0)
                    {

                        DynamicRefreshList(oDUClaimOrders, "tblDUClaimOrder");
                        $("#winAdvSearch").icsWindow("close");

                    }
                    else
                    {
                        alert("Sorry, No data found.");
                        DynamicRefreshList([], "tblDUClaimOrder");
                    }

                } else {
                    alert("Sorry, No data found.");
                    DynamicRefreshList([], "tblDUClaimOrder");
                }
                $(".lblLoadingMessage").hide();
            }
        });
    });
    $("#txtSearchByClaimNo").keydown(function (e) {
        var nkeyCode = e.keyCode || e.which;
        if(nkeyCode==13){

            var ncboDate = 0;
            var dFromDate = icsdateformat(new Date());
            var dToDate = icsdateformat(new Date());
            var sClaimNo = $('#txtSearchByClaimNo').val();
            var sPINo = "";
            var sOrderNo = '';
            var ncboOrderType = 0;
            var ncboClaimType = 0;

            var sParams = _sContractorIds + "~" +
                          ncboDate + "~" +
                          dFromDate + "~" +
                          dToDate + "~" +
                          sClaimNo + "~" +
                          sPINo+"~"+
                          sOrderNo+"~"+
                      ncboOrderType+"~"+
                       ncboClaimType+"~"+
                          _nBUID;
            var oDUClaimOrder = {
                Note : sParams
            };
            GetsOrders(oDUClaimOrder);
        }
        else if(nkeyCode==8){
            $("#txtSearchByClaimNo").val("");

        }
    });
    function GetsOrders(oDUClaimOrder)
    {
        $.ajax
          ({
              type: "POST",
              dataType: "json",
              url : _sBaseAddress+"/DUClaimOrder/AdvSearch",
              data:  JSON.stringify(oDUClaimOrder),
              contentType: "application/json; charset=utf-8",
              success: function (data) {
                  //debugger;
                  var oDUClaimOrders = jQuery.parseJSON(data);
                  if (oDUClaimOrders.length>0)
                  {
                      DynamicRefreshList(oDUClaimOrders, "tblDUClaimOrder");
                  }
                  else
                  {
                      alert("Data Not found");
                      DynamicRefreshList([], "tblDUClaimOrder");
                  }
              },
              error: function (xhr, status, error)
              {
                  alert(error);
              }
          });
    }

