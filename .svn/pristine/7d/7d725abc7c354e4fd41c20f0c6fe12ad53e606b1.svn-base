@using ESimSol.BusinessObjects;
@model ConsumptionReturnChallan
  @foreach (Company oOrganizationalInformation in Model.Companys)
  {          
<table border="0" cellspacing="0">
        <tr>
            <td width="100%"   style="text-align: center; font-size:15px; font-weight:bold">
             @Model.Companys[0].Name 
            </td>
        </tr>
        <tr>
            <td width="100%"   style="text-align: center; font-size:8px">
               HeadOffice: @Model.Companys[0].Address
            </td>
        </tr>
        <tr>
            <td width="100%"   style="text-align: center; font-size:8px">
                Tel : @Model.Companys[0].Phone, E-mail : @Model.Companys[0].Email, Web : @Model.Companys[0].WebAddress
            </td>
        </tr>   
</table>
  }
<br />

<table>     
        <tr style="height:200px; vertical-align:bottom">
            <td width="100%"   style="text-align: center; font-size:12px; font-weight:bold">
                <u>Material Consumption</u>
            </td>
        </tr>
</table>

<br />

<table>     
        <tr style="height:100px; vertical-align:bottom;">
        <td width="50%"   style="text-align: left; font-size:8px; font-weight:bold">
                <u> Project Name:@Model.Name </u>
            </td> 
            <td width="50%"   style="text-align: right; font-size:8px; font-weight:bold">
                <u> Date:@Model.ErrorMessage </u>
            </td> 
</table>
 
 <br />
 
<table cellpadding="3" cellspacing="3" style="min-height:1000px !important;">    
    @{
        int nSLNo = 0;
       
    }
    @{
        double nQty = 0;
       
    }
    @{
        double nValue = 0;
       
    }
  
   @{
        double nRValue = 0;
       
    }
    @{
        double nReturnQty = 0;
       
    }
    @{
        double nConsumedQty = 0;
       
    }
     @{
        double nCQty = 0;
       
    }
    <tr border="1" bgcolor="#DEDEDE">
        <td width="4%" style="text-align: center; font-size:9px; font-weight:bold">
            SL
        </td>
        <td width="35%" style="text-align: center; font-size:9px; font-weight:bold">
           ProductName
           </td>
        <td width="12%" style="text-align: center; font-size:9px; font-weight:bold">
            Delivered Qty
        </td>
         <td width="12%" style="text-align: center; font-size:9px; font-weight:bold">
           Return Qty
        </td>
        <td width="12%" style="text-align: center; font-size:9px; font-weight:bold">
           Consumed Qty
        </td>
        <td width="8%" style="text-align: center; font-size:9px; font-weight:bold">
           Unit
        </td>
        <td width="14%" style="text-align: center; font-size:9px; font-weight:bold">
          Challan Value(BDT)
        </td>
        <td width="14%" style="text-align: center; font-size:9px; font-weight:bold">
          Return Value(BDT)
        </td>
        
    </tr>
    @foreach (ConsumptionReturnChallan oConsumptionReturnChallan in Model.MaterialConsumptionforPrint)
    {
            nConsumedQty = oConsumptionReturnChallan.Quantity - oConsumptionReturnChallan.ReturnQty;
            nSLNo++;
            nQty += oConsumptionReturnChallan.Quantity;
            nValue += oConsumptionReturnChallan.Value;
            nCQty += nConsumedQty;
            nReturnQty += oConsumptionReturnChallan.ReturnQty;
            nRValue += oConsumptionReturnChallan.ReturnValue;
            <tr border="1">
                <td style="text-align: left; font-size:9px">
                     @nSLNo.ToString()
                </td>
                <td  style="text-align: left; font-size:9px">
                     @oConsumptionReturnChallan.ProductNameCode
                </td>
                <td  style="text-align: left; font-size:9px">
                     @oConsumptionReturnChallan.Quantity
                </td>
                 <td  style="text-align: left; font-size:9px">
                     @oConsumptionReturnChallan.ReturnQty
                </td>
                 <td  style="text-align: left; font-size:9px">
                     @(oConsumptionReturnChallan.Quantity-oConsumptionReturnChallan.ReturnQty)
                </td>
                <td  style="text-align: left; font-size:9px">
                     @oConsumptionReturnChallan.MUName
                </td>
                <td  style="text-align: right; font-size:9px">
                     @oConsumptionReturnChallan.Value
                </td>
                <td  style="text-align: right; font-size:9px">
                    @oConsumptionReturnChallan.ReturnValue
                </td>              
           </tr>
    }
  <tr border="0">
            <td style="text-align: left; font-size:9px"></td>
            <td style="text-align: right; font-size:9px">
                Total :
            </td>
            <td  style="text-align: right; font-size:9px">
              @nQty.ToString() 
            </td>
            <td  style="text-align: right; font-size:9px">
              @nReturnQty.ToString() 
            </td>
            <td  style="text-align: right; font-size:9px">
              @nCQty.ToString() 
            </td>
            <td style="text-align: left; font-size:9px"></td>
            <td  style="text-align: right; font-size:9px">
              @nValue.ToString() 
            </td>
            <td  style="text-align: right; font-size:9px">
              @nRValue.ToString() 
            </td>
        </tr>   
</table>
<br />
<br />
<br />
<br />
<table border="0" cellpadding="3" cellspacing="3">

    <tr>
        <td width="33%"  style="text-align: left; font-size:9px">
        
        </td>

        <td width="34%"  style="text-align: left; font-size:9px">
        </td>

        <td width="33%"  style="text-align: left; font-size:9px">
        </td>    
    </tr>
    <tr>
        <td width="33%"  style="text-align: left; font-size:9px">
        __________________ 
        </td>
        <td width="34%"  style="text-align: center; font-size:9px">
        __________________        
        </td>
        <td width="33%"  style="text-align: right; font-size:9px">
        __________________         
        </td>
    </tr>
    <tr>
        <td width="33%"  style="text-align: left; font-size:9px">       
           Store Incharge
        </td>
        <td width="34%"  style="text-align: center; font-size:9px">       
          Received By
        </td>
        <td width="33%"  style="text-align: right; font-size:9px">       
           Authorized By
        </td>
    </tr>
    <tr>
        <td width="33%"  style="text-align: left; font-size:9px">       
           
        </td>
        <td width="34%"  style="text-align: center; font-size:9px">       
         
        </td>
        <td width="33%"  style="text-align: right; font-size:9px">       
         
        </td>
    </tr>
    <tr>
        <td></td>
        <td></td>
        <td></td>    
    </tr>
</table>   