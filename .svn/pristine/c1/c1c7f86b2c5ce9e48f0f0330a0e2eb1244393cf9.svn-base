
<div class="modal-header">
    <h4>Vehicle Chassis</h4>
</div>
<div class="modal-body ms-custom-control" id="modal-body">
    <div class="row">
        <div class="col-md-12">
            <div class="col-md-2 align-right">
                <label class="control-label">File No</label>
            </div>
            <div class="col-md-4">
                <input type="text" ng-model="VehicleChassis.FileNo" class="form-control" disabled />
            </div>
            <div class="col-md-2 align-right">
                <label class="control-label">Chassis No</label>
            </div>
            <div class="col-md-4">
                <input type="text" ng-model="VehicleChassis.ChassisNo" class="form-control" />
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-md-12">
            <div class="col-md-2  align-right">
                <label class="control-label">Manufacturer</label>
            </div>
            <div class="col-md-4">
                <div class="input-group">
                    <input ng-model="VehicleChassis.ManufacturerName" class="form-control input-group-sm" ng-keydown="SearchKeyDownManufacturer($event)" placeholder="Type Manufacturer & Press Enter" required />
                    <span class="input-group-btn">
                        <button type="button" class="btn btn-sm" aria-label="left Align" ng-click="ClearManufacturer()">
                            <span class="glyphicon glyphicon-refresh" aria-hidden="true"></span>
                        </button>
                        <span style="margin-left:2px;"></span>
                        <button type="button" class="btn btn-sm" aria-label="left Align" ng-click="PickManufacturer()">
                            <span class="glyphicon glyphicon-search" aria-hidden="true"></span>
                        </button>
                    </span>
                </div>
            </div>
            <div class="col-md-2  align-right">
                <label class="control-label">Engine Layout</label>
            </div>
            <div class="col-md-4  align-right">
                <input type="text" ng-model="VehicleChassis.EngineLayout" class="form-control" />
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-md-12">
            <div class="col-md-2 align-right">
                <label class="control-label">Engine Position</label>
            </div>
            <div class="col-md-4">
                <input type="text" ng-model="VehicleChassis.EnginePosition" class="form-control" />
            </div>
            <div class="col-md-2 align-right">
                <label class="control-label">Torque Split</label>
            </div>
            <div class="col-md-4">
                <input type="text" ng-model="VehicleChassis.TorqueSplit" class="form-control" />
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-md-12">
            <div class="col-md-2 align-right">
                <label class="control-label">Drive Wheels</label>
            </div>
            <div class="col-md-4">
                <input type="text" ng-model="VehicleChassis.DriveWheels" class="form-control" />
            </div>
            <div class="col-md-2 align-right">
                <label class="control-label"> Wheel Size Front</label>
            </div>
            <div class="col-md-4">
                <input type="text" ng-model="VehicleChassis.WheelSizeFront" class="form-control" />
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-md-12">
            <div class="col-md-2 align-right">
                <label class="control-label">Steering</label>
            </div>
            <div class="col-md-4">
                <input type="text" ng-model="VehicleChassis.Steering" class="form-control" />
            </div>
            <div class="col-md-2 align-right">
                <label class="control-label">Wheel Size Rear</label>
            </div>
            <div class="col-md-4">
                <input type="text" ng-model="VehicleChassis.WheelSizeRear" class="form-control" />
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-md-12">
            <div class="col-md-2 align-right">
                <label class="control-label">Front Brake Diameter</label>
            </div>
            <div class="col-md-4">
                <input type="text" ng-model="VehicleChassis.FrontBrakeDiameter" class="form-control" />
            </div>
            <div class="col-md-2 align-right">
                <label class="control-label">Tyres Front</label>
            </div>
            <div class="col-md-4">
                <input type="text" ng-model="VehicleChassis.TyersFront" class="form-control" />
            </div>

        </div>
    </div>
    <div class="row">
        <div class="col-md-12">
            <div class="col-md-2 align-right">
                <label class="control-label">Rear Brake Diameter</label>
            </div>
            <div class="col-md-4">
                <input type="text" ng-model="VehicleChassis.RearBrakeDiameter" class="form-control" />
            </div>
            <div class="col-md-2 align-right">
                <label class="control-label">Tyers Rear</label>
            </div>
            <div class="col-md-4">
                <input type="text" ng-model="VehicleChassis.TyersRear" class="form-control" />
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-md-12">
            <div class="col-md-2 align-right">
                <label class="control-label"> Brakes f/r</label>
            </div>
            <div class="col-md-4">
                <input type="text" ng-model="VehicleChassis.BrakesFR" class="form-control" />
            </div>
            <div class="col-md-2 align-right">
                <label class="control-label">Top Gear Ratio</label>
            </div>
            <div class="col-md-4">
                <input type="text" ng-model="VehicleChassis.TopGearRatio" class="form-control" />
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-md-12">
            <div class="col-md-2 align-right">
                <label class="control-label">GearBox</label>
            </div>
            <div class="col-md-4">
                <input type="text" ng-model="VehicleChassis.Gearbox" class="form-control" />
            </div>
            <div class="col-md-2 align-right">
                <label class="control-label">Final Drive Ratio</label>
            </div>
            <div class="col-md-4">
                <input type="text" ng-model="VehicleChassis.FinalDriveRatio" class="form-control" />
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-md-12">
            <div class="col-md-2 align-right">
                <label class="control-label">Remarks</label>
            </div>
            <div class="col-md-10">
                <input type="text" ng-model="VehicleChassis.Remarks" class="form-control" />
            </div>
        </div>
    </div>
</div>
<div class="modal-footer">
    <button type="button" id="btnSave" class="btn btn-default btn-xs " aria-label="right Align" ng-click="Save()"><span class="glyphicon glyphicon-floppy-disk" aria-hidden="true"> Save</span></button>
    <button type="button" id="btnClose" class="btn btn-default btn-xs " aria-label="right Align" ng-click="Close()"><span class="glyphicon glyphicon-remove" aria-hidden="true"> Close</span></button>
</div>

<style type="text/css">
    .regionVehicleChassis .form-horizontal .control-label {
        padding-top: 1px;
    }

    .regionVehicleChassis .form-control {
        height: 26px;
        font-size: 12px;
        width: 100%;
    }
</style>


