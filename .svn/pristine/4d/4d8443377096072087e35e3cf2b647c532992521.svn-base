                        }
                        RefreshTotal();
                    }
                    else
                    {
                        alert("Sorry, No FN Batch Detail Found.");
                    }
                });


            }
            else{
                alert("Data Not Found.");
            }
        }
       
        else {
            alert("Data not found");
            return false;
        }
    }

</script>