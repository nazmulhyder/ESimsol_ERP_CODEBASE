    function BUClean() 
    {
        $("#txtBUName").removeClass("fontColorOfPickItem");
        $("#txtBUName").val("Group Accounts");
        var oVoucherRefReport = $('#divVoucherBillDetails').data('obj');
        oVoucherRefReport.BusinessUnitIDs="0";
