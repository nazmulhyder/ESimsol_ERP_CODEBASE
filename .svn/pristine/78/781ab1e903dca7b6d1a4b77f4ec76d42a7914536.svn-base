                { field: 'Qty_Batch', name:'Qty_Batch',cellFilter:'number:2', cellClass:'text-right',  width:'10%' },
                { field: 'Value', name:'Value',cellFilter:'number:2', cellClass:'text-right',  width:'10%' },
                { field: 'Currency', name:'Currency', width:'10%' },
