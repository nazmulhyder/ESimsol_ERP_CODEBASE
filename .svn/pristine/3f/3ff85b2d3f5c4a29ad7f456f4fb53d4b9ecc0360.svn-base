    function Save_EmployeeSettlementClearanceSection()
    {
        var oESCS= {
            ESCSID:0,
            Name:$("#txtEmployeeSettlementClearanceSectionName").val()
        };

        if(oESCS.Name=="" ||oESCS.Name==null)
        {
            alert("Please enter name!");
            return;
        }
        $.ajax({
            type: "POST",
            dataType: "json",
            url: _sBaseAddress + "/EmployeeSettlement/EmployeeSettlementClearanceSection_IU",
            traditional: true,
            data: JSON.stringify(oESCS),
            contentType: "application/json; charset=utf-8",

            success: function(data) {
                var oEmployeeSettlementClearanceSection = jQuery.parseJSON(data);
                if (oEmployeeSettlementClearanceSection.ESCSID > 0) {
                    if(oESCS.ESCSID==0)
                    {
                        alert("Data Saved sucessfully");
                        _nESCSID=oEmployeeSettlementClearanceSection.ESCSID;
                        var oEmployeeSettlementClearanceSections = $('#tblEmployeeSettlementClearanceSection').datagrid('getRows');
                        var nIndex = oEmployeeSettlementClearanceSections.length;
                        $('#tblEmployeeSettlementClearanceSection').datagrid('appendRow', oEmployeeSettlementClearanceSection);
                        $('#tblEmployeeSettlementClearanceSection').datagrid('selectRow', nIndex);
                    }
                    else if(oESCS.ESCSID>0)
                    {
                        alert("Data Edited sucessfully");
                        _nESCSID=oEmployeeSettlementClearanceSection.ESCSID;
                        var oRateScheme = $('#tblEmployeeSettlementClearanceSection').datagrid('getSelected');
                        var SelectedRowIndex = $('#tblEmployeeSettlementClearanceSection').datagrid('getRowIndex', oRateScheme);
                        $('#tblEmployeeSettlementClearanceSection').datagrid('updateRow', { index: SelectedRowIndex, row: oEmployeeSettlementClearanceSection });
                    }
                    $("#txtEmployeeSettlementClearanceSectionName").val("");
                } else {
                    alert(oEmployeeSettlementClearanceSection.ErrorMessage);
                }
            },
            error: function(xhr, status, error) {
                alert(error);
            }
        });
    }

    $('#btnDelete_EmployeeSettlementClearanceSection').click(function (e)
    {
        var oEmployeeSettlementClearanceSection = $('#tblEmployeeSettlementClearanceSection').datagrid('getSelected');
