<div class="modal-header">
    <h4 class="modal-title" id="modal-title" ng-bind="PickerTitle">Advance Search</h4>
</div>
<div class="modal-body regionSearch" style="height:460px; overflow:auto;">
    <fieldset style="margin-bottom: 0px;">
        <legend>Searching Criteria</legend>
        <div class="row col-md-12">
            <div class="col-md-3 text-right"><label class="control-label"> LC No:</label></div>
            <div class="col-md-9 text-left"><input type="text" class="form-control" ng-model="LCNo"></div>
        </div>
        <div class="row col-md-12">
            <div class="col-md-3 text-right"><label class="control-label">Invoice No:</label></div>
            <div class="col-md-9 text-left"><input type="text" class="form-control" ng-model="InvoiceNo"></div>
        </div>
        <div class="row col-md-12">
            <div class="col-md-3 text-right"><label class="control-label">Buyer Name:</label></div>
            <div class="col-md-9 text-left">
                <div class="input-group">
                    <input type="text" class="form-control" ng-model="BuyerName" ng-keydown="SearchKeyDownBuyer($event)" placeholder="Type Buyer Name & Press Enter">
                    <span class="input-group-btn">
                        <button type="button" class="btn btn-sm" aria-label="Left Align" ng-click="PickBuyer()"> <span class="glyphicon glyphicon-search" aria-hidden="true"></span></button>
                    </span>
                </div>
            </div>
        </div>
        <div class="row col-md-12">
            <div class="col-md-3 text-right"><label class="control-label">Advice Bank:</label></div>
            <div class="col-md-9 text-left">
                <div class="input-group">
                    <input ng-model="AdviceBankName" class="form-control" ng-keydown="SearchKeyDownAdviceBank($event)" placeholder="Type Bank Acccount & Press Enter" />
                    <span class="input-group-btn">
                        <button type="button" class="btn btn-sm" aria-label="Left Align" ng-click="AdviceBank()"> <span class="glyphicon glyphicon-search" aria-hidden="true"></span></button>
                    </span>
                </div>
            </div>
        </div>
        <div class="row col-md-12">
            <div class="col-md-3 text-right"><label class="control-label">Issue Bank:</label></div>
            <div class="col-md-9 text-left">
                <div class="input-group">
                    <input ng-model="IssueBankName" class="form-control" ng-keydown="SearchKeyDownIssueBank($event)" placeholder="Type Bank Name & Press Enter" />
                    <span class="input-group-btn">
                        <button type="button" class="btn btn-sm" aria-label="Left Align" ng-click="IssueBank()"> <span class="glyphicon glyphicon-search" aria-hidden="true"></span></button>
                    </span>
                </div>
            </div>
        </div>
        <div class="row col-md-12">
            <div class="col-md-3 text-right"><label class="control-label">Invoice Date:</label></div>
            <div class="col-md-9 text-left">
                <div class="col-md-4 text-left">
                    <select class="form-control" ng-model="cboInvoiceDate" ng-change="InvoiceDateChange()" ng-options="item.id as item.Value for item in CompareOperators"></select>
                </div>
                <div class="col-md-8 text-left">
                    <div class="col-md-12">
                        <div class="input-group date date-container" style="width:43%;float:left">
                            <input type="text" class="form-control" ng-model="InvoiceDateStart"><span class="input-group-addon"><i class="glyphicon glyphicon-th"></i></span>
                        </div>
                        <div style="width:14%;float:inherit;">
                            <span class="input-group-addon" style="height:26px;"><span class="label label-primary">To</span></span>
                        </div>
                        <div class="input-group date date-container" style="width:43%;float:left">
                            <input type="text" class="form-control" ng-model="InvoiceDateEnd" ng-disabled="InvoiceDateEndDisabled"><span class="input-group-addon"><i class="glyphicon glyphicon-th"></i></span>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="row col-md-12">
            <div class="col-md-3 text-right"><label class="control-label">L/C Rcv Date:</label></div>
            <div class="col-md-9 text-left">
                <div class="col-md-4 text-left">
                    <select class="form-control" ng-model="cboLCRcvDate" ng-change="LCRcvDateChange()" ng-options="item.id as item.Value for item in CompareOperators"></select>
                </div>
                <div class="col-md-8 text-left">
                    <div class="col-md-12">
                        <div class="input-group date date-container" style="width:43%;float:left">
                            <input type="text" class="form-control" ng-model="LCRcvDateStart"><span class="input-group-addon"><i class="glyphicon glyphicon-th"></i></span>
                        </div>
                        <div style="width:14%;float:inherit;">
                            <span class="input-group-addon" style="height:26px;"><span class="label label-primary">To</span></span>
                        </div>
                        <div class="input-group date date-container" style="width:43%;float:left">
                            <input type="text" class="form-control" ng-model="LCRcvDateEnd" ng-disabled="LCRcvDateEndDisabled"><span class="input-group-addon"><i class="glyphicon glyphicon-th"></i></span>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="row col-md-12">
            <div class="col-md-3 text-right"><label class="control-label">Send To Buyer Date:</label></div>
            <div class="col-md-9 text-left">
                <div class="col-md-4 text-left">
                    <select class="form-control" ng-model="cboSendToBuyerDate" ng-change="SendToBuyerDateChange()" ng-options="item.id as item.Value for item in CompareOperators"></select>
                </div>
                <div class="col-md-8 text-left">
                    <div class="col-md-12">
                        <div class="input-group date date-container" style="width:43%;float:left">
                            <input type="text" class="form-control" ng-model="SendToBuyerDateStart"><span class="input-group-addon"><i class="glyphicon glyphicon-th"></i></span>
                        </div>
                        <div style="width:14%;float:inherit;">
                            <span class="input-group-addon" style="height:26px;"><span class="label label-primary">To</span></span>
                        </div>
                        <div class="input-group date date-container" style="width:43%;float:left">
                            <input type="text" class="form-control" ng-model="SendToBuyerDateEnd" ng-disabled="SendToBuyerDateEndDisabled"><span class="input-group-addon"><i class="glyphicon glyphicon-th"></i></span>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="row col-md-12">
            <div class="col-md-3 text-right"><label class="control-label">Buyer Accepted Date:</label></div>
            <div class="col-md-9 text-left">
                <div class="col-md-4 text-left">
                    <select class="form-control" ng-model="cboBuyerAcceptDate" ng-change="BuyerAcceptDateChange()" ng-options="item.id as item.Value for item in CompareOperators"></select>
                </div>
                <div class="col-md-8 text-left">
                    <div class="col-md-12">
                        <div class="input-group date date-container" style="width:43%;float:left">
                            <input type="text" class="form-control" ng-model="BuyerAcceptDateStart"><span class="input-group-addon"><i class="glyphicon glyphicon-th"></i></span>
                        </div>
                        <div style="width:14%;float:inherit;">
                            <span class="input-group-addon" style="height:26px;"><span class="label label-primary">To</span></span>
                        </div>
                        <div class="input-group date date-container" style="width:43%;float:left">
                            <input type="text" class="form-control" ng-model="BuyerAcceptDateEnd" ng-disabled="BuyerAcceptDateEndDisabled"><span class="input-group-addon"><i class="glyphicon glyphicon-th"></i></span>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="row col-md-12">
            <div class="col-md-3 text-right"><label class="control-label">EnCashment Date:</label></div>
            <div class="col-md-9 text-left">
                <div class="col-md-4 text-left">
                    <select class="form-control" ng-model="cboEnCashmentDate" ng-change="EnCashmentDateChange()" ng-options="item.id as item.Value for item in CompareOperators"></select>
                </div>
                <div class="col-md-8 text-left">
                    <div class="col-md-12">
                        <div class="input-group date date-container" style="width:43%;float:left">
                            <input type="text" class="form-control" ng-model="EnCashmentDateStart"><span class="input-group-addon"><i class="glyphicon glyphicon-th"></i></span>
                        </div>
                        <div style="width:14%;float:inherit;">
                            <span class="input-group-addon" style="height:26px;"><span class="label label-primary">To</span></span>
                        </div>
                        <div class="input-group date date-container" style="width:43%;float:left">
                            <input type="text" class="form-control" ng-model="EnCashmentDateEnd" ng-disabled="EnCashmentDateEndDisabled"><span class="input-group-addon"><i class="glyphicon glyphicon-th"></i></span>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="row col-md-12">
            <div class="col-md-3 text-right"><label class="control-label">Session:</label></div>
            <div class="col-md-9 text-left">
                <select class="form-control" ng-model="cboSession" ng-options="item.BusinessSessionID as item.SessionName for item in Sessions"></select>
            </div>
        </div>


        <div class="row col-md-12">
            <div class="col-md-3 text-right"><label class="control-label">Bill Amount:</label></div>
            <div class="col-md-9 text-left">
                <div class="col-md-4 text-left">
                    <select class="form-control" ng-model="cboBillAmount" ng-change="BillAmountChange()" ng-options="item.id as item.Value for item in CompareOperators"></select>
                </div>
                <div class="col-md-8 input-group text-left">
                    <input type="number" class="form-control" ng-model="BillAmountStart" style="text-align:right;"><span class="input-group-addon"><span class="label label-primary">To</span></span>
                    <input type="number" class="form-control" ng-model="BillAmountEnd" style="text-align:right;" ng-disabled="BillAmountEndDisabled">
               </div>
            </div>
        </div>
       
        <div class="row col-md-12">
            <div class="col-md-3 text-right"><label class="control-label">Discrepency:</label></div>
            <div class="col-md-9 text-left">
                <div class="col-md-4 text-left">
                    <select class="form-control" ng-model="cboDiscrepency" ng-change="DiscrepencyChange()" ng-options="item.id as item.Value for item in CompareOperators"></select>
                </div>
                <div class="col-md-8 input-group text-left">
                    <input type="number" class="form-control" ng-model="DiscrepencyStart" style="text-align:right;"><span class="input-group-addon"><span class="label label-primary">To</span></span>
                    <input type="number" class="form-control" ng-model="DiscrepencyEnd" style="text-align:right;" ng-disabled="DiscrepencyEndDisabled">
                </div>
            </div>
        </div>

        <div class="row col-md-12">
            <div class="col-md-3 text-right"><label class="control-label">Status:</label></div>
            <div class="col-md-9 text-left">
                <div ui-grid="gridOptionsStatus" ui-grid-selection style="height:120px;"></div>
            </div>
        </div>
    </fieldset>
</div>
<div class="modal-footer">
    <label ng-hide="lblLoadingMessage" style="float:left;color:green; ">Loading Please Wait...</label>
    <button class="btn btn-success" type="button" ng-click="reset()">Reset</button>
    <button class="btn btn-primary" type="button" ng-click="search()">Search</button>
    <button class="btn btn-warning" type="button" ng-click="closeAdvanceSearchModal()">Close</button>
</div>
<style type="text/css">
    .regionSearch .form-control {
        height: 26px;
        padding: 0px 5px;
        font-size: 12px;
    }

    .regionSearch .control-label {
        padding-top: 1px;
    }

    .regionSearch .col-md-12 {
        width: 100%;
        padding-right: 1px;
        padding-left: 1px;
        margin-bottom: 5px;
    }

    .regionSearch .col-md-3 {
        width: 29%;
        padding-left: 1px;
    }

    .regionSearch .col-md-9 {
        width: 71%;
        padding-right: 1px;
    }

    .regionSearch .col-md-4 {
        width: 28%;
        padding-right: 1px;
        padding-left: 1px;
    }

    .regionSearch .col-md-8 {
        width: 72%;
        padding-right: 1px;
        padding-left: 1px;
    }

    .regionSearch .btn-sm {
        padding: 3px 10px;
    }

    .regionSearch .input-group-addon {
        padding: 3px 6px;
    }
</style>
