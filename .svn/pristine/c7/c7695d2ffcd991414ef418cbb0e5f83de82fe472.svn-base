<html>
@{
    ViewBag.Title = "Export PI Registers";
}
<body>
    @model ESimSol.BusinessObjects.ExportPIRegister
    <div id="progbarParent" style="width:100%;height:100%;margin:0 auto;text-align:center; margin-left: auto;margin-right: auto;left: 0;right: 0;z-index: 1;">
        <div style="margin: 0px auto;margin-top: 251px;text-align:center;width:390px;">
            <label style="font-size:18px;">Please wait</label>
            <div id="progbar" style="width:100%;height:37px;"></div>
        </div>
    </div>
    <div class="menuMainCollectionTable">
        <div class="easyui-panel" title="Export PI Registers" style="font-family:Tahoma; text-align:center; height:88%;">
            <fieldset style="height:95%; text-align:center;">
                <legend style="font-weight:bold">Serching Criteria :</legend>
                <table border="0" cellspacing="2" cellpadding="2" style="font-size:11px; font-weight:bold; text-align:center; width:100%">
                    <tr>
                        <td colspan="5" style="height:20px;"></td>                        
                    </tr>
                    <tr>
                        <td style="width:15%;text-align:right">PI No :</td>
                        <td style="width:30%;text-align:left"> 
                            <input type="text" style="width:98%" id="txtPINo" />
                        </td>
                        <td style="width:5%;text-align:right"></td>
                        <td style="width:12%;text-align:right">PI Date :</td>
                        <td style="width:38%;text-align:right">
                            <table border="0" cellpadding="1" cellspacing="1" width="100%">
                                <tr>
                                    <td style="width:30%; text-align:center">
                                        <select id="cboPIIssueDate" style="width:100%"></select>
                                    </td>
                                    <td style="width:30%; text-align:center">
                                        <input id="txtPIIssueDateStart" type="text" class="easyui-datebox" required="required" data-options="formatter:icsdateformat,parser:icsdateparser" style=" width:100%;" />
                                    </td>
                                    <td style="width:10%; text-align:center">to</td>
                                    <td style="width:30%; text-align:center">
                                        <input id="txtPIIssueDateEnd" type="text" class="easyui-datebox" required="required" data-options="formatter:icsdateformat,parser:icsdateparser" style=" width:100%;" />
                                    </td>
                                </tr>
                            </table> 
                        </td>
                    </tr>
                    <tr>
                        <td style="width:15%;text-align:right">Mother Buyer :</td>
                        <td style="width:30%;text-align:left">
                            <table border="0" cellpadding="0" cellspacing="0" style="width:100%">
                                <tr>
                                    <td style="width:70%; text-align:left">
                                        <input type="text" style="width:98%" id="txtMotherBuyer" placeholder="Press enter with Mother Buyer name" />
                                    </td>
                                    <td style="width:30%; text-align:right">
                                        <input type="button" value="C" style="width:30%" id="btnClearMotherBuyer" />
                                        <input type="button" value="Pick" style="width:65%" id="btnPickMotherBuyer" />
                                    </td>
                                </tr>
                            </table>
                        </td>
                        <td style="width:5%;text-align:right"></td>
                        <td style="width:12%;text-align:right"> Buyer:</td>
                        <td style="width:38%;text-align:right">
                            <table border="0" cellpadding="0" cellspacing="0" style="width:100%">
                                <tr>
                                    <td style="width:70%; text-align:left">
                                        <input type="text" style="width:98%" id="txtBuyer" placeholder="Press enter with  Buyer name" />
                                    </td>
                                    <td style="width:30%; text-align:right">
                                        <input type="button" value="C" style="width:30%" id="btnClearBuyer" />
                                        <input type="button" value="Pick" style="width:65%" id="btnPickBuyer" />
                                    </td>
                                </tr>
                            </table>
                        </td>
                    </tr>
                    <tr>
                        <td style="width:15%;text-align:right"> Marketing Person :</td>
                        <td style="width:30%;text-align:left">
                            <select id="cboMktPerson" style="width:100%">  </select>
                        </td>
                        <td style="width:5%;text-align:right"></td>
                        <td style="width:12%;text-align:right">PI Type :</td>
                        <td style="width:38%;text-align:right">
                            <select id="cboPIType" style="width:100%">  </select>
                        </td>
                    </tr>
                    <tr>
                        <td style="width:15%;text-align:right"> Currency:</td>
                        <td style="width:30%;text-align:left">
                            <select id="cboCurrency" style="width:100%">  </select>
                        </td>
                        <td style="width:5%;text-align:right"></td>
                        <td style="width:12%;text-align:right">Product Nature :</td>
                        <td style="width:38%;text-align:right">
                            <select id="cboProductNature" style="width:100%">  </select>
                        </td>
                    </tr>
                
                    <tr>
                        <td style="width:15%;text-align:right"> Report Layout :</td>
                        <td style="width:30%;text-align:left">
                            <select id="cboReportLayout" style="width:100%">  </select>
                        </td>
                        <td style="width:5%;text-align:right"></td>
                        <td style="width:12%;text-align:right">Due Options :</td>
                        <td style="width:38%;text-align:left"><select id="cboDueOptions" style="width:100%">
                                <option value="0">--Select One--</option>
                                <option value="1">Due With Challan</option>
                                <option value="2">Only Due</option>
                            </select></td>
                    </tr>
                </table>
            </fieldset>
        </div>
        <fieldset>
            <legend style="font-weight:bold"> Action : </legend>
            <table border="0" cellspacing="2" cellpadding="2" style="font-size:11px; font-weight:bold; width:100%;">
                <tr>
                    <td style="width:50%;text-align:right">
                                                 
                    </td>
                    <td style="width:50%;text-align:right">
                        <a id="btnPreview" href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-print" plain="true">Preview</a>
                        <a id="btnExportToExcel" href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-excel" plain="true">Export to Excel</a>
                        <a id="btnClose" href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-cancel" plain="true">Close</a>
                    </td>
                </tr>
            </table>
        </fieldset>
    </div>
</body>
</html>


<script type="text/javascript">
    $(document).ready(function () {
        var nBUID =@Html.Raw(new System.Web.Script.Serialization.JavaScriptSerializer().Serialize(ViewBag.BUID));
        var oReportLayouts = @Html.Raw(new System.Web.Script.Serialization.JavaScriptSerializer().Serialize(ViewBag.ReportLayouts));
        var oCompareOperators = @Html.Raw(new System.Web.Script.Serialization.JavaScriptSerializer().Serialize(ViewBag.CompareOperators));
        var oExportPITypes = @Html.Raw(new System.Web.Script.Serialization.JavaScriptSerializer().Serialize(ViewBag.ExportPITypes));
        var oMktPersons = @Html.Raw(new System.Web.Script.Serialization.JavaScriptSerializer().Serialize(ViewBag.MktPersons));
        var oProductNatureList = @Html.Raw(new System.Web.Script.Serialization.JavaScriptSerializer().Serialize(ViewBag.ProductNatureList));
        var oCurrencyList = @Html.Raw(new System.Web.Script.Serialization.JavaScriptSerializer().Serialize(ViewBag.CurrencyList));

        $("#progbar").progressbar({ value: 0 });
        $("#progbarParent").hide();
        $("#cboPIIssueDate").icsLoadCombo({ List: oCompareOperators, OptionValue: "id", DisplayText: "Value" });
        $("#cboPIType").icsLoadCombo({ List: oExportPITypes, OptionValue: "id", DisplayText: "Value", InitialValue:"-Select One-"});
        $("#cboReportLayout").icsLoadCombo({ List: oReportLayouts, OptionValue: "id", DisplayText: "Value" });
        $("#cboMktPerson").icsLoadCombo({ List: oMktPersons, OptionValue: "MarketingAccountID", DisplayText: "Name" });
        $("#cboProductNature").icsLoadCombo({ List: oProductNatureList, OptionValue: "id", DisplayText: "Value" });
        $("#cboCurrency").icsLoadCombo({ List: oCurrencyList, OptionValue: "CurrencyID", DisplayText: "Symbol" });
        $('#txtPIIssueDateStart').datebox({ disabled : true });
        $('#txtPIIssueDateEnd').datebox({ disabled : true });
        $('#txtPIIssueDateStart').datebox('setValue', icsdateformat(new Date()));
        $('#txtPIIssueDateEnd').datebox('setValue', icsdateformat(new Date()));

        $('#txtMotherBuyer').data('MotherBuyers', []);
        $('#txtBuyer').data('Buyers', []);
        $('#txtMarketingAccount').data('MarketingAccounts', []);
    });

    function ValidateSearch()
    {

        var nPIIssueDate = parseInt($('#cboPIIssueDate').val());
        if(nPIIssueDate===1 || nPIIssueDate===2 || nPIIssueDate===3 || nPIIssueDate===4)
        {
            var sPIIssueDateStart   = $('#txtPIIssueDateStart').datebox('getValue');
            if(sPIIssueDateStart===null || sPIIssueDateStart==="")
            {
                alert("Please select PI start date!");
                $('#txtPIIssueDateStart').focus();
                return false;
            }
        }
        if(nPIIssueDate===5 || nPIIssueDate===6)
        {
            var sPIIssueDateStart   = $('#txtPIIssueDateStart').datebox('getValue');
            var sPIIssueDateEnd   = $('#txtPIIssueDateEnd').datebox('getValue');
            if(sPIIssueDateStart===null || sPIIssueDateStart==="")
            {
                alert("Please select PI start date!");
                $('#txtPIIssueDateStart').focus();
                return false;
            }
            if(sPIIssueDateEnd===null || sPIIssueDateEnd==="")
            {
                alert("Please select PI end date!");
                $('#txtPIIssueDateEnd').focus();
                return false;
            }
            if(new Date(sPIIssueDateStart) > new Date(sPIIssueDateEnd))
            {
                alert("Start date must be smallar than or equal end date!");
                $('#txtPIIssueDateStart').focus();
                return false;
            }
        }

        var nReportLayout = parseInt($('#cboReportLayout').val());
        if(nReportLayout<=0)
        {
            alert("Please select an Report Layout!");
            return;
        }
        var sPINo =$.trim($('#txtPINo').val());
        var nPIType = parseInt($('#cboPIType').val());
        var oMotherBuyers = $('#txtMotherBuyer').data('MotherBuyers');
        var oBuyers = $('#txtBuyer').data('Buyers');
        var nMktPerson = $('#cboMktPerson').val();
        if(nPIIssueDate === 0 && sPINo === ""   && oMotherBuyers.length<=0 && oBuyers.length<=0 && nMktPerson==0 )
        {
            alert("Please select atleast one searching criteriea!");
            return false;
        }
        return true;
    }

    function RefreshObject()
    {
        debugger;
        var oMotherBuyers = $('#txtMotherBuyer').data('MotherBuyers');
        var oBuyers = $('#txtBuyer').data('Buyers');
        var nMktPerson = $('#cboMktPerson').val();

        var nPIIssueDate = parseInt($('#cboPIIssueDate').val());
        var sPIIssueDateStart   = $('#txtPIIssueDateStart').datebox('getValue');
        var sPIIssueDateEnd   = $('#txtPIIssueDateEnd').datebox('getValue');

        var sSearchingData  =  nPIIssueDate+'~';
        sSearchingData = sSearchingData + sPIIssueDateStart+'~';
        sSearchingData = sSearchingData + sPIIssueDateEnd+'~';
        sSearchingData = sSearchingData + nMktPerson;

        var oExportPIRegister = {
            BUID : parseInt(sessionStorage.getItem("BUID")),
            ExportPINo : $.trim($('#txtPINo').val()),
            PIType : parseInt($('#cboPIType').val()),
            CurrencyID:parseInt($('#cboCurrency').val()),
            ProductNatureInt: parseInt($('#cboProductNature').val()),
            MotherBuyerName : ICS_PropertyConcatation(oMotherBuyers, 'ContractorID'),
            BuyerName : ICS_PropertyConcatation(oBuyers, 'ContractorID'),
            DueOptions: $('#cboDueOptions').val(),
            ReportLayout : parseInt($('#cboReportLayout').val()),
            SearchingData : sSearchingData
        };
        return oExportPIRegister;
    }

    $('#btnPreview').click(function(e){
        if(!ValidateSearch()) return;
        var oExportPIRegister = RefreshObject();
        $.ajax({
            type: "POST",
            dataType: "json",
            url : _sBaseAddress+  "/ExportPIRegister/SetSessionSearchCriteria",
            traditional: true,
            data:  JSON.stringify(oExportPIRegister),
            contentType: "application/json; charset=utf-8",
            success: function (data) {
                var sFeedBackMessage = jQuery.parseJSON(data);
                if (sFeedBackMessage==="Successful")
                {
                    var tsv=((new Date()).getTime())/1000;
                    window.open(_sBaseAddress+'/ExportPIRegister/PrintExportPIRegister?ts='+tsv,"_blank");
                }
            },
            error: function (xhr, status, error) {
                alert(error);
            }
        });
    });
    $('#btnExportToExcel').click(function(e){
        if(!ValidateSearch()) return;
        var oExportPIRegister = RefreshObject();
        $.ajax({
            type: "POST",
            dataType: "json",
            url : _sBaseAddress+  "/ExportPIRegister/SetSessionSearchCriteria",
            traditional: true,
            data:  JSON.stringify(oExportPIRegister),
            contentType: "application/json; charset=utf-8",
            success: function (data) {
                var sFeedBackMessage = jQuery.parseJSON(data);
                if (sFeedBackMessage==="Successful") {
                    var tsv=((new Date()).getTime())/1000;
                    window.open(_sBaseAddress+'/ExportPIRegister/ExportToExcelExportPIRegister?ts='+tsv,"_blank");
                }
            },
            error: function (xhr, status, error) {
                alert(error);
            }
        });
    });
    $('#cboPIIssueDate').change(function(e){
        //EqualTo = 1, NotEqualTo = 2, GreaterThen = 3, SmallerThen = 4, Between = 5, NotBetween = 6
        var nCompareOperator =   parseInt($('#cboPIIssueDate').val());
        if(nCompareOperator===0)
        {
            $('#txtPIIssueDateStart').datebox({ disabled : true });
            $('#txtPIIssueDateEnd').datebox({ disabled : true });
        }
        else if (nCompareOperator===1 || nCompareOperator===2 || nCompareOperator===3 || nCompareOperator===4)
        {
            $('#txtPIIssueDateStart').datebox({ disabled : false });
            $('#txtPIIssueDateEnd').datebox({ disabled : true });
        }
        else
        {
            $('#txtPIIssueDateStart').datebox({ disabled : false });
            $('#txtPIIssueDateEnd').datebox({ disabled : false });
        }
        $('#txtPIIssueDateStart').datebox('setValue', icsdateformat(new Date()));
        $('#txtPIIssueDateEnd').datebox('setValue', icsdateformat(new Date()));
    });

    $('#btnClose').click(function (e) {
        window.location.href = window.location.href;
    });

    ///MotherBuyer Pick
    function PickMotherBuyer()
    {
        var nBUID = parseInt(sessionStorage.getItem("BUID"));
        var oContractor = { Params: 6 + '~' + $.trim($('#txtMotherBuyer').val())+'~'+ nBUID };//here 6 is MotherBuyer
        var obj = {
            BaseAddress: sessionStorage.getItem("BaseAddress"),
            Object: oContractor,
            ControllerName: "Contractor",
            ActionName: "ContractorSearchByNameType",
            IsWinClose: false
        };
        $("#progbar").progressbar({ value: 0 });
        $("#progbarParent").show();
        var intervalID = setInterval(updateProgress, 250);
        $.icsDataGets(obj, function (response) {
            clearInterval(intervalID);
            $("#progbarParent").hide();
            if (response.status && response.objs.length > 0) {
                if (response.objs[0].ContractorID > 0) {
                    var tblColums = []; var oColumn = { field: "Name", title: "Name", width: 200, align: "left" }; tblColums.push(oColumn);
                    oColumn = { field: "Address", title: "Address", width: 120, align: "left" }; tblColums.push(oColumn);
                    oColumn = { field: "Email", title: "Email", width: 90, align: "left" }; tblColums.push(oColumn);
                    oColumn = { field: "ShortName", title: "Short Name", width: 90, align: "left" }; tblColums.push(oColumn);
                    var oPickerParam = {
                        winid: 'winMotherBuyers',
                        winclass: 'clsMotherBuyers',
                        winwidth: 600,
                        winheight: 460,
                        tableid: 'tblMotherBuyers',
                        tablecolumns: tblColums,
                        datalist: response.objs,
                        multiplereturn: true,
                        searchingbyfieldName: 'Name',
                        windowTittle: 'MotherBuyer List'
                    };
                    $.icsPicker(oPickerParam);
                    IntializePickerbutton(oPickerParam);
                }
                else { alert(response.objs[0].ErrorMessage); }
            }else{
                alert("Data Not Found.");
                return;
            }
        });
    }
    $("#txtMotherBuyer").keydown(function (e) {
        var code = (e.keyCode ? e.keyCode : e.which);
        if (code == 13) // Enter Press
        {
            if($.trim($('#txtMotherBuyer').val())===null || $.trim($('#txtMotherBuyer').val())==="")
            {
                alert("Press enter with MotherBuyer name");
                return;
            }
            PickMotherBuyer();
        }
    });
    $("#btnPickMotherBuyer").click(function () {
        $('#txtMotherBuyer').val('');
        PickMotherBuyer();
    });
    $('#txtMotherBuyer').keydown(function (e) {
        var code = (e.keyCode ? e.keyCode : e.which);
        if (code == 8) //backspace=8
        {
            $("#txtMotherBuyer").removeClass("fontColorOfPickItem");
            $('#txtMotherBuyer').data('MotherBuyers', []);
        }
    });
    $('#btnClearMotherBuyer').click(function(e){
        $("#txtMotherBuyer").val("");
        $('#txtMotherBuyer').data('MotherBuyers', []);
        $("#txtMotherBuyer").removeClass("fontColorOfPickItem");
    });
    //End MotherBuyer Picker

    ///Buyer Pick
    function PickBuyer()
    {
        var nBUID = parseInt(sessionStorage.getItem("BUID"));
        var oContractor = { Params: 2 + '~' + $.trim($('#txtBuyer').val())+'~'+ nBUID };//here 2 is Buyer
        var obj = {
            BaseAddress: sessionStorage.getItem("BaseAddress"),
            Object: oContractor,
            ControllerName: "Contractor",
            ActionName: "ContractorSearchByNameType",
            IsWinClose: false
        };
        $("#progbar").progressbar({ value: 0 });
        $("#progbarParent").show();
        var intervalID = setInterval(updateProgress, 250);
        $.icsDataGets(obj, function (response) {
            clearInterval(intervalID);
            $("#progbarParent").hide();
            if (response.status && response.objs.length > 0) {
                if (response.objs[0].ContractorID > 0) {
                    var tblColums = []; var oColumn = { field: "Name", title: "Name", width: 200, align: "left" }; tblColums.push(oColumn);
                    oColumn = { field: "Address", title: "Address", width: 120, align: "left" }; tblColums.push(oColumn);
                    oColumn = { field: "Email", title: "Email", width: 90, align: "left" }; tblColums.push(oColumn);
                    oColumn = { field: "ShortName", title: "Short Name", width: 90, align: "left" }; tblColums.push(oColumn);
                    var oPickerParam = {
                        winid: 'winBuyers',
                        winclass: 'clsBuyers',
                        winwidth: 600,
                        winheight: 460,
                        tableid: 'tblBuyers',
                        tablecolumns: tblColums,
                        datalist: response.objs,
                        multiplereturn: true,
                        searchingbyfieldName: 'Name',
                        windowTittle: 'Buyer List'
                    };
                    $.icsPicker(oPickerParam);
                    IntializePickerbutton(oPickerParam);
                }
                else { alert(response.objs[0].ErrorMessage); }
            }else{
                alert("Data Not Found.");
                return;
            }
        });
    }
    $("#txtBuyer").keydown(function (e) {
        var code = (e.keyCode ? e.keyCode : e.which);
        if (code == 13) // Enter Press
        {
            if($.trim($('#txtBuyer').val())===null || $.trim($('#txtBuyer').val())==="")
            {
                alert("Press enter with Buyer name");
                return;
            }
            PickBuyer();
        }
    });
    $("#btnPickBuyer").click(function () {
        $('#txtBuyer').val('');
        PickBuyer();
    });
    $('#txtBuyer').keydown(function (e) {
        var code = (e.keyCode ? e.keyCode : e.which);
        if (code == 8) //backspace=8
        {
            $("#txtBuyer").removeClass("fontColorOfPickItem");
            $('#txtBuyer').data('Buyers', []);
        }
    });
    $('#btnClearBuyer').click(function(e){
        $("#txtBuyer").val("");
        $('#txtBuyer').data('Buyers', []);
        $("#txtBuyer").removeClass("fontColorOfPickItem");
    });
    //End Buyer Picker




    function IntializePickerbutton(oPickerobj) {
        $("#" + oPickerobj.winid).find("#btnOk").click(function () {
            //for Single Select
            SetPickerValueAssign(oPickerobj);
        });
        $(document).find('.' + oPickerobj.winclass).keydown(function (e) {
            if (e.which == 13)//enter=13
            {
                SetPickerValueAssign(oPickerobj);
            }
        });
    }

    function SetPickerValueAssign(oPickerobj) {
        var oreturnObj = null, oreturnobjs = [];
        if (oPickerobj.multiplereturn) {
            oreturnobjs = $('#' + oPickerobj.tableid).datagrid('getChecked');
        } else {
            oreturnObj = $('#' + oPickerobj.tableid).datagrid('getSelected');
        }
        $("#" + oPickerobj.winid).icsWindow("close");
        $("#" + oPickerobj.winid).remove();

        if (oPickerobj.winid === 'winMotherBuyers')
        {
            if (oreturnobjs != null && oreturnobjs.length > 0)
            {
                $('#txtMotherBuyer').val(oreturnobjs.length+"'s MotherBuyers seleted");
                $('#txtMotherBuyer').addClass('fontColorOfPickItem');
                $('#txtMotherBuyer').data('MotherBuyers', oreturnobjs);
                $('#txtMotherBuyer').focus();
            }
        }else  if (oPickerobj.winid === 'winBuyers')
        {
            if (oreturnobjs != null && oreturnobjs.length > 0) {
                $('#txtBuyer').val(oreturnobjs.length+"'s Buyers seleted");
                $('#txtBuyer').addClass('fontColorOfPickItem');
                $('#txtBuyer').data('Buyers', oreturnobjs);
                $('#txtBuyer').focus();
            }
        }

    }

    function updateProgress() {
        var value =$('#progbar').progressbar('getValue');
        if (value < 96){
            value += Math.floor(Math.random() * 10);
            $('#progbar').progressbar('setValue', value);
        }
    }

    function hideShow(miliseconds) {
        $("#progbarParent").hide();
    }
</script>