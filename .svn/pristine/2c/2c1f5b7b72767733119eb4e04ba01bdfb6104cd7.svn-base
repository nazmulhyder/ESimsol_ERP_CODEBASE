@{
    ViewBag.Title = "Spinning Unit Machine List";
}

@model IEnumerable<ESimSol.BusinessObjects.SUVehicle>
    <head>
        <title></title>
        <script src="@Url.Content("~/Views/SUVehicle/SUVehicle.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Views/SUVehicle/SUVehicles.js")" type="text/javascript"></script>
      
    </head>
    <body>
       
        <div id="winSUVehicle" class="easyui-window winstyle" title="Spinnig Unit Vehicles" data-options="modal:true,closed:true,collapsible:false,minimizable:false,maximizable:false,closable:false">
            <fieldset>
                <table>
                    <tr>

                        <td style="width:100px;" align="right">
                            <label>Vehicle No :</label>
                        </td>
                        <td style="width:250px;" colspan="2">
                            <input id="txtVehicleNoSUVehicle" type="text" style="width:245px;"/>
                        </td>
                    </tr>
                    <tr>
                        <td style="width:100px;" align="right">
                            <label>Driver Name :</label>
                        </td>
                        <td style="width:250px;">
                            <input id="txtDriverNameSUVehicle" type="text" style="width:245px;" />
                        </td>
                    </tr>
                    <tr>
                        <td style="width:100px;" align="right">
                            <label>Contact No :</label>
                        </td>
                        <td style="width:250px;">
                            <input id="txtDriverContactNoSUVehicle" type="text" style="width:245px;" />
                        </td>
                    </tr>
                    <tr>
                        <td style="width:100px;" align="right">
                            <label>Remarks :</label>
                        </td>
                        <td style="width:250px;" colspan="2">
                            <input id="txtRemarksSUVehicle" type="text" style="width:245px;" />
                        </td>
                    </tr>
                </table>
            </fieldset>
            <fieldset class="actionfieldsetstyle">
                <legend>Actions : </legend>
                <table>
                    <tr>
                        <td style="width:250px; text-align:right"></td>
                        <td style="width:50px; text-align:right;">
                            <a id="btnSaveSUVehicle" href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-ok" plain="true">Save</a>
                        </td>
                        <td style="width:50px; text-align:right;">
                            <a id="btnCloseSUVehicle" href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-cancel" plain="true">Close</a>
                        </td>
                    </tr>
                </table>
            </fieldset>
        </div>
        <div class="menuMainCollectionTable">
            <table id="tblSUVehicles" title="Spinning Unit Machine List" class="easyui-datagrid" fit="true" fitcolumns="false" rownumbers="true" pagination="false" singleselect="true" autorowheight="false" toolbar="#toolbarSUVehicle"></table>
            <div id="toolbarSUVehicle">
                <table>
                    <tr>
                        <td>
                            <input id="txtVehicleNoSUVehicles" type="text" style="width:150px" placeholder="Search by Vehicle No" />
                            <a id="btnNewSUVehicles" href="javascript:void(0)" class="easyui-linkbutton easyui-tooltip" title="Add Vehicle" iconcls="icon-add" plain="true">Add</a>
                            <a id="btnEditSUVehicles" href="javascript:void(0)" class="easyui-linkbutton easyui-tooltip" title="Edit Vehicle" iconcls="icon-edit" plain="true">Edit</a>
                            <a id="btnViewSUVehicles" href="javascript:void(0)" class="easyui-linkbutton easyui-tooltip" title="View Vehicle" iconcls="icon-details" plain="true">Details</a>
                            <a id="btnDeleteSUVehicles" href="javascript:void(0)" class="easyui-linkbutton easyui-tooltip" title="Delete Selected Machine" iconcls="icon-remove" plain="true">Delete</a>
                        </td>
                        <td>
                            @using (Html.BeginForm("PrintSUVehicles", "SUVehicle", FormMethod.Post, new { enctype = "multipart/form-data", target = "_blank" }))
                            {
                                <input type="text" name="txtCollectionPrintText" id="txtCollectionPrintText" hidden="hidden" />
                                <input type="submit" id="btnPrintList" value="     Print List" class="icon-print PrintList" />
                            }
                        </td>
                    </tr>
                </table>
            </div>
        </div>
    </body>
    
    <script type="text/javascript">
       
    var _oSUVehicle = {ErrorMessage:"",MKTPersonID:0,SUVehicleDateTime:''};
    var _oBaseSUVehicle = {ErrorMessage:"",MKTPersonID:0,SUVehicleDateTime:''};
    var _oSUVehicles = [];
    var _oDBSUVehicles = [];
    var _sBaseAddress = "";
    var _oCurrentUser={};
    
    var _otblColumns=[];
    var _nbtnIDNo =0;
    
    $(document).ready(function() {
        //debugger;
        _sBaseAddress = @Html.Raw(new System.Web.Script.Serialization.JavaScriptSerializer().Serialize(HttpContext.Current.Session[SessionInfo.BaseAddress]));
        _oCurrentUser=@Html.Raw(new System.Web.Script.Serialization.JavaScriptSerializer().Serialize(HttpContext.Current.Session[SessionInfo.CurrentUser]));
        _oDBSUVehicles = @Html.Raw(new System.Web.Script.Serialization.JavaScriptSerializer().Serialize(Model));
        



        //debugger;
        //var data = [];
        //data = _oSUVehicles;
        //data = { "total": "" + data.length + "", "rows": data };
        //$('#tblSUVehicles').datagrid("loadData", data);



        debugger;
        InitializeSUVehiclesEvents();
        InitializeSUVehicleEvents();
    });




    $(document).keydown(function (e) { if (e.keyCode === 27) { $('div').icsWindow("close")}});

</script>
