       _sBackLink=sessionStorage.getItem("BackLink");
       
    });

    function Refresh(nReportType )
    {  


      
        var oProductReconciliationReport= {
                         
            ProductID: _oProductReconciliation.ProductID,
            ReportType:nReportType ,
            StartDate: _dStartDate,
            EndDate:_dEndDate
        }

     
        $.ajax({
            type: "POST",
            dataType: "json",            
            url : _sBaseAddress+  "/ProductReconciliationReport/GetsPRDetail",
            traditional: true,
            data:  JSON.stringify(oProductReconciliationReport),
            contentType: "application/json; charset=utf-8",
            success: function (data) {
           
                var oProductReconciliationReports = jQuery.parseJSON(data);
                if (oProductReconciliationReports.length>0)
                {                    
                    if(nReportType==8)
                    {
                        RefreshList_StockInHand(oProductReconciliationReports);
                    }
                    else if(nReportType==7)
                    {
                        RefreshList_DocInCnF(oProductReconciliationReports);
                    }
                    else if(nReportType==6)
                    {
                        RefreshList_DocReceived(oProductReconciliationReports);
                    }
                    else if(nReportType==5)
                    {
                        RefreshList_ShipmentDone(oProductReconciliationReports);
                    }
                    else if(nReportType==4)
                    {
                        RefreshList_InvoiceWithoutShimment(oProductReconciliationReports);
                    }
                    else if(nReportType==3)
                    {
                        RefreshList_LCOpen(oProductReconciliationReports);
                    }
                    //else if(nReportType==2)
                    //{
                    //    RefreshList_ReqForLC(oProductReconciliationReports);
                    //}
                    else if(nReportType==1)
                    {
                        RefreshList_POReceived(oProductReconciliationReports);
                    }

                    else if(nReportType==10)
                    {
                        RefreshList_ProYetToWithOutLC(oProductReconciliationReports);
                    }
                    else if(nReportType==11)
                    {
                        RefreshList_ProYetToWithLC(oProductReconciliationReports);
                    }

                    else if(nReportType==12)
                    {
                        RefreshList_ProYetToSample(oProductReconciliationReports);
                    }
                    else if(nReportType==13)
                    {
                        RefreshList_LCRecedDOnotRecd(oProductReconciliationReports);
                    }
                    else if(nReportType==14)
                    {
                        RefreshList_PIIssueLCnDONotRecd(oProductReconciliationReports);
                    }

                 
                   
                }
                else 
                {
                    alert("Data not found");
                }
            },
            error: function (xhr, status, error) {
                alert(error);
            }
        });
    }    
