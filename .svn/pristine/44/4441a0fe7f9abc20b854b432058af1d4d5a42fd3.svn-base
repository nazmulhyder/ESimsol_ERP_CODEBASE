                                     if(result.EASPID>0)
                                     {
                                         debugger;
                                         $scope.EmployeeAdvanceSalaryProcessgridOptions.data[index]=result;
                                         $scope.EmployeeAdvanceSalaryProcessgridApi.grid.modifyRows($scope.EmployeeAdvanceSalaryProcessgridOptions.data);
                                         $scope.EmployeeAdvanceSalaryProcessgridApi.selection.selectRow(result);
                                     }
                                     else
                                     {
