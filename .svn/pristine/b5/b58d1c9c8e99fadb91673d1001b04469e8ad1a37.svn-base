@{
    ViewBag.Title = "Export TR List";
}
@model IEnumerable<ESimSol.BusinessObjects.ExportTR>

 <head>
  <title>ExportTR</title>

  <script src="@Url.Content("~/Views/ExportTR/ExportTR.js")" type="text/javascript"></script>
  <script src="@Url.Content("~/Views/ExportTR/ExportTRs.js")" type="text/javascript"></script>
</head>

    
<body>
 
    <div id="winExportTR" class="easyui-window winstyle" title="Bank Informations" data-options="modal:true,closed:true,collapsible:false,minimizable:false,maximizable:false,closable:false">
        <fieldset>
            <table class="tbl">
                <tr>
                    <td class="tdLabel">
                        <label style="font-family:Tahoma">Truck Receipt No :</label>
                    </td>
                    <td class="tdInput">
                        <input id="txtTruckReceiptNo" type="text" disabled="disabled" />
                    </td>
                </tr>
                <tr>
                    <td class="tdLabel">
                        <label>Truck Receipt Date :</label>
                    </td>
                    <td class="tdInput">
                        <input type="text" class="easyui-datebox" style="width: 190px;" id="txtTruckReceiptDate"
                               data-options="formatter:icsdateformat,parser:icsdateparser" />
                    </td>
                </tr>
                <tr>
                    <td class="tdLabel">
                        <label>Carrier :</label>
                    </td>
                    <td class="tdInput">
                        <input type="text" class="textInputType" id="txtCarrier" />
                    </td>
                </tr>
                <tr>
                    <td class="tdLabel">
                        <label>Truck No :</label>
                    </td>
                    <td class="tdInput">
                        <input type="text" class="textInputType" id="txtTruckNo" />
                    </td>
                </tr>
                <tr>
                    <td class="tdLabel">
                        <label>Driver Name :</label>
                    </td>
                    <td class="tdInput">
                        <input type="text" class="textInputType" id="txtDriverName" />
                    </td>
                </tr>

            </table>
        </fieldset>
        <fieldset class="actionfieldsetstyle">
            <legend>Actions : </legend>
            <a id="btnSaveExportTR" href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-save" plain="true">Save</a>
            <a id="btnCloseExportTR" href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-cancel" plain="true">Close</a>
        </fieldset>
    </div>

<div class="menuMainCollectionTable">
    <table id="tblExportTRs" title="Export TR List" class="easyui-datagrid" fit="true" fitcolumns="true" rownumbers="true" pagination="fasle" singleselect="true" autorowheight="false" toolbar="#toolbarExportTR">
        <thead>
            <tr>
                <th field="TruckReceiptNo" width="60">Truck Receipt No</th>
                <th field="TruckReceiptDateInString" width="100" align="center">Truck Receipt Date</th>
                <th field="Carrier" width="120">Carrier</th>
                <th field="TruckNo" width="120">Truck No</th>
                <th field="DriverName" width="120">Driver Name</th>
                <th field="ActivityInString" width="120">Activity</th>
            </tr>
        </thead>
    </table>
    <div id="toolbarExportTR">
        <a id="btnAddExportTR" href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-add" plain="true">Add</a>
        <a id="btnEditExportTR" href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-edit" plain="true">Edit</a>
        <a id="btnDeleteExportTR" href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-remove" plain="true">Delete</a>
        <a id="btnViewExportTR" href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-details" plain="true">Details</a>
        <a id="btnActiveInactive" href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-active" plain="true">Activity</a>
    </div>
</div>

</body>
<style type="text/css">
                          #winExportTR {
                              width: 400px;
                          }
</style>



<script type="text/javascript">
    var _sBaseAddress="";
    var _oExportTR=null;
    var _oExportTRs=[];
    var _nBUID = 0;
    $(document).ready(function () {
        _sBaseAddress =@Html.Raw( new System.Web.Script.Serialization.JavaScriptSerializer().Serialize(HttpContext.Current.Session[SessionInfo.BaseAddress]));
        _oExportTRs =@Html.Raw( new System.Web.Script.Serialization.JavaScriptSerializer().Serialize(Model));
        _nBUID = @Html.Raw( new System.Web.Script.Serialization.JavaScriptSerializer().Serialize(ViewBag.BUID));
        LoadExportTRsEvents();
        LoadExportTREvents();
    });

    $(document).keydown(function (e) { if (e.keyCode == 27) { $('#winExportTR').icsWindow('close'); } });

</script>
