@model IEnumerable<ESimSol.BusinessObjects.EmployeeSalary>
    @{
        ViewBag.Title = "Employee Salary";
    }
    <div style="margin-left:0px; height:500px" class="menuMainCollectionTable">
        <fieldset>
            <legend>Searching Criteria</legend>
            <div style="margin-top:50px; margin-left:200px;">
                <label style="text-align:right; width:160px;">BusinessUnit :</label>
                <input id="txtBusinessUnit_Collection" type="text" style="width:200px;" placeholder="Pick Business Unit" />
                <a id="btnBusinessUnitPicker_Collection" href="javascript:void(0)" class="easyui-linkbutton" plain="true" iconcls="icon-pick"></a>
                <a id="btnResetBusinessUnitPicker_Collection" href="javascript:void(0)" class="easyui-linkbutton" plain="true" iconcls="icon-clear"></a>

                <br />
                <label style="text-align:right; width:160px;">Location :</label>
                <input id="txtLocation_Colection" style="width:200px;" placeholder="Pick Location" type="text" />
                <a id="btnLocationPicker_Colection" href="javascript:void(0)" class="easyui-linkbutton" plain="true" iconcls="icon-pick"></a>
                <a id="btnResetLocationPicker_Colection" href="javascript:void(0)" class="easyui-linkbutton" plain="true" iconcls="icon-clear"></a>

                <br />
                <label style="text-align:right; width:160px;">Department :</label>
                <input id="txtDepartment_Collection" style="width:200px;" type="text" placeholder="Pick Department" />
                <a id="btnDepartmentPicker_Collection" href="javascript:void(0)" class="easyui-linkbutton" plain="true" iconcls="icon-pick"></a>
                <a id="btnResetDepartmentPicker_Collection" href="javascript:void(0)" class="easyui-linkbutton" plain="true" iconcls="icon-clear"></a>

                <br />
                <label style="text-align:right; width:160px;">Designation :</label>
                <input id="txtDesignation_Collection" type="text" style="width:200px;" placeholder="Pick Designation" />
                <a id="btnDesignationPicker_Collection" href="javascript:void(0)" class="easyui-linkbutton" plain="true" iconcls="icon-pick"></a>
                <a id="btnResetDesignationPicker_Collection" href="javascript:void(0)" class="easyui-linkbutton" plain="true" iconcls="icon-clear"></a>

                <br />
                <label style="text-align:right; width:160px;">Scheme :</label>
                <input id="txtSalaryScheme" type="text" style="width:200px;" placeholder="Type salary scheme name & enter" />
                <a id="btnSalaryScheme" href="javascript:void(0)" class="easyui-linkbutton" plain="true" iconcls="icon-pick"></a>
                <a id="btnCSalaryScheme" href="javascript:void(0)" class="easyui-linkbutton" plain="true" iconcls="icon-clear"></a>

                <br />
                <label style="text-align:right; width:160px;">Group :</label>
                <input id="txtEmployeeGroup" style="width:200px;" type="text" placeholder="Enter Group" />
                <a id="btnEmployeeGroupPicker" href="javascript:void(0)" class="easyui-linkbutton" plain="true" iconcls="icon-pick"></a>
                <a id="btnEmployeeGroupPickerReset" href="javascript:void(0)" class="easyui-linkbutton" plain="true" iconcls="icon-clear"></a>

                <br />
                <label style="text-align:right; width:160px;">Block :</label>
                <input id="txtEmployeeBlock" style="width:200px;" type="text" placeholder="Enter Block" />
                <a id="btnEmployeeBlockPicker" href="javascript:void(0)" class="easyui-linkbutton" plain="true" iconcls="icon-pick" />
                <a id="btnEmployeeBlockPickerReset" href="javascript:void(0)" class="easyui-linkbutton" plain="true" iconcls="icon-clear"></a>

                <br />
                <label style="text-align:right; width:160px;">Employee :</label>
                <input id="txtEmployee_Collection" style="width:200px;" type="text" placeholder="Type Code & Enter" />
                <a id="btnEmployee" href="javascript:void(0)" class="easyui-linkbutton" plain="true" iconcls="icon-pick"></a>
                <a id="btnCEmployee" href="javascript:void(0)" class="easyui-linkbutton" plain="true" iconcls="icon-clear"></a>

                <br /> 
                <label style="text-align:right; width:160px;"> S. Range :</label>
                <input id="txtStartSalaryRange" type="text" style="width:90px;" class="number" /> To
                <input id="txtEndeSalaryRange" type="text" style="width:90px;" class="number" />
                <br />
                <label style="text-align:right; width:160px;">
                    <input type="radio" name="rbMonth" value="1" checked="checked" onclick="hideText(this);" />Month Range : 
                </label>

                <select id="cboSalaryMonth_Collection" class="selection-search" style="width: 50px;"></select>
                <select id="cboYear" style="width:60px;"></select>
                And

                <select id="cboSalaryMonth_CollectionTo" class="selection-search" style="width: 50px;"></select>
                <select id="cboYearTo" style="width:60px;"></select>

                <br />
                <label style="text-align:right; width:160px;">
                    <input type="radio" name="rbMonth" value="0" onclick="showText(this);" />Year Range
                </label>
                <select id="cboYearComparison" style="width:60px;" disabled></select>
                To
                <select id="cboYearToComparison" style="width:60px;" disabled></select>

                <br />
                <label style="text-align:right; width:160px;"></label>
                <select id="cboGroupBy" style="width:200px">
                    <option value="0">BUWise</option>
                    <option value="1">Location Wise</option>
                    <option value="2">Dept Wise</option>
                </select>
                <br />
                <label style="text-align:right; width:160px;"></label>

                <a style="margin-left:230px;" id="btnExcel" href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-excel" plain="true" onclick="ExcelPrint()"></a>
                <br />
            </div>
        </fieldset>

        </div>

