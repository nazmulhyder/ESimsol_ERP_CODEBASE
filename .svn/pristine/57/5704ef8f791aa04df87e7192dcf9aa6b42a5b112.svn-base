    function validateManualAtt()
    {
        //debugger;
        sDescription="";
        var CurrInTime=$('#timeSpinner').timespinner('getValue');
        var CurrOutTime=$('#txtOutTime').timespinner('getValue');

        var CurrOSD=$('#chkIsOSD').is(':checked');
        var CurrDayOff=$('#chkIsDayoff').is(':checked');
        var CurrPresent=$('#chkIsPresent').is(':checked');
        var CurrAbsent=$('#chkIsAbsent').is(':checked');
        var CurrConsiderLate=$('#chkIsConsiderLate').is(':checked')?1:0;
        var CurrEarlyLeave=$('#chkIsConsiderEarlyLeave').is(':checked')?1:0;
        var CurrRemark=$('#txtRemark').val();
        var oAttDaily =$("#tblAttendanceDaily").datagrid("getSelected");
        if(CurrRemark!=oAttDaily.Remark)
        {
            sDescription+='Remark :'+oAttDaily.Remark+",";
        }
        if(CurrInTime!=oAttDaily.InTimeInString || CurrOutTime!=oAttDaily.OutTimeInString)
        {
            if(oAttDaily.InTimeInString=="-")
            {
                oAttDaily.InTimeInString="00:00";
            }
            if(oAttDaily.OutTimeInString=="-")
            {
                oAttDaily.OutTimeInString="00:00";
            }

            sDescription+='Att-'+oAttDaily.InTimeInString+"-"+oAttDaily.OutTimeInString+",";
        }
        var PrevConsiderlate=0;
        if(oAttDaily.LateArrivalMinute <= 0  && oAttDaily.InTimeInString != "-") {
            PrevConsiderlate=1;
        }
        if(CurrConsiderLate!=PrevConsiderlate)
        {
            if(PrevConsiderlate==0)
            {
                sDescription+='Cancel No Late,';
            }
            else
            {
                sDescription+='Set No Late,';
            }
        }
        var PrevEarlyLeave=0;
        if(oAttDaily.EarlyDepartureMinute <= 0 && oAttDaily.OutTimeInString != "-") {
            PrevEarlyLeave=1;
        }
        if(CurrEarlyLeave!=PrevEarlyLeave)
        {
            if(PrevEarlyLeave==0)
            {
                sDescription+='Cancel No Early,';
            }
            else
            {
                sDescription+='Set No Early,';
            }
        }

        if(CurrOSD!=oAttDaily.IsOSD)
        {
            if(oAttDaily.IsOSD==1)
            {
                sDescription+='OSD,';
            }
            else
            {
                sDescription+='No OSD,';
            }
        }

        if(CurrDayOff!=oAttDaily.IsDayOff)
        {
            if(oAttDaily.IsDayOff==1)
            {
                sDescription+='DayOff,';
            }
            else
            {
                sDescription+='No DayOff,';
            }
        }
        var PrevPresent=$('#txtOutTime').data('PrevPresent');

        if(oAttDaily.InTimeInString=="-")
        {
            oAttDaily.InTimeInString="00:00";
        }
        if(oAttDaily.OutTimeInString=="-")
        {
            oAttDaily.OutTimeInString="00:00";
        }
        if(CurrInTime==oAttDaily.InTimeInString && CurrPresent==PrevPresent && CurrOutTime==oAttDaily.OutTimeInString  && CurrOSD==oAttDaily.IsOSD && CurrDayOff==oAttDaily.IsDayOff && CurrAbsent==oAttDaily.IsAbsent && CurrConsiderLate==PrevConsiderlate && CurrEarlyLeave==PrevEarlyLeave && CurrRemark==oAttDaily.Remark && CurrAbsent==oAttDaily.IsAbsent)
        {
            alert("Sorry There is no change to Update");
            return false;
        }

        return true;
    }
