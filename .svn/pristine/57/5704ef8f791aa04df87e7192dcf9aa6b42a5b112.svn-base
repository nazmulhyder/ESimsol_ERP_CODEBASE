    function SortAttendanceDailys()
    {
     
        var nPresent = 0;
        var nAbsent = 0;
        var nLate = 0;
        var nEarlyLeaving =0;
        var nOT=0;
        var nNoWork=0;
        var nLeave=0;
        var nNoOutTime=0;

        var nDayoff=0;
        var nHoliday=0;
        var oAttendanceDailys=$("#cboPrintType").data("AttendanceDailys");
        var oPresentAttendanceDailys=[];
        var oAbsentAttendanceDailys=[];
        var oDayoffAttendanceDailys= [];
        var oHolidayAttendanceDailys=[];
        var oLateAttendanceDailys=[];
        var oEarlyAttendanceDailys=[];
        var oOTAttendanceDailys=[];
        var oNoWorkAttendanceDailys= [];
        var oLeaveAttendanceDailys=[];
        var oNoOutTimeAttendanceDailys=[];
        for(var i = 0 ; i< oAttendanceDailys.length ; i++)
        {
            if((oAttendanceDailys[i].InTimeInString !="-" || oAttendanceDailys[i].OutTimeInString !="-" || oAttendanceDailys[i].IsOSD== true) && oAttendanceDailys[i].IsLeave== false )
            {
                oPresentAttendanceDailys.push(GetAttendanceDailyObj(oAttendanceDailys[i]));
                nPresent++;
            }

            if(oAttendanceDailys[i].InTimeInString == "-" && oAttendanceDailys[i].OutTimeInString == "-" && oAttendanceDailys[i].IsLeave== false  && oAttendanceDailys[i].IsOSD== false && oAttendanceDailys[i].LeaveHeadID==false && oAttendanceDailys[i].IsDayOff==false && oAttendanceDailys[i].IsHoliday==false)
            {
                oAbsentAttendanceDailys.push(GetAttendanceDailyObj(oAttendanceDailys[i]));
                nAbsent++;
            }
            
            if(oAttendanceDailys[i].IsDayOff == true)
            {
                oDayoffAttendanceDailys.push(GetAttendanceDailyObj(oAttendanceDailys[i]));
                nDayoff++;
            }
            if(oAttendanceDailys[i].IsHoliday == true)
            {
                oHolidayAttendanceDailys.push(GetAttendanceDailyObj(oAttendanceDailys[i]));
                nHoliday++;
            }
            if(oAttendanceDailys[i].LateArrivalMinute > 0)
            {
                oLateAttendanceDailys.push(GetAttendanceDailyObj(oAttendanceDailys[i]));
                nLate++;
            }

            if(oAttendanceDailys[i].EarlyDepartureMinute > 0)
            {

                oEarlyAttendanceDailys.push(GetAttendanceDailyObj(oAttendanceDailys[i]));
                nEarlyLeaving++;
            }
            if(oAttendanceDailys[i].OverTimeInMinute > 0)
            {
                oOTAttendanceDailys.push(GetAttendanceDailyObj(oAttendanceDailys[i]));
                nOT++;
            }
            if(oAttendanceDailys[i].IsNoWork== true)
            {
                oNoWorkAttendanceDailys.push(GetAttendanceDailyObj(oAttendanceDailys[i]));
                nNoWork++;
            }
            if(oAttendanceDailys[i].IsLeave== true && oAttendanceDailys[i].LeaveHeadID>0)
            {
                oLeaveAttendanceDailys.push(GetAttendanceDailyObj(oAttendanceDailys[i]));
                nLeave++;
            }
            if(oAttendanceDailys[i].OutTimeInString == "-" && oAttendanceDailys[i].InTimeInString != "-")
            {
                oNoOutTimeAttendanceDailys.push(GetAttendanceDailyObj(oAttendanceDailys[i]));
                nNoOutTime++;
            }
        }
        $("#cboPrintType").data("PresentAttendanceDailys",oPresentAttendanceDailys);
        $("#cboPrintType").data("AbsentAttendanceDailys",oAbsentAttendanceDailys);
        $("#cboPrintType").data("LateAttendanceDailys",oLateAttendanceDailys);
        $("#cboPrintType").data("EarlyAttendanceDailys",oEarlyAttendanceDailys);
        $("#cboPrintType").data("OTAttendanceDailys",oOTAttendanceDailys);
        $("#cboPrintType").data("NoWorkAttendanceDailys",oNoWorkAttendanceDailys);
        $("#cboPrintType").data("LeaveAttendanceDailys",oLeaveAttendanceDailys);
        $("#cboPrintType").data("DayoffAttendanceDailys",oDayoffAttendanceDailys);
        $("#cboPrintType").data("HolidayAttendanceDailys",oHolidayAttendanceDailys);
        $("#cboPrintType").data("NoOutTimeAttendanceDailys",oNoOutTimeAttendanceDailys);
  
        $("#cboPrintType").data("Present",nPresent);
        $("#cboPrintType").data("Absent",nAbsent);
        $("#cboPrintType").data("Late",nLate);
