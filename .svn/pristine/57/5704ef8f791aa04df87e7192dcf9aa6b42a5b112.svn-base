    function Search(pageNumber,pageSize)
    {
        debugger;

        if(!ValidateInput())return;
        var oHCMSearchObj=RefreshObject();
        if(pageSize==-2)
        {
            _IsManual=false;
        }
        if(pageSize==-1||_IsManual==true)
        {
            _IsManual=true;
            oHCMSearchObj.IsManual=_IsManual;
        }

        $.ajax({
            type: "POST",
            dataType: "json",
            url: sessionStorage.getItem("BaseAddress") + "/AttendanceDailyV2/GetCount",
            data:JSON.stringify(oHCMSearchObj),
            contentType: "application/json; charset=utf-8",
            success: function (data) {
                debugger;
                var oAtt = jQuery.parseJSON(data);
                $("#cboPrintType").data("TotalAttendanceCount",parseInt(oAtt.TotalAttendanceCount)),
                $('#tblAttendanceDaily').datagrid('getPager').pagination({
               total:parseInt($("#cboPrintType").data("TotalAttendanceCount")),
                pageSize:50
            })

               
            },
            error: function (xhr, status, error) {
                alert(error);
            }
        });

        if(pageSize<=0)
        {
            $("#cboPrintType").data("PrevPageNum",1);
            pageNumber=0;
            pageSize=50;
            $("#cboPrintType").data("AttendanceDailys",[]);
            DynamicRefreshList([], 'tblAttendanceDaily');
        }
      
            
        
            oHCMSearchObj.LoadRecords=pageSize;
            oHCMSearchObj.RowLength= $("#cboPrintType").data("AttendanceDailys").length;
        $.ajax({
            type: "POST",
            dataType: "json",
            url: sessionStorage.getItem("BaseAddress") + "/AttendanceDailyV2/CompAdvanceSearch",
            data:JSON.stringify(oHCMSearchObj),
            contentType: "application/json; charset=utf-8",
            success: function (data) {
                var oAttendanceDailys =[];
                oAttendanceDailys =data;
                if(oAttendanceDailys.length>0)
                {
                    debugger;
                    
                     
                    
                    var otempAttendanceDailys= $("#cboPrintType").data("AttendanceDailys");
                  //  DynamicRefreshList($("#cboPrintType").data("AttendanceDailys"), 'tblAttendanceDaily');
                    var nScrollTo=otempAttendanceDailys.length;
                    for (var j = 0; j < oAttendanceDailys.length; j++)
                    {
                        $('#tblAttendanceDaily').datagrid('appendRow',oAttendanceDailys[j]);
                        //otempAttendanceDailys.push(oAttendanceDailys[j]);
                        
                    }

                    $("#cboPrintType").data("AttendanceDailys", $('#tblAttendanceDaily').datagrid('getRows'));

                    //DynamicRefreshList(otempAttendanceDailys, 'tblAttendanceDaily');
                    RefreshTotalWorkingHour();
                    $('#tblAttendanceDaily').datagrid('scrollTo',parseInt(nScrollTo)+10);
               
                   
                    $("#cboPrintType").data("AttendanceDailys",$('#tblAttendanceDaily').datagrid('getRows'));
                    $('#tblAttendanceDaily').datagrid('getPager').pagination({
                        total:parseInt($("#cboPrintType").data("TotalAttendanceCount")),
                        pageSize:50
                    })
                   
                   SortAttendanceDailys();
                }
                else
                {
                        alert("No more data found by this date !");
              
                }
            },
            error: function (xhr, status, error) {
                alert(error);
            }
        });
    }


