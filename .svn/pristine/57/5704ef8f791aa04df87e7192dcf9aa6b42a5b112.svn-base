    function RefreshTotalWorkingHour()
    {

        var oAttendanceDailys = $('#tblAttendanceDaily').datagrid('getRows');
        if(oAttendanceDailys.length >0)
        {
            var nTotalOT = 0;
            for(var i =0;i<oAttendanceDailys.length;i++)
            {
                nTotalOT = parseFloat(nTotalOT)+parseFloat(oAttendanceDailys[i].OverTimeInMinute);
            }
        }
        var OTST = "";
        if (nTotalOT > 0)
        {
            if (nTotalOT / 60 >= 1) { OTST += ((nTotalOT - nTotalOT % 60) / 60) + "h "; }
            if (nTotalOT % 60 != 0) { OTST += (nTotalOT % 60) + "m"; }
        }
        else OTST="-";
        var FooterField=[];
        var obj=new Object();
        obj['AttendanceDateInString']='<div style="width:100%"><div style="width:50%;float:left;background-color:blue">.</div><div style="width:50%;float:right;text-align:center">DayOff</div></div>';
        obj['InTimeInString']='<div style="width:100%"><div style="width:50%;float:left;background-color:#A400FC">.</div><div style="width:50%;float:right;text-align:center">Leave</div></div>';
        obj['OutTimeInString']='<div style="width:100%"><div style="width:50%;float:left;background-color:Black">.</div><div style="width:50%;float:right;text-align:center">Holiday</div></div>';
        obj['AttStatusInString']='<div style="width:100%"><div style="width:50%;float:left;background-color:green">.</div><div style="width:50%;float:right;text-align:center">Present</div></div>';
        obj['ShiftName']='<div style="width:100%"><div style="width:50%;float:left;background-color:red">.</div><div style="width:50%;float:right;text-align:center">Absent</div></div>';
        obj['TotalWorkingHourSt'] = "Total OT";
        obj['OverTimeInMinuteHourSt'] = OTST;
        FooterField.push(obj);
        $('#tblAttendanceDaily').datagrid('reloadFooter',FooterField);
    }





