<script type="text/javascript">
    var isChkcnt=false,_bIsPressedCheckBox=false,_IsManual=false;
    $(document).ready(function ()
    {
        //debugger;
        var oAURolesMappings=@Html.Raw(new System.Web.Script.Serialization.JavaScriptSerializer().Serialize(ViewBag.AuthorizationRolesMappings));
        var oTimeCards= @Html.Raw(new System.Web.Script.Serialization.JavaScriptSerializer().Serialize(@ViewBag.TimeCards));
        var oTimeCardFormats= @Html.Raw(new System.Web.Script.Serialization.JavaScriptSerializer().Serialize(@ViewBag.TimeCardFormats));
        var nSelectedFormatID= @Html.Raw(new System.Web.Script.Serialization.JavaScriptSerializer().Serialize(@ViewBag.SelectedFormatID));
        var nSelectedTimeCardID= @Html.Raw(new System.Web.Script.Serialization.JavaScriptSerializer().Serialize(@ViewBag.SelectedTimeCardID));
        var oEmployeeCategorys=@Html.Raw(new System.Web.Script.Serialization.JavaScriptSerializer().Serialize(@ViewBag.EmployeeCategorys));
        $("#progressbar").progressbar({ value: 0 });
        $("#progressbarParent").hide();

        $("#cboPrintType").data("Timecards",oTimeCards);
        $("#cboPrintType").data("SelectedTimeCardID",parseInt(nSelectedTimeCardID));
        $("#cboEmpCategory").data("EmployeeCategorys",oEmployeeCategorys);
  
        RefreshAdvSearch();
        RefreshControl();
    });

    $(document).keyup(function(e) {
        if (e.keyCode === 27)
        {
            $('#timeSpinner').timespinner('setValue', '00:00');
            $('#txtOutTime').timespinner('setValue', '00:00');
            $('#txtRemark').val('');
            $('#IsOSD').prop('checked', false);
            $('#chkIsConsiderLate').prop('checked', false);
            $('#chkIsConsiderEarlyLeave').prop('checked', false);
            $('#chkIsAbsent').prop('checked', false);
            $('#chkIsPresent').prop('disabled',false);
            $("#winManualAttendance").icsWindow('close');

            $("#winManualAttendanceHistory").icsWindow('close');
            DynamicRefreshList([],'tblManualAttendanceHistory');
        }
    });


    function updateProgress()
    {
        var value =$('#progressbar').progressbar('getValue');
        if (value < 96){
            value += Math.floor(Math.random() * 5);
            $('#progressbar').progressbar('setValue', value);
        }
    }

    function hideShow(miliseconds)
    {
        $("#progressbarParent").hide();
    }

    function RefreshControl()
    {
        $('#btnAttHistory').hide();
        $('#dtStartDate').datebox('setValue', icsdateformat(new Date()));
        $('#txtStartSalaryRange,#txtEndSalaryRange').icsCurrencyBox(null, null, 2);

        $("#cboPrintType").data("AttendanceDailys",[]);
        $("#cboPrintType").data("PresentAttendanceDailys",[]);
        $("#cboPrintType").data("AbsentAttendanceDailys",[]);
        $("#cboPrintType").data("LateAttendanceDailys",[]);
        $("#cboPrintType").data("EarlyAttendanceDailys",[]);
        $("#cboPrintType").data("OTAttendanceDailys",[]);
        $("#cboPrintType").data("NoWorkAttendanceDailys",[]);
        $("#cboPrintType").data("LeaveAttendanceDailys",[]);
        $("#cboPrintType").data("DayoffAttendanceDailys",[]);
        $("#cboPrintType").data("HolidayAttendanceDailys",[]);
        $("#cboPrintType").data("NoOutTimeAttendanceDailys",[]);
        $("#cboPrintType").data("TotalAttendanceCount",0);
        $("#cboPrintType").data("PrevPageNum",1);
        $("#cboPrintType").data("Present",0);
