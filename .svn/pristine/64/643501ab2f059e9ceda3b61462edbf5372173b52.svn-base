        </td>
        <td width="20%" style="text-align: center; font-size:9px; font-weight:bold">
           Point Name
        </td>
        <td width="40%" style="text-align: center; font-size:9px; font-weight:bold">
            Note
        </td>
        
         
    </tr>
    @foreach (DiagramIdentification oDiagramIdentification in Model.DiagramIdentificationPrint)
    {       
        
            nSLNo++;
           
            <tr border="1">
                <td style="text-align: left; font-size:9px">
                     @nSLNo.ToString()
                </td>
                <td  style="text-align: left; font-size:9px">
                     @oDiagramIdentification.MesurementPoint
                </td>
                <td  style="text-align: left; font-size:9px">
                     @oDiagramIdentification.PointName
                </td>
                <td  style="text-align: left; font-size:9px">
                     @oDiagramIdentification.Note
                </td> 
                        
           </tr>
         
    }

