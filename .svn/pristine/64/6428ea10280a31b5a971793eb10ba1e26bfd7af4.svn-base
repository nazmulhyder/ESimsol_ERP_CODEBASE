    function ValidationRecipe(bIsDyes){

        _oFNLabdipShade = $("#tblFNLabdipShade").datagrid("getSelected");
        if (_oFNLabdipShade == null || _oFNLabdipShade.FNLabdipShadeID <= 0) { alert("Please select an item from list."); return false; }
