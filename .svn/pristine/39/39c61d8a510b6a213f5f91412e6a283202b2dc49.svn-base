                $('#tblFNBatchQCDetail').data('GSM',oFNBQCD.GSM);
                $('#tblFNBatchQCDetail').data('Bowl_Skew',oFNBQCD.Bowl_Skew);
                $('#tblFNBatchQCDetail').data('IsPassed',oFNBQCD.IsPassed);
