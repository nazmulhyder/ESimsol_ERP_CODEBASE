    function SetTotalFNBatchQCFaults()
    {
        $("#txtNoOfFault").val(0);

        var nTFaultPoint_Yard =0;
        var nInspectionPoint_Yard = 0;
        var nFaultPoint_Yard = 0;
        var nTFaultPoint_Yard = 0;
        var oFNBatchQCDetail = $("#tblFNBatchQCDetail").datagrid("getSelected");
        var nIndex=$('#tblFNBatchQCDetail').datagrid('getRowIndex',oFNBatchQCDetail);

        var nTNoOfFault=0;
        var nFaultPoint=0;
        var nFaultTotal=0;
        var oFBQCFaults = $("#tblFNBatchQCFault").datagrid("getRows");
        var nLength = oFBQCFaults.length;
        if(nLength>0)
        {
            for(var i=0;i<nLength;i++)
            {
                nFaultPoint = parseFloat(nFaultPoint) + parseFloat(oFBQCFaults[i].FaultPoint);
                nTNoOfFault = parseFloat(nTNoOfFault) + parseFloat(oFBQCFaults[i].NoOfFault);
                nFaultTotal = parseFloat(nFaultTotal) + parseFloat(oFBQCFaults[i].FaultTotal);
                nTFaultPoint_Yard = parseFloat(nTFaultPoint_Yard) + parseFloat(oFBQCFaults[i].FaultPoint * oFBQCFaults[i].NoOfFault); //Sum(p => (p.FaultPoint * p.NoOfFault)))
            }

            nFaultPoint_Yard= nTFaultPoint_Yard * 36 * 100; 
            var nWidth = oFNBatchQCDetail.Qty * parseFloat($('#txtActualWidth').val()); 

            if (parseFloat(nFaultPoint_Yard) > 0 && parseFloat(nWidth) > 0)
                nInspectionPoint_Yard = (nFaultPoint_Yard / nWidth);

            //console.log(" TFP: "+nTFaultPoint_Yard+"; ==> FP: "+nFaultPoint_Yard+ "/ Width: "+nWidth+" == INSP: "+nInspectionPoint_Yard);

            oFNBatchQCDetail.PointsYard=parseFloat(nTFaultPoint_Yard).toFixed(2);
            oFNBatchQCDetail.InspectionPoint=parseFloat(nInspectionPoint_Yard).toFixed(2);
            Update_QCDetail(oFNBatchQCDetail, nIndex);
            $("#tblFNBatchQCDetail").datagrid("updateRow", { index: nIndex, row: oFNBatchQCDetail });
        }
        else if(oFNBatchQCDetail!=null)
        {
            oFNBatchQCDetail.InspectionPoint=0;
            Update_QCDetail(oFNBatchQCDetail, nIndex); 
            //console.log("Update_QCDetail-Not Null");
            $("#tblFNBatchQCDetail").datagrid("updateRow", { index: nIndex, row: oFNBatchQCDetail });
        }

        $("#lblTPoint").text(formatPrice(nFaultPoint,2));
        $("#lblTNoOfFault").text(formatPrice(nTNoOfFault,2));
        $("#lblTotalValue").text(formatPrice(nFaultTotal,2));
    }
