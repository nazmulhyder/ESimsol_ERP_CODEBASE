    function Refresh_InspectionPoints()
    {
        var oFBQCDetais = $("#tblFNBatchQCDetail").datagrid("getRows");
        
        var nLength = oFBQCDetais.length;
        if(nLength>0)
        {
            for(var i=0;i<nLength;i++)
            {
                oFNBatchQCDetail=oFBQCDetais[i];
                /* ======================CALCULATE POINTS PER 1000YRD============= */
                var nInspectionPoint_Yard=0;
                var nFaultPoint_Yard= oFNBatchQCDetail.PointsYard * 36* 100;
                var nWidth = oFNBatchQCDetail.Qty * parseFloat($('#txtActualWidth').val()); 

                if (parseFloat(nFaultPoint_Yard) > 0 && parseFloat(nWidth) > 0)
                    nInspectionPoint_Yard = (nFaultPoint_Yard / nWidth);

                //console.log("GRID TFP: "+oFNBatchQCDetail.PointsYard+"; ==> FP: "+nFaultPoint_Yard+ "/ Width: "+nWidth+" == INSP: "+nInspectionPoint_Yard);
                oFNBatchQCDetail.InspectionPoint=parseFloat(nInspectionPoint_Yard).toFixed(2);
                /* =========================END OF CALCULATION=================== */
        
                $("#tblFNBatchQCDetail").datagrid("updateRow", { index: i, row: oFNBatchQCDetail }); 
            }
        }
    }

