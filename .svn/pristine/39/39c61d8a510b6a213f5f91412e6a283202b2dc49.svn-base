    function GetFNBatchQCFalults(oFNBQCD){
        var obj = {
            BaseAddress: _sBaseAddress,
            Object: oFNBQCD,
            ObjectId: oFNBQCD.FNBatchQCDetailID,
            ControllerName: "FNBatch",
            ActionName: "GetFNBatchQCFalults",
            TableId: "tblFNBatchQCFault",
            IsWinClose: false,
            Message:""
        };
        $.icsDataGet(obj, function (response) {
            if (response.status && response.obj != null) {
                if(response.obj.length>0)
                {
                    $('#divFNBatchQC').data('FNBatchQCFaults',response.obj);
                    Refresh_FNBatchQCFaults(response.obj[0].FNBatchQCDetailID);
                    DynamicRefreshList(response.obj,'tblFNBatchQCFault');
                }
                else
                {
                    if(response.obj.length<=0)
                        console.log("No Fault Found!");
                    else
                        console.log(response.obj[0].ErrorMessage);
                }
            }
        });
    }
    function endEditing() {
        debugger;
        if (editIndex == undefined) {
            return true;
        }
        if ($('#tblFNBatchQCDetail').datagrid('validateRow', editIndex)) 
        {
            $('#tblFNBatchQCDetail').datagrid('endEdit', editIndex);
            $('#tblFNBatchQCDetail').datagrid('selectRow', editIndex);
            var oFNBatchQCDetail = $('#tblFNBatchQCDetail').datagrid('getSelected');

            if(oFNBatchQCDetail==null){
                editIndex =undefined;
                return;
            }
        
            debugger;
            var sLotNo=$('#tblFNBatchQCDetail').data('LotNo');
            var nQty=$('#tblFNBatchQCDetail').data('Qty');
            var nGrade=$('#tblFNBatchQCDetail').data('Grade');
            var nShadeID=$('#tblFNBatchQCDetail').data('ShadeID');
            var nGSM=$('#tblFNBatchQCDetail').data('GSM');
            var nBowl_Skew= $('#tblFNBatchQCDetail').data('Bowl_Skew');
            var bIsPassed=$('#tblFNBatchQCDetail').data('IsPassed');
            if(sLotNo != oFNBatchQCDetail.LotNo ||  parseFloat(nQty) != parseFloat(oFNBatchQCDetail.Qty) || parseInt(nGrade)!=parseInt(oFNBatchQCDetail.Grade) || parseInt(nShadeID)!=parseInt(oFNBatchQCDetail.ShadeID)
                || parseInt(nGSM)!=parseInt(oFNBatchQCDetail.GSM) || parseInt(nBowl_Skew)!=parseInt(oFNBatchQCDetail.Bowl_Skew) || parseInt(bIsPassed)!=parseInt(oFNBatchQCDetail.IsPassed) )
            {
                /* ======================CALCULATE POINTS PER 1000YRD============= */
                var nInspectionPoint_Yard=0;
                var nFaultPoint_Yard= oFNBatchQCDetail.PointsYard * 36* 100;
                var nWidth = oFNBatchQCDetail.Qty * parseFloat($('#txtActualWidth').val()); 

                if (parseFloat(nFaultPoint_Yard) > 0 && parseFloat(nWidth) > 0)
                    nInspectionPoint_Yard = (nFaultPoint_Yard / nWidth);

                //console.log(" TFP: "+oFNBatchQCDetail.PointsYard+"; ==> FP: "+nFaultPoint_Yard+ "/ Width: "+nWidth+" == INSP: "+nInspectionPoint_Yard);
                oFNBatchQCDetail.InspectionPoint=parseFloat(nInspectionPoint_Yard).toFixed(2);
                /* =========================END OF CALCULATION=================== */
                //console.log("save end-editing");
                var nIndex=editIndex;
                var obj = {
                    BaseAddress: _sBaseAddress,
                    Object: oFNBatchQCDetail,
                    ObjectId: oFNBatchQCDetail.FNBatchQCDetailID,
                    ControllerName: "FNBatch",
                    ActionName: "FNBatchQCDetailSave",
                    TableId: "",
                    IsWinClose: false,
                    Message: ""
                };
                $.icsSave(obj, function (response) {
                    //console.log(response.obj);
                    if (response.status && response.obj != null) { debugger;
                        if(response.obj.FNBatchQCDetailID>0)
                        {
                            $("#tblFNBatchQCDetail").datagrid("updateRow", { index: nIndex, row: response.obj });
                            SetTotal();
                        }
                        else{
                            $('#tblFNBatchQCDetail').datagrid('selectRow', nIndex).datagrid('beginEdit', nIndex);
                            $('#tblFNBatchQCDetail').datagrid('endEdit', editIndex);
                            return false;
                        }
                    } 
                });
            }
            editIndex = undefined;
            return true;
        }
        else {
            return false;
        }
    }

