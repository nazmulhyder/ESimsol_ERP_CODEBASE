                                         if (row.MKTPName=='Total'){
                                            return 'color:#016B18; font-weight:bold;';
                                        }
                                    }
                    ">

             <thead>
                 <tr>
                     <th field="SCNoFull" width="12%" align="left">PO No</th>
                     <th field="SCDateSt" width="10%" align="center"> Date</th>
