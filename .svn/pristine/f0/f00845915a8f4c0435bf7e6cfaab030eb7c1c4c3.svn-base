        var sTradingDeliveryChallanEndDate   = $('#txtTradingDeliveryChallanEndDate').datebox('getValue');

        var nTradingDeliveryChallanAmount = parseInt($('#cboTradingDeliveryChallanAmount').val());
        var sTradingDeliveryChallanAmountStart = icsRemoveComma($.trim($('#txtTradingDeliveryChallanAmountStart').val()));
        var sTradingDeliveryChallanAmountEnd = icsRemoveComma($.trim($('#txtTradingDeliveryChallanAmountEnd').val()));

        var nDeliveryBy = parseInt($('#cboDeliveryBy').val());
        var oBuyers = $('#txtBuyerName').data('Buyers');
        var oProducts = $('#txtProductName').data('Products');

        if(oBuyers===null) {oBuyers = []; }
        if(oProducts===null) {oProducts = []; }
        var sBuyerIDs =""; var sProductIDs ="";

        for(var i=0; i<oBuyers.length; i++)
        {
            sBuyerIDs  = sBuyerIDs + oBuyers[i].ContractorID+ ",";
        }
        if(sBuyerIDs.length>0)
        {
            sBuyerIDs=sBuyerIDs.substring(0, sBuyerIDs.length-1);
        }
        for(var i=0; i<oProducts.length; i++)
        {
            sProductIDs  = sProductIDs + oProducts[i].ProductID+ ",";
        }
        if(sProductIDs.length>0)
        {
            sProductIDs=sProductIDs.substring(0, sProductIDs.length-1);
        }

        var sSearchingData  =  sChallanNo+'~';
        sSearchingData = sSearchingData + sInvocieNo+'~';
        sSearchingData = sSearchingData + nChallanDate+'~';
        sSearchingData = sSearchingData + sTradingDeliveryChallanStartDate+'~';
        sSearchingData = sSearchingData + sTradingDeliveryChallanEndDate+'~';
        sSearchingData = sSearchingData + nTradingDeliveryChallanAmount+'~';
        sSearchingData = sSearchingData + sTradingDeliveryChallanAmountStart+'~';
        sSearchingData = sSearchingData + sTradingDeliveryChallanAmountEnd+'~';
        sSearchingData = sSearchingData + nDeliveryBy+'~';
        sSearchingData = sSearchingData + sBuyerIDs +'~';
        sSearchingData = sSearchingData + sProductIDs;

        var oTradingDeliveryChallan = {
            Note : sSearchingData,
            BUID : parseInt(sessionStorage.getItem("BUID"))
        };

        $.ajax({
            type: "POST",
            dataType: "JSON",
            url: sessionStorage.getItem("BaseAddress")+  "/TradingDeliveryChallan/AdvanceSearch",
            data:  JSON.stringify(oTradingDeliveryChallan),
            contentType: "application/json; charset=utf-8",
            success: function (data) {
                var  oTradingDeliveryChallans = jQuery.parseJSON(data);
                if (oTradingDeliveryChallans != null) {
                    if(oTradingDeliveryChallans.length>0)
                    {
                        if(oTradingDeliveryChallans[0].ErrorMessage=="")
                        {
                            RefreshList(oTradingDeliveryChallans);
                            $('#tblTradingDeliveryChallans').data('TradingDeliveryChallans', oTradingDeliveryChallans);
                            $("#winAdvanceSearch").icsWindow('close');
                        }
                        else
                        {
                            alert(oTradingDeliveryChallans[0].ErrorMessage);
                        }
                    }
                    else
                    {
                        alert("Data not found!!");
                    }
                }
            },
            error: function (xhr, status, error) {
                alert(error);
            }
        });
    });

    $('#txtSearchByChallanNo').keyup(function (e) {
