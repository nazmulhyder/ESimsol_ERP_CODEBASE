    $('#btnPrintList').click(function (e) {
        var oPurchaseOrders= $('#tblPurchaseOrders').datagrid('getRows');
        var ids ="";
        if(oPurchaseOrders.length >0)
        {
            for(var i =0;i<oPurchaseOrders.length;i++)
            {
                ids =ids+oPurchaseOrders[i].POID+",";
            }
            ids= ids.substring(0, ids.length - 1);
        }
        else{
            return;
        }
        var tsv = ((new Date()).getTime())/1000;
        window.open(sessionStorage.getItem("BaseAddress") + '/PurchaseOrder/PrintPurchaseOrders?ids='+ids+"&ts="+tsv, "_blank");
    });

