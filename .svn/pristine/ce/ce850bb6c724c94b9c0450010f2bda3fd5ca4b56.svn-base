@{
    ViewBag.Title = "Purchase Orders List";
}
@model IEnumerable<ESimSol.BusinessObjects.PurchaseOrder>
<head>
    
</head>
<body>
    <div id="progbarParent" style="width:100%;height:100%;margin:0 auto;text-align:center; margin-left: auto;margin-right: auto;left: 0;right: 0;z-index: 1;">
        <div style="margin: 0px auto;margin-top: 251px;text-align:center;width:390px;">
            <label style="font-size:18px;">Please wait</label>
            <div id="progbar" style="width:100%;height:37px;"></div>
        </div>
    </div>
    <div id="divPO" class="menuMainCollectionTable" style="margin-left: 0px; height:100%; width:100%">
        <table id="tblPurchaseOrders" title="PurchaseOrder List" class="easyui-datagrid" fit="true" fitcolumns="true" rownumbers="true" pagination="true" singleselect="true" autorowheight="false" toolbar="#toolbarPurchaseOrder">
            <thead>
                <tr>
                    <th field="FullPONo" width="10%">PO No</th>
                    <th field="PODateSt" width="10%">Date</th>
                    <th field="PaymentModeSt" width="10%">Payment</th>
                    <th field="POStatusSt" width="10%">Status</th>   
                    <th field="ApprovalStatus" width="150">App Status</th>                 
                    <th field="ContractorName" width="20%">Supplier Name</th>
                    <th field="ConcernPersonName" width="20%">Concern Person</th>
                    <th field="ApprovedByName" width="15%">ApprovedBy</th>
                    <th field="AmountSt" width="15%" align="right">PO Amount</th>
                    <th field="PrepareByName" width="15%">Prepare By Name</th>
                    
                </tr>
            </thead>
        </table>
        <div id="toolbarPurchaseOrder">
            <table>
                <tr>
                    <td>
                        <input type="text" id="txtSearchByPONo" placeholder="Search By PO No" style="width: 100px;" />
                        <a id="btnAdvSearch" href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-search" plain="true">Search</a>
                        <a id="btnAdd" href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-add" plain="true">New</a>
                        <a id="btnEdit" href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-edit" plain="true">Edit</a>
                        <a id="btnView" href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-details" plain="true">View</a>
                        <a id="btnDelete" href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-remove" plain="true">Delete</a>
                        <a id="btnWaitForApproved" href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-reload" plain="true">Wait for Approved</a>
                        <a id="btnSendTo" href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-forword" plain="true">Send</a> 
                        <a id="btnApproved" href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-approved" plain="true">Approve</a>
                        <a id="btnUndoApproved" href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-approved" plain="true">Undo Approve</a>
                        <a id="btnCreatePI" href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-approved" plain="true">Create PI</a>   
                        <a id="btnPrintFormat" href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-setting" plain="true">Print Format</a>                     
                        <a id="btnPreview" href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-print" plain="true">Preview</a>
                        <a id="btnPreviewWithSub" href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-print" plain="true">Preview With Subject</a>
                        <a id="btnPrintList" href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-print" plain="true">Print List</a>
                        <a id="btnWOPreview" href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-print" plain="true">WO</a>
                    </td>
                </tr>
            </table>
        </div>
    </div>
    
    @*send to picker*@
    <div id="winSendTo" class="easyui-window winClass" title="Send To" data-options="modal:true,closed:true,collapsible:false,minimizable:false,maximizable:false,closable:false">
        <div style="width:360px;height:170px; float: left;align-content:center;">
            <div style="margin-left: 10%; margin-top:6%;">
                <table border="0" cellpadding="2" cellspacing="0" style="width:100%;">
                    <tr>
                        <td>
                            <fieldset style="margin-left:-10%;">
                                <table>
                                    <tr>
                                        <td width="30%">Send To </td>
                                        <td width="80%">
                                            <input id="txtSendTo" type="text" required="required" style="width:200px;" disabled />
                                        </td>
                                    </tr>
                                    <tr>
                                        <td width="30%">Person </td>
                                        <td width="80%">
                                            <select id="cboPersonList" style="width:200px;"></select>
                                        </td>
                                    </tr>
                                </table>
                            </fieldset>
                        </td>
                    </tr>
                    <tr>
                        <td style="width: 100%; text-align: right;" colspan="2">
                            <fieldset style="margin-left:-10%;">
                                <legend style="font-weight: bold; text-align:left;">Actions : </legend>
                                <a href="javascript:void(0)" id="btnOkSendTo" class="easyui-linkbutton" iconcls="icon-ok" plain="true">Ok</a>
                                <a href="javascript:void(0)" id="btnCloseSendTo" class="easyui-linkbutton" iconcls="icon-cancel" plain="true">Close</a>
                            </fieldset>
                        </td>
                    </tr>
                </table>
            </div>
        </div>
    </div>

    <div id="winAdvanceSearch" class="easyui-window" title="Advance Search" style="width:540px;height:375px;padding:2px;" data-options="modal:true,closed:true,collapsible:false,minimizable:false,maximizable:false,closable:false">
        <div>
            <fieldset>
                <legend style="font-weight:bold"> Searching Criteria : </legend>
                <table border="0" cellspacing="2" cellpadding="2" style="font-size:11px; font-weight:bold">
                    <tr>
                        <td style="width:170px; text-align:right">
                            PO No :
                        </td>
                        <td style="width:370px">
                            <input type="text" style="width: 370px;" id="txtPONo" />
                        </td>
                    </tr>
                    <tr>
                        <td style="width:170px; text-align:right">
                            Ref Type :
                        </td>
                        <td style="width:370px">
                            <select id="cboRefType" style="width:100%"><option value="0">--Select PO Type--</option><option value="1">Open</option><option value="2">@ViewBag.ClientOperationSetting.Value</option><option value="3">Requisition</option></select>
                        </td>
                    </tr>
                    <tr>
                        <td style="width:170px; text-align:right">
                            Ref No :
                        </td>
                        <td style="width:370px">
                            <input type="text" style="width: 370px;" id="txtRefNo" />
                        </td>
                    </tr>
                    <tr>
                        <td style="width:170px; text-align:right">
                            PO Date :
                        </td>
                        <td style="width:370px">
                            <table border="0" cellpadding="0" cellspacing="0" style="font-size: 12px">
                                <tr>
                                    <td style="width: 120px; font-size: 12px; text-align: left">
                                        <select id="cboPODate" style="width:120px"></select>
                                    </td>
                                    <td style="width: 120px; font-size: 12px">
                                        <input id="txtPOStartDate" type="text" style="width: 120px;" class="easyui-datebox" disabled="disabled" data-options="formatter:icsdateformat,parser:icsdateparser" />
                                    </td>
                                    <td style="width: 10px; font-size: 12px">
                                        To
                                    </td>
                                    <td style="width: 120px; font-size: 12px">
                                        <input id="txtPOEndDate" type="text" style="width: 120px;" class="easyui-datebox" disabled="disabled" data-options="formatter:icsdateformat,parser:icsdateparser" />
                                    </td>
                                </tr>
                            </table>
                        </td>
                    </tr>
                    <tr>
                        <td style="width:170px; text-align:right">
                            PO Amount :
                        </td>
                        <td style="width:370px">
                            <table border="0" cellpadding="0" cellspacing="0" style="font-size: 12px">
                                <tr>
                                    <td style="width: 120px; font-size: 12px; text-align: left">
                                        <select id="cboPOAmount" style="width:120px"></select>
                                    </td>
                                    <td style="width: 120px; font-size: 12px">
                                        <input id="txtPOAmountStart" style="width:114px" />
                                    </td>
                                    <td style="width: 10px; font-size: 12px">
                                        To
                                    </td>
                                    <td style="width: 120px; font-size: 12px">
                                        <input id="txtPOAmountEnd" style="width:114px" />
                                    </td>
                                </tr>
                            </table>
                        </td>
                    </tr>
                    <tr>
                        <td style="width:170px; text-align:right">
                            Approved By :
                        </td>
                        <td style="width:370px">
                            <select id="cboApprovedBy" style="width:375px"></select>
                        </td>
                    </tr>
                    <tr>
                        <td style="width:170px; text-align:right">
                            Supplier(s) :
                        </td>
                        <td style="width:370px">
                            <input type="text" style="width: 282px;" id="txtSupplierName" placeholder="Press enter with supplier name/code" />
                            <input type="button" style="width: 30px;" id="btnSupplierClear" value="C" />
                            <input type="button" style="width: 50px;" id="btnSupplierPicker" value="Pick" />
                        </td>
                    </tr>
                    <tr>
                        <td style="width:170px; text-align:right">
                            Product(s) :
                        </td>
                        <td style="width:370px">
                            <input type="text" style="width: 282px;" id="txtProductName" placeholder="Press enter with product name/code" />
                            <input type="button" style="width: 30px;" id="btnProductClear" value="C" />
                            <input type="button" style="width: 50px;" id="btnProductPicker" value="Pick" />
                        </td>
                    </tr>
                </table>
            </fieldset>
        </div>
        <fieldset style="width:498px; vertical-align:top;">
            <legend style="font-weight:bold"> Action : </legend>
            <table border="0" cellspacing="2" cellpadding="2" style="font-size:11px; font-weight:bold;">
                <tr>
                    <td style="width:100px;text-align:right">
                        <a id="btnAdvSearchReset" href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-reload" plain="true"> Reset</a>
                    </td>
                    <td style="width:408px;text-align:right;">
                        <a id="btnSearch" href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-search" plain="true">Search</a>
                        <a id="btnAdvSearchClose" href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-cancel" plain="true">Close</a>
                    </td>
                </tr>
            </table>
        </fieldset>
    </div>
    <div id="winPrintFormat2" style="width:550px;" class="easyui-window winstyle" title="Adv. Search" data-options="modal:true,closed:true,collapsible:false,minimizable:false,maximizable:false,closable:false">
        <fieldset>
            <legend>Print Setup</legend>
            <table style="width:100%;">
                <tr>
                    <td style="text-align:right;">
                        Print Title :
                    </td>
                    <td>
                        <input id="chkPrintTitleInNormalFormat" type="checkbox" checked onclick="if(this.checked){PrintTitleFormat(1)}" /> Normal Format
                    </td>
                    <td>
                        <input id="chkPrintTitlePadFormat" type="checkbox" onclick="if(this.checked){PrintTitleFormat(2)}" /> Pad Format
                    </td>
                    <td>
                        <input id="chkPrintTitleImageFormat" type="checkbox" onclick="if(this.checked){PrintTitleFormat(3)}" /> Image Format
                    </td>

                </tr>
            </table>
        </fieldset>
        <fieldset class="actionfieldsetstyle">
            <legend>Actions : </legend>
            <a id="btnOkPrintFormat2" href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-ok" plain="true">Ok</a>
            <a id="btnClosePrintFormat2" href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-cancel" plain="true">Close</a>
        </fieldset>
    </div>
</body>

<script type="text/javascript">
    $(document).ready(function() {
        var oPurchaseOrders = @Html.Raw(new System.Web.Script.Serialization.JavaScriptSerializer().Serialize(Model));
        var oAuthorizationRolesMapping = @Html.Raw(new System.Web.Script.Serialization.JavaScriptSerializer().Serialize(HttpContext.Current.Session[SessionInfo.AuthorizationRolesMapping]));
        var oApprovalUsers=@Html.Raw(new System.Web.Script.Serialization.JavaScriptSerializer().Serialize(ViewBag.ApprovalUsers));
        var oDateCompareOperatorObjs =@Html.Raw(new System.Web.Script.Serialization.JavaScriptSerializer().Serialize(ViewBag.DateCompareOperatorObjs));
        var oAmountCompareOperatorObjs =@Html.Raw(new System.Web.Script.Serialization.JavaScriptSerializer().Serialize(ViewBag.AmountCompareOperatorObjs));
        var oClientOperationSetting = @Html.Raw(new System.Web.Script.Serialization.JavaScriptSerializer().Serialize(ViewBag.ClientOperationSetting));
        
        var nSelectedRowIndex  =parseInt(sessionStorage.getItem("SelectedRowIndex"));
        var oTempPurchaseOrders =sessionStorage.getItem("PurchaseOrders");
        if(oTempPurchaseOrders!=null)
        {
            oPurchaseOrders = jQuery.parseJSON(oTempPurchaseOrders);
        }

        $('#divPO').data("IsApprovalHead",false);
        sessionStorage.setItem("ImageFormat",1);//default set
        RefreshList(oPurchaseOrders);
        RefreshControlLayout(oAuthorizationRolesMapping);
        $("#progbar").progressbar({ value: 0 });
        $("#progbarParent").hide();
        $('#txtSupplierName').data('Suppliers', []);
        $('#txtProductName').data('Products', []);
        $('#cboApprovedBy').data('ApprovalUsers', oApprovalUsers);
        $('#cboPODate').data('DateCompareOperatorObjs', oDateCompareOperatorObjs);
        $('#cboPOAmount').data('AmountCompareOperatorObj', oAmountCompareOperatorObjs);
        $('#tblPurchaseOrders').data('PurchaseOrders', oPurchaseOrders);
        $('#txtPOAmountStart,#txtPOAmountEnd').icsCurrencyBox();

    });

    $("#btnAdd").click(function(){
        var oPurchaseOrders= $('#tblPurchaseOrders').datagrid('getRows');
        sessionStorage.setItem("PurchaseOrders", JSON.stringify(oPurchaseOrders));
        sessionStorage.setItem("SelectedRowIndex", -1);
        sessionStorage.setItem("PurchaseOrderHeader", "Add PurchaseOrder");
        sessionStorage.setItem("BackLink", window.location.href);
        window.location.href = sessionStorage.getItem("BaseAddress")+ "/PurchaseOrder/ViewPurchaseOrder?buid="+parseInt(sessionStorage.getItem("BUID"))+"&id=0";
    });

    $("#btnEdit").click(function(){
        var oPurchaseOrder= $('#tblPurchaseOrders').datagrid('getSelected');
        if(oPurchaseOrder==null || parseInt(oPurchaseOrder.POID)<=0)
        {
            alert("Please select a item from list!");
            return;
        }
        if(parseInt(oPurchaseOrder.ApproveBy)!=0)
        {
            alert("Please selected purchase Order already approved!");
            return;
        }
        var SelectedRowIndex=$('#tblPurchaseOrders').datagrid('getRowIndex',oPurchaseOrder);
        var oPurchaseOrders= $('#tblPurchaseOrders').datagrid('getRows');
        sessionStorage.setItem("PurchaseOrders", JSON.stringify(oPurchaseOrders));
        sessionStorage.setItem("SelectedRowIndex", SelectedRowIndex);
        sessionStorage.setItem("PurchaseOrderHeader", "Edit PurchaseOrder");
        sessionStorage.setItem("BackLink", window.location.href);
        window.location.href = sessionStorage.getItem("BaseAddress")+  "/PurchaseOrder/ViewPurchaseOrder?buid="+parseInt(oPurchaseOrder.BUID)+"&id="+parseInt(oPurchaseOrder.POID);
    });

    $("#btnView").click(function(){
        var oPurchaseOrder= $('#tblPurchaseOrders').datagrid('getSelected');
        if(oPurchaseOrder==null || parseInt(oPurchaseOrder.POID)<=0)
        {
            alert("Please select a item from list!");
            return;
        }
        var SelectedRowIndex=$('#tblPurchaseOrders').datagrid('getRowIndex',oPurchaseOrder);
        var oPurchaseOrders= $('#tblPurchaseOrders').datagrid('getRows');
        sessionStorage.setItem("PurchaseOrders", JSON.stringify(oPurchaseOrders));
        sessionStorage.setItem("SelectedRowIndex", SelectedRowIndex);
        sessionStorage.setItem("PurchaseOrderHeader", "View PurchaseOrder");
        sessionStorage.setItem("BackLink", window.location.href);
        window.location.href = sessionStorage.getItem("BaseAddress")+  "/PurchaseOrder/ViewPurchaseOrder?buid="+parseInt(oPurchaseOrder.BUID)+"&id="+parseInt(oPurchaseOrder.POID);
    });

    $('#btnDelete').click(function (e) {
        var oPurchaseOrder= $('#tblPurchaseOrders').datagrid('getSelected');
        if(oPurchaseOrder==null || parseInt(oPurchaseOrder.POID)<=0)
        {
            alert("Invalid Field!!! please select a valid Field!");
            return false;
        }
        if(parseInt(oPurchaseOrder.ApproveBy)!=0)
        {
            alert("Please selected purchase Order already approved!");
            return;
        }
        if (!confirm("Confirm to Delete?")) return ;
        var SelectedRowIndex=$('#tblPurchaseOrders').datagrid('getRowIndex',oPurchaseOrder);
        $.ajax({
            type: "POST",
            dataType: "json",
            url: sessionStorage.getItem("BaseAddress")+ "/PurchaseOrder/Delete",
            traditional: true,
            data:  JSON.stringify(oPurchaseOrder),
            contentType: "application/json; charset=utf-8",
            success: function (data) {
                debugger;
                feedbackmessage = jQuery.parseJSON(data);
                if (feedbackmessage == "Deleted")
                {
                    alert("Delete sucessfully");
                    $('#tblPurchaseOrders').datagrid('deleteRow',SelectedRowIndex);
                }
                else
                {
                    alert(feedbackmessage);
                }
            },
            error: function (xhr, status, error) {
                alert(error);
            }
        });
    });

    $('#btnApproved').click(function (e) {
        var oPurchaseOrder= $('#tblPurchaseOrders').datagrid('getSelected');
        if(oPurchaseOrder==null || parseInt(oPurchaseOrder.POID)<=0)
        {
            alert("Invalid Field!!! please select a valid Field!");
            return false;
        }

        if(parseInt(oPurchaseOrder.ApproveBy)!=0)
        {
            alert("Selected purchase PO already approved!");
            return;
        }

        if($('#divPO').data("IsApprovalHead") && oPurchaseOrder.LastApprovalSequence!=oPurchaseOrder.ApprovalSequence)
        {
            alert("Invalid Approval State!!"); return false;
        }

        if (!confirm("Confirm to Approved?")) return ;
        var SelectedRowIndex=$('#tblPurchaseOrders').datagrid('getRowIndex',oPurchaseOrder);
        $.ajax({
            type: "POST",
            dataType: "json",
            url: sessionStorage.getItem("BaseAddress")+ "/PurchaseOrder/Approved",
            traditional: true,
            data:  JSON.stringify(oPurchaseOrder),
            contentType: "application/json; charset=utf-8",
            success: function (data) {
                debugger;
                oPurchaseOrder = jQuery.parseJSON(data);
                if (parseInt(oPurchaseOrder.POID)>0) {
                    alert("Approved Successfully");
                    $('#tblPurchaseOrders').datagrid('updateRow',{index: SelectedRowIndex,	row: oPurchaseOrder});
                }
                else {
                    alert(oPurchaseOrder.ErrorMessage);
                }
            },
            error: function (xhr, status, error) {
                alert(error);
            }
        });
    });

    $('#btnUndoApproved').click(function (e) {
        var oPurchaseOrder= $('#tblPurchaseOrders').datagrid('getSelected');
        if(oPurchaseOrder==null || parseInt(oPurchaseOrder.POID)<=0)
        {
            alert("Invalid Field!!! please select a valid Field!");
            return false;
        }

        if(parseInt(oPurchaseOrder.Status) == 0)
        {
            alert("Selected Purchase Order is Initialize!");
            return;
        }

        //if($('#divPO').data("IsApprovalHead") && oPurchaseOrder.LastApprovalSequence!=oPurchaseOrder.ApprovalSequence)
        //{
        //    alert("Invalid Approval State!!"); return false;
        //}

        if (!confirm("Confirm to Undo Approved?")) return ;
        var SelectedRowIndex=$('#tblPurchaseOrders').datagrid('getRowIndex',oPurchaseOrder);
        $.ajax({
            type: "POST",
            dataType: "json",
            url: sessionStorage.getItem("BaseAddress")+ "/PurchaseOrder/UndoApproved",
            traditional: true,
            data:  JSON.stringify(oPurchaseOrder),
            contentType: "application/json; charset=utf-8",
            success: function (data) {
                debugger;
                oPurchaseOrder = jQuery.parseJSON(data);
                if (parseInt(oPurchaseOrder.POID)>0) {
                    alert("Undo Approve Successfully");
                    $('#tblPurchaseOrders').datagrid('updateRow',{index: SelectedRowIndex,	row: oPurchaseOrder});
                }
                else {
                    alert(oPurchaseOrder.ErrorMessage);
                }
            },
            error: function (xhr, status, error) {
                alert(error);
            }
        });
    });

    $('#btnWaitForApproved').click(function (e) {
        var oPurchaseOrder = {
            PurchaseOrderID : 0,
            BUID : parseInt(sessionStorage.getItem("BUID"))
        };

        $.ajax({
            type: "POST",
            dataType: "JSON",
            url: sessionStorage.getItem("BaseAddress")+  "/PurchaseOrder/WaitForApproval",
            data:  JSON.stringify(oPurchaseOrder),
            contentType: "application/json; charset=utf-8",
            success: function (data) {
                var  oPurchaseOrders = jQuery.parseJSON(data);
                if (oPurchaseOrders != null) {
                    if(oPurchaseOrders.length>0)
                    {
                        if(oPurchaseOrders[0].ErrorMessage=="")
                        {
                            RefreshList(oPurchaseOrders);
                            $('#tblPurchaseOrders').data('PurchaseOrders', oPurchaseOrders);
                        }
                        else
                        {
                            alert(oPurchaseOrders[0].ErrorMessage);
                        }
                    }
                    else
                    {
                        alert("Data not found!!");
                    }
                }
            },
            error: function (xhr, status, error) {
                alert(error);
            }
        });
    });

    $("#btnCreatePI").click(function(){
        var oPurchaseOrder= $('#tblPurchaseOrders').datagrid('getSelected');
        if(oPurchaseOrder==null || parseInt(oPurchaseOrder.POID)<=0)
        {
            alert("Please select a item from list!");
            return;
        }
        if(parseInt(oPurchaseOrder.ApproveBy)==0)
        {
            alert("Your selected purchase Order is not approved!");
            return;
        }
        if(parseInt(oPurchaseOrder.PaymentModeInt)!=2)
        {
            alert("Please select LC type PO!");
            return;
        }

        $.ajax({
            type: "POST",
            dataType: "JSON",
            url: sessionStorage.getItem("BaseAddress")+  "/PurchaseOrder/GetPIByPO",
            data:  JSON.stringify(oPurchaseOrder),
            contentType: "application/json; charset=utf-8",
            success: function (data) {
                var  results = jQuery.parseJSON(data);
                if (results != null) {
                    if(results.length>0)
                    {
                        if(results[0].ErrorMessage=="")
                        {
                            var SelectedRowIndex=$('#tblPurchaseOrders').datagrid('getRowIndex',oPurchaseOrder);
                            var oPurchaseOrders= $('#tblPurchaseOrders').datagrid('getRows');
                            sessionStorage.setItem("PurchaseOrders", JSON.stringify(oPurchaseOrders));
                            sessionStorage.setItem("PurchaseOrder_Info", JSON.stringify(oPurchaseOrder));
                            sessionStorage.setItem("SelectedRowIndex", SelectedRowIndex);
                            sessionStorage.setItem("PurchaseOrderHeader", "Create PI");
                            sessionStorage.setItem("BackLink", window.location.href);
                            window.location.href = sessionStorage.getItem("BaseAddress")+  "/ImportPI/ViewImportPIReference?id="+results[0].ImportPIID+"&buid="+parseInt(oPurchaseOrder.BUID);
                        }
                        else
                        {
                            alert(results[0].ErrorMessage);
                        }
                    }
                    else
                    {
                        var SelectedRowIndex=$('#tblPurchaseOrders').datagrid('getRowIndex',oPurchaseOrder);
                        var oPurchaseOrders= $('#tblPurchaseOrders').datagrid('getRows');
                        sessionStorage.setItem("PurchaseOrders", JSON.stringify(oPurchaseOrders));
                        sessionStorage.setItem("PurchaseOrder_Info", JSON.stringify(oPurchaseOrder));
                        sessionStorage.setItem("SelectedRowIndex", SelectedRowIndex);
                        sessionStorage.setItem("PurchaseOrderHeader", "Create PI");
                        sessionStorage.setItem("BackLink", window.location.href);
                        window.location.href = sessionStorage.getItem("BaseAddress")+  "/ImportPI/ViewImportPIReference?id=0&buid="+parseInt(oPurchaseOrder.BUID);
                    }
                }
            },
            error: function (xhr, status, error) {
                alert(error);
            }
        });


    });



    var nApprovalHeadID =  0;
    $("#btnSendTo").click(function () {
        debugger;

        var oPurchaseOrder= $('#tblPurchaseOrders').datagrid('getSelected');

        if(oPurchaseOrder==null || oPurchaseOrder.PRID<=0)
        {
            alert("Please select a item from list!");
            return false;
        }

        if(oPurchaseOrder.ApproveBy!=0)
        {
            alert("Selected item is already Approved!");
            return false;
        }

        var oApprovalHead = {
            ModuleID:31,
            BUID:oPurchaseOrder.BUID,
            Sequence:oPurchaseOrder.ApprovalSequence + 1
        };
