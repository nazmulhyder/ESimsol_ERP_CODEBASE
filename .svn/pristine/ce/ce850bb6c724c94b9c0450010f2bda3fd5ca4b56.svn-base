    $("#btnPreview").click(function() {

        var oPurchaseOrder= $('#tblPurchaseOrders').datagrid('getSelected');
        if(oPurchaseOrder==null || oPurchaseOrder.POID<=0)
        {
            alert("Invalid Field!!! please select a valid Field!");
            return false;
        }
        var subject = "";
        window.open(sessionStorage.getItem("BaseAddress")+ '/PurchaseOrder/PrintPurchaseOrder?id=' + oPurchaseOrder.POID+'&IsWOPriview=false&Subject='+subject+ "&nTitleTypeInImg="+sessionStorage.getItem("ImageFormat"), "_blank");

    });

    $("#btnPreviewWithSub").click(function() {

        var oPurchaseOrder= $('#tblPurchaseOrders').datagrid('getSelected');
        if(oPurchaseOrder==null || oPurchaseOrder.POID<=0)
        {
            alert("Invalid Field!!! please select a valid Field!");
            return false;
        }
        var subject = prompt("Please type the report subject you prefer", oPurchaseOrder.SubjectName);
      
        if (subject != null && subject != "")
        {
            debugger;
            oPurchaseOrder.SubjectName=subject;
            $.ajax({
                type: "POST",
                dataType: "json",
                url: sessionStorage.getItem("BaseAddress")+ "/PurchaseOrder/UpdateReportSubject",
                traditional: true,
                data:  JSON.stringify(oPurchaseOrder),
                contentType: "application/json; charset=utf-8",
                success: function (data) {
                    debugger;
                    oPurchaseOrder = jQuery.parseJSON(data);
                    if (parseInt(oPurchaseOrder.POID)>0) {
                        
                        window.open(sessionStorage.getItem("BaseAddress")+ '/PurchaseOrder/PrintPurchaseOrder?id=' + oPurchaseOrder.POID+'&IsWOPriview=false&Subject='+oPurchaseOrder.SubjectName+ "&nTitleTypeInImg="+sessionStorage.getItem("ImageFormat"), "_blank");
                    }
                    else {
                        alert(oPurchaseOrder.ErrorMessage);
                    }
                },
                error: function (xhr, status, error) {
                    alert(error);
                }
            });
        }
        else{
            alert("Reprt Subject can not be Empty!");
        }
    });

