    function RefreshControlLayout(oAURolesMapping)
    {
        $("#btnAdvSearch,#btnAdd,#btnEdit,#btnView,#btnDelete,#btnWaitForApproved,#btnApproved,#btnUndoApproved,#btnPreview,#btnWOPreview, #btnPrintList,#btnSendTo").hide();

        if(PermissionChecker('AdvSearch','PurchaseOrder',oAURolesMapping)){$("#btnAdvSearch").show();}
        if(PermissionChecker('Add','PurchaseOrder',oAURolesMapping)){$("#btnAdd").show();}
        if(PermissionChecker('Edit','PurchaseOrder',oAURolesMapping)){$("#btnEdit").show();}
        if(PermissionChecker('View','PurchaseOrder',oAURolesMapping)){$("#btnView").show();}
        if(PermissionChecker('Delete','PurchaseOrder', oAURolesMapping)){$("#btnDelete").show();}
        if(PermissionChecker('Approved','PurchaseOrder',oAURolesMapping)){$("#btnApproved,#btnUndoApproved").show();}
        if(PermissionChecker('Approved','PurchaseOrder',oAURolesMapping)){$("#btnWaitForApproved").show();}
        if(PermissionChecker('Preview','PurchaseOrder',oAURolesMapping)){$("#btnPreview,#btnWOPreview").show();}
        if(PermissionChecker('PrintList','PurchaseOrder',oAURolesMapping)){$("#btnPrintList").show();}

        if(PermissionChecker('FollowUp','PurchaseOrder',oAURolesMapping))
        {
            $("#btnSendTo").show();    $('#divPO').data("IsApprovalHead",true);  console.log($('#divPO').data("IsApprovalHead"));
        }
        else $('#tblPurchaseOrders').datagrid('hideColumn', 'ApprovalStatus');
    }
</script>