<div class="modal-header">
    <h5>{{modalMessage.headerTitle}}</h5>
</div>
<div class="modal-body" id="modal-body">
    <h5>{{modalMessage.bodyText}}</h5>
</div>
<div class="modal-footer">
    <button class="btn btn-sm btn-success" type="button" ng-click="modalMessageOk()" ng-hide="modalMessage.feedbackType" ng-value><span class="glyphicon glyphicon-ok" aria-hidden="true"></span> {{modalMessage.sucessText}}</button>
    <button class="btn btn-sm btn-danger" type="button" ng-click="modalMessageCancel()"><span class="glyphicon glyphicon-remove" aria-hidden="true"></span>   {{modalMessage.cancelText}}</button>
</div>