$('#chkActive').click(function (e)
{
    if($("#chkActive").is( ":checked" ))
    {
        $('#chkInActive').prop('checked', false);
    }
});

$('#chkInActive').click(function (e)
{
    if($("#chkInActive").is( ":checked" ))
    {
        $('#chkActive').prop('checked', false);
    }
});

function LoadLeaveHead(oLeaveHeads)
{
    var listItems="";
    listItems += "<option value=0>" +"-LeaveHead-" +"</option>";
    for (i = 0; i < oLeaveHeads.length; i++)
    {
        var Name ="";
        if(oLeaveHeads[i].ShortName!="")
        {
            Name=oLeaveHeads[i].ShortName;
        }
        else
        {
            Name=oLeaveHeads[i].Name;
        }
        listItems += "<option value='" + oLeaveHeads[i].LeaveHeadID+"'>" +  Name +"</option>";
    }
    $("#cboLeaveHead").html(listItems);
}

function LoadSession()
{
    var listItems="";
    listItems += "<option value=0>" +"-Session-" +"</option>";
    for (i = 0; i < _oACSs.length; i++)
    {
        listItems += "<option value='" + _oACSs[i].ACSID+"'>" +  _oACSs[i].Session+"</option>";
    }
    $("#cboSession").html(listItems);
}

$('#chkbDate').click(function (e)
{
    if($("#chkbDate").is(':checked'))
    {
        $('#dtFrom').datebox({disabled:false});
        $('#dtTo').datebox({disabled:false});
    }
    else
    {
        $('#dtFrom').datebox({disabled:true});
        $('#dtTo').datebox({disabled:true});
    }
    $('#dtFrom,#dtTo').datebox('setValue', icsdateformat(new Date()));
});

function Print()
{
    var sParams=RetParam();
    console.log(sParams);
    if(sParams=="")
    {
        alert("Please enter at least one searching item!");
        return;
    }
    var tsv=(new Date()).getTime()/1000;
    window.open(_sBaseAddress+ "/LeaveApplication/PrintLeaveLedger?sParams="+sParams+"&ts="+tsv, "_blank");
}
