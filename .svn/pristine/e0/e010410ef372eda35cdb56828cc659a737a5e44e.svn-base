@{
    ViewBag.Title = "Supplier Orders List";
}
@model IEnumerable<ESimSol.BusinessObjects.PurchaseOrder>
<head>
    
</head>
<body>
    <div id="progbarParent" style="width:100%;height:100%;margin:0 auto;text-align:center; margin-left: auto;margin-right: auto;left: 0;right: 0;z-index: 1;">
        <div style="margin: 0px auto;margin-top: 251px;text-align:center;width:390px;">
            <label style="font-size:18px;">Please wait</label>
            <div id="progbar" style="width:100%;height:37px;"></div>
        </div>
    </div>
    <div class="menuMainCollectionTable" style="margin-left: 0px; height:100%; width:100%">
        <table id="tblPurchaseOrders" title="Supplier Order List" class="easyui-datagrid" fit="true" fitcolumns="true" rownumbers="true" pagination="true" singleselect="true" autorowheight="false" toolbar="#toolbarPurchaseOrder">
            <thead>
                <tr>
                    <th field="FullPONo" width="10%">SO No</th>
                    <th field="PODateSt" width="10%">Date</th>
                    <th field="POStatusSt" width="10%">Status</th>                    
                    <th field="ContractorName" width="20%">Supplier Name</th>
                    <th field="ConcernPersonName" width="20%">Concern Person</th>
                    <th field="ApprovedByName" width="15%">ApprovedBy</th>
                    <th field="AmountSt" width="15%" align="right">SO Amount</th>
                </tr>
            </thead>
        </table>
        <div id="toolbarPurchaseOrder">
            <table>
                <tr>
                    <td>
                        <input type="text" id="txtSearchByPONo" placeholder="Search By SO No" style="width: 150px;" />
                        <a id="btnAdvSearch" href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-search" plain="true">Search</a>
                        <a id="btnAdd" href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-add" plain="true">New</a>
                        <a id="btnEdit" href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-edit" plain="true">Edit</a>
                        <a id="btnView" href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-details" plain="true">View</a>
                        <a id="btnDelete" href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-remove" plain="true">Delete</a>
                        <a id="btnWaitForApproved" href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-reload" plain="true">Wait for Approved</a>
                        <a id="btnApproved" href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-approved" plain="true">Approve</a>                        
                        <a id="btnPreview" href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-print" plain="true">Preview</a>
                        <a id="btnPrintList" href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-print" plain="true">Print List</a>
                    </td>
                </tr>
            </table>
        </div>
    </div>
    
    <div id="winAdvanceSearch" class="easyui-window" title="Advance Search" style="width:540px;height:375px;padding:2px;" data-options="modal:true,closed:true,collapsible:false,minimizable:false,maximizable:false,closable:false">
        <div>
            <fieldset>
                <legend style="font-weight:bold"> Searching Criteria : </legend>
                <table border="0" cellspacing="2" cellpadding="2" style="font-size:11px; font-weight:bold">
                    <tr>
                        <td style="width:170px; text-align:right">
                            SO No :
                        </td>
                        <td style="width:370px">
                            <input type="text" style="width: 370px;" id="txtPONo" />
                        </td>
                    </tr>
                    <tr>
                        <td style="width:170px; text-align:right">
                            Ref Type :
                        </td>
                        <td style="width:370px">
                            <select id="cboRefType" style="width:100%"><option value="0">--Select SO Type--</option><option value="1">Open</option><option value="3">Spare Parts Request</option></select>
                        </td>
                    </tr>
                    <tr>
                        <td style="width:170px; text-align:right">
                            Ref No :
                        </td>
                        <td style="width:370px">
                            <input type="text" style="width: 370px;" id="txtRefNo" />
                        </td>
                    </tr>
                    <tr>
                        <td style="width:170px; text-align:right">
                            SO Date :
                        </td>
                        <td style="width:370px">
                            <table border="0" cellpadding="0" cellspacing="0" style="font-size: 12px">
                                <tr>
                                    <td style="width: 120px; font-size: 12px; text-align: left">
                                        <select id="cboPODate" style="width:120px"></select>
                                    </td>
                                    <td style="width: 120px; font-size: 12px">
                                        <input id="txtPOStartDate" type="text" style="width: 120px;" class="easyui-datebox" disabled="disabled" data-options="formatter:icsdateformat,parser:icsdateparser" />
                                    </td>
                                    <td style="width: 10px; font-size: 12px">
                                        To
                                    </td>
                                    <td style="width: 120px; font-size: 12px">
                                        <input id="txtPOEndDate" type="text" style="width: 120px;" class="easyui-datebox" disabled="disabled" data-options="formatter:icsdateformat,parser:icsdateparser" />
                                    </td>
                                </tr>
                            </table>
                        </td>
                    </tr>
                    <tr>
                        <td style="width:170px; text-align:right">
                            SO Amount :
                        </td>
                        <td style="width:370px">
                            <table border="0" cellpadding="0" cellspacing="0" style="font-size: 12px">
                                <tr>
                                    <td style="width: 120px; font-size: 12px; text-align: left">
                                        <select id="cboPOAmount" style="width:120px"></select>
                                    </td>
                                    <td style="width: 120px; font-size: 12px">
                                        <input id="txtPOAmountStart" style="width:114px" />
                                    </td>
                                    <td style="width: 10px; font-size: 12px">
                                        To
                                    </td>
                                    <td style="width: 120px; font-size: 12px">
                                        <input id="txtPOAmountEnd" style="width:114px" />
                                    </td>
                                </tr>
                            </table>
                        </td>
                    </tr>
                    <tr>
                        <td style="width:170px; text-align:right">
                            Approved By :
                        </td>
                        <td style="width:370px">
                            <select id="cboApprovedBy" style="width:375px"></select>
                        </td>
                    </tr>
                    <tr>
                        <td style="width:170px; text-align:right">
                            Supplier(s) :
                        </td>
                        <td style="width:370px">
                            <input type="text" style="width: 282px;" id="txtSupplierName" placeholder="Press enter with supplier name/code" />
                            <input type="button" style="width: 30px;" id="btnSupplierClear" value="C" />
                            <input type="button" style="width: 50px;" id="btnSupplierPicker" value="Pick" />
                        </td>
                    </tr>
                    <tr>
                        <td style="width:170px; text-align:right">
                            Part(s) :
                        </td>
                        <td style="width:370px">
                            <input type="text" style="width: 282px;" id="txtProductName" placeholder="Press enter with Part No/Name" />
                            <input type="button" style="width: 30px;" id="btnProductClear" value="C" />
                            <input type="button" style="width: 50px;" id="btnProductPicker" value="Pick" />
                        </td>
                    </tr>
                </table>
            </fieldset>
        </div>
        <fieldset style="width:498px; vertical-align:top;">
            <legend style="font-weight:bold"> Action : </legend>
            <table border="0" cellspacing="2" cellpadding="2" style="font-size:11px; font-weight:bold;">
                <tr>
                    <td style="width:100px;text-align:right">
                        <a id="btnAdvSearchReset" href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-reload" plain="true"> Reset</a>
                    </td>
                    <td style="width:408px;text-align:right;">
                        <a id="btnSearch" href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-search" plain="true">Search</a>
                        <a id="btnAdvSearchClose" href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-cancel" plain="true">Close</a>
                    </td>
                </tr>
            </table>
        </fieldset>
    </div>
</body>

<script type="text/javascript">
    $(document).ready(function() {
        debugger;
        var oPurchaseOrders = @Html.Raw(new System.Web.Script.Serialization.JavaScriptSerializer().Serialize(Model));
        var oAuthorizationRolesMapping = @Html.Raw(new System.Web.Script.Serialization.JavaScriptSerializer().Serialize(HttpContext.Current.Session[SessionInfo.AuthorizationRolesMapping]));
        var oApprovalUsers=@Html.Raw(new System.Web.Script.Serialization.JavaScriptSerializer().Serialize(ViewBag.ApprovalUsers));
        var oDateCompareOperatorObjs =@Html.Raw(new System.Web.Script.Serialization.JavaScriptSerializer().Serialize(ViewBag.DateCompareOperatorObjs));
        var oAmountCompareOperatorObjs =@Html.Raw(new System.Web.Script.Serialization.JavaScriptSerializer().Serialize(ViewBag.AmountCompareOperatorObjs));
        var nSelectedRowIndex  =parseInt(sessionStorage.getItem("SelectedRowIndex"));
        var oTempPurchaseOrders =sessionStorage.getItem("PurchaseOrders");
        if(oTempPurchaseOrders!=null)
        {
            oPurchaseOrders = jQuery.parseJSON(oTempPurchaseOrders);
        }
        RefreshList(oPurchaseOrders);
        RefreshControlLayout(oAuthorizationRolesMapping);
        $("#progbar").progressbar({ value: 0 });
        $("#progbarParent").hide();
        $('#txtSupplierName').data('Suppliers', []);
        $('#txtProductName').data('Products', []);
        $('#cboApprovedBy').data('ApprovalUsers', oApprovalUsers);
        $('#cboPODate').data('DateCompareOperatorObjs', oDateCompareOperatorObjs);
        $('#cboPOAmount').data('AmountCompareOperatorObj', oAmountCompareOperatorObjs);
        $('#tblPurchaseOrders').data('PurchaseOrders', oPurchaseOrders);
        $('#txtPOAmountStart,#txtPOAmountEnd').icsCurrencyBox();
        
    });

    $("#btnAdd").click(function(){
        var oPurchaseOrders= $('#tblPurchaseOrders').datagrid('getRows');
        sessionStorage.setItem("PurchaseOrders", JSON.stringify(oPurchaseOrders));
        sessionStorage.setItem("SelectedRowIndex", -1);
        sessionStorage.setItem("PurchaseOrderHeader", "Add SupplierOrder");
        sessionStorage.setItem("BackLink", window.location.href);
        window.location.href = sessionStorage.getItem("BaseAddress")+ "/SupplierOrder/ViewSupplierOrder?buid="+parseInt(sessionStorage.getItem("BUID"))+"&id=0";
    });

    $("#btnEdit").click(function(){
        var oPurchaseOrder= $('#tblPurchaseOrders').datagrid('getSelected');
        if(oPurchaseOrder==null || parseInt(oPurchaseOrder.POID)<=0)
        {
            alert("Please select a item from list!");
            return;
        }
        if(parseInt(oPurchaseOrder.ApproveBy)!=0)
        {
            alert("Please selected Supplier Order already approved!");
            return;
        }
        var SelectedRowIndex=$('#tblPurchaseOrders').datagrid('getRowIndex',oPurchaseOrder);
        var oPurchaseOrders= $('#tblPurchaseOrders').datagrid('getRows');
        sessionStorage.setItem("PurchaseOrders", JSON.stringify(oPurchaseOrders));
        sessionStorage.setItem("SelectedRowIndex", SelectedRowIndex);
        sessionStorage.setItem("PurchaseOrderHeader", "Edit SupplierOrder");
        sessionStorage.setItem("BackLink", window.location.href);
        window.location.href = sessionStorage.getItem("BaseAddress")+  "/SupplierOrder/ViewSupplierOrder?buid="+parseInt(oPurchaseOrder.BUID)+"&id="+parseInt(oPurchaseOrder.POID);
    });

    $("#btnView").click(function(){
        var oPurchaseOrder= $('#tblPurchaseOrders').datagrid('getSelected');
        if(oPurchaseOrder==null || parseInt(oPurchaseOrder.POID)<=0)
        {
            alert("Please select a item from list!");
            return;
        }
        var SelectedRowIndex=$('#tblPurchaseOrders').datagrid('getRowIndex',oPurchaseOrder);
        var oPurchaseOrders= $('#tblPurchaseOrders').datagrid('getRows');
        sessionStorage.setItem("PurchaseOrders", JSON.stringify(oPurchaseOrders));
        sessionStorage.setItem("SelectedRowIndex", SelectedRowIndex);
        sessionStorage.setItem("PurchaseOrderHeader", "View SupplierOrder");
        sessionStorage.setItem("BackLink", window.location.href);
        window.location.href = sessionStorage.getItem("BaseAddress")+  "/SupplierOrder/ViewSupplierOrder?buid="+parseInt(oPurchaseOrder.BUID)+"&id="+parseInt(oPurchaseOrder.POID);
    });

    $('#btnDelete').click(function (e) {
        var oPurchaseOrder= $('#tblPurchaseOrders').datagrid('getSelected');
        if(oPurchaseOrder==null || parseInt(oPurchaseOrder.POID)<=0)
        {
            alert("Invalid Field!!! please select a valid Field!");
            return false;
        }
        if(parseInt(oPurchaseOrder.ApproveBy)!=0)
        {
            alert("Please selected Supplier Order already approved!");
            return;
        }
        if (!confirm("Confirm to Delete?")) return ;
        var SelectedRowIndex=$('#tblPurchaseOrders').datagrid('getRowIndex',oPurchaseOrder);
        $.ajax({
            type: "POST",
            dataType: "json",
            url: sessionStorage.getItem("BaseAddress")+ "/SupplierOrder/Delete",
            traditional: true,
            data:  JSON.stringify(oPurchaseOrder),
            contentType: "application/json; charset=utf-8",
            success: function (data) {
                debugger;
                feedbackmessage = jQuery.parseJSON(data);
                if (feedbackmessage == "Deleted")
                {
                    alert("Delete sucessfully");
                    $('#tblPurchaseOrders').datagrid('deleteRow',SelectedRowIndex);
                }
                else
                {
                    alert(feedbackmessage);
                }
            },
            error: function (xhr, status, error) {
                alert(error);
            }
        });
    });

    $('#btnApproved').click(function (e) {
        var oPurchaseOrder= $('#tblPurchaseOrders').datagrid('getSelected');
        if(oPurchaseOrder==null || parseInt(oPurchaseOrder.POID)<=0)
        {
            alert("Invalid Field!!! please select a valid Field!");
            return false;
        }
        if(parseInt(oPurchaseOrder.ApproveBy)!=0)
        {
            alert("Please selected purchase PO already approved!");
            return;
        }
        if (!confirm("Confirm to Approved?")) return ;
        var SelectedRowIndex=$('#tblPurchaseOrders').datagrid('getRowIndex',oPurchaseOrder);
        $.ajax({
            type: "POST",
            dataType: "json",
            url: sessionStorage.getItem("BaseAddress")+ "/SupplierOrder/Approved",
            traditional: true,
            data:  JSON.stringify(oPurchaseOrder),
            contentType: "application/json; charset=utf-8",
            success: function (data) {
                debugger;
                oPurchaseOrder = jQuery.parseJSON(data);
                if (parseInt(oPurchaseOrder.POID)>0) {
                    alert("Approved Successfully");
                    $('#tblPurchaseOrders').datagrid('updateRow',{index: SelectedRowIndex,	row: oPurchaseOrder});
                }
                else {
                    alert(oPurchaseOrder.ErrorMessage);
                }
            },
            error: function (xhr, status, error) {
                alert(error);
            }
        });
    });

    $('#btnWaitForApproved').click(function (e) {
        var oPurchaseOrder = {
            PurchaseOrderID : 0,
            BUID : parseInt(sessionStorage.getItem("BUID"))
        };

        $.ajax({
            type: "POST",
            dataType: "JSON",
            url: sessionStorage.getItem("BaseAddress")+  "/SupplierOrder/WaitForApproval",
            data:  JSON.stringify(oPurchaseOrder),
            contentType: "application/json; charset=utf-8",
            success: function (data) {
                var  oPurchaseOrders = jQuery.parseJSON(data);
                if (oPurchaseOrders != null) {
                    if(oPurchaseOrders.length>0)
                    {
                        if(oPurchaseOrders[0].ErrorMessage=="")
                        {
                            RefreshList(oPurchaseOrders);
                            $('#tblPurchaseOrders').data('PurchaseOrders', oPurchaseOrders);
                        }
                        else
                        {
                            alert(oPurchaseOrders[0].ErrorMessage);
                        }
                    }
                    else
                    {
                        alert("Data not found!!");
                    }
                }
            },
            error: function (xhr, status, error) {
                alert(error);
            }
        });
    });

    //Start Search    
    function RefreshComboBoxControls()
    {
        var oApprovalUsers = $('#cboApprovedBy').data('ApprovalUsers');
        var oDateCompareOperatorObjs = $('#cboPODate').data('DateCompareOperatorObjs');
        var oAmountCompareOperatorObjs = $('#cboPOAmount').data('AmountCompareOperatorObj');
        $("#cboApprovedBy").icsLoadCombo({ List: oApprovalUsers, OptionValue: "UserID", DisplayText: "UserName" });
        $("#cboPODate").icsLoadCombo({ List: oDateCompareOperatorObjs, OptionValue: "id", DisplayText: "Value" });
        $("#cboPOAmount").icsLoadCombo({ List: oAmountCompareOperatorObjs, OptionValue: "id", DisplayText: "Value" });
    }
    
    function ValidateSearch()
    {
        var sPONo =$.trim($('#txtPONo').val());
        var sRefNo =$.trim($('#txtRefNo').val());
        var nRefType = parseInt($('#cboRefType').val());

        if(sRefNo!=null && sRefNo!="")
        {
            if(nRefType===0)
            {
                alert("Please select Reference Type!");
                return false;
            }
        }

        var nPODate = parseInt($('#cboPODate').val());
        if(nPODate===1 || nPODate===2 || nPODate===3 || nPODate===4)
        {
            var sPOStartDate   = $('#txtPOStartDate').datebox('getValue');
            if(sPOStartDate===null || sPOStartDate==="")
            {
                alert("Please select PO start date!");
                $('#txtPOStartDate').focus();
                return false;
            }
        }
        if(nPODate===5 || nPODate===6)
        {
            var sPOStartDate   = $('#txtPOStartDate').datebox('getValue');
            var sPOEndDate   = $('#txtPOEndDate').datebox('getValue');
            if(sPOStartDate===null || sPOStartDate==="")
            {
                alert("Please select PO start date!");
                $('#txtPOStartDate').focus();
                return false;
            }
            if(sPOEndDate===null || sPOEndDate==="")
            {
                alert("Please select PO end date!");
                $('#txtPOEndDate').focus();
                return false;
            }
            if(new Date(sPOStartDate) > new Date(sPOEndDate))
            {
                alert("Start date must be smallar than or equal end date!");
                $('#txtPOStartDate').focus();
                return false;
            }
        }

        var nPOAmount = parseInt($('#cboPOAmount').val());
        if(nPOAmount===1 || nPOAmount===2 || nPOAmount===3 || nPOAmount===4)
        {
            var sPOAmountStart   = $.trim($('#txtPOAmountStart').val());
            if(sPOAmountStart===null || sPOAmountStart==="")
            {
                alert("Please enter PO start Amount!");
                $('#txtPOAmountStart').focus();
                return false;
            }
            if(icsRemoveComma(sPOAmountStart)<=0)
            {
                alert("Please enter PO start Amount!");
                $('#txtPOAmountStart').focus();
                return false;
            }
        }
        if(nPOAmount===5 || nPOAmount===6)
        {
            var sPOAmountStart = $.trim($('#txtPOAmountStart').val());
            if(sPOAmountStart===null || sPOAmountStart==="")
            {
                alert("Please enter PO start Amount!");
                $('#txtPOAmountStart').focus();
                return false;
            }
            if(icsRemoveComma(sPOAmountStart)<=0)
            {
                alert("Please enter PO start Amount!");
                $('#txtPOAmountStart').focus();
                return false;
            }

            var sPOAmountEnd = $.trim($('#txtPOAmountEnd').val());
            if(sPOAmountEnd===null || sPOAmountEnd==="")
            {
                alert("Please enter PO end Amount!");
                $('#txtPOAmountEnd').focus();
                return false;
            }
            if(icsRemoveComma(sPOAmountEnd)<=0)
            {
                alert("Please enter PO end Amount!");
                $('#txtPOAmountEnd').focus();
                return false;
            }
            if(icsRemoveComma(sPOAmountStart) >= icsRemoveComma(sPOAmountEnd))
            {
                alert("Start amount must be smallar than end amount!");
                $('#txtPOAmountStart').focus();
                return false;
            }
        }

        var nApprovedBy = parseInt($('#cboApprovedBy').val());
        var oSuppliers = $('#txtSupplierName').data('Suppliers');
        var oProducts = $('#txtProductName').data('Products');
        
        if(sPONo==="" && sRefNo==="" && nRefType===0 && nPODate===0 && nPOAmount===0 && nApprovedBy===0 && oSuppliers.length<=0 && oProducts.length<=0)
        {
            alert("Please select atleast one searching criteriea!");
            return false;
        }       
        return true;
    }

    $('#btnAdvSearch').click(function(e){
        $("#winAdvanceSearch").icsWindow('open', "Advance Search");
        $("#winAdvanceSearch input").not("input[type='button']").val("");
        $('#txtPOStartDate').datebox('setValue', icsdateformat(new Date()));
        $('#txtPOEndDate').datebox('setValue', icsdateformat(new Date()));        
        $('#txtPOAmountStart').val('0.00');
        $('#txtPOAmountEnd').val('0.00');
        $('#txtPOAmountStart').prop("disabled", true);
        $('#txtPOAmountEnd').prop("disabled", true);
        $('#txtSupplierName').data('Suppliers', []);
        $('#txtProductName').data('Products', []);
        RefreshComboBoxControls();
    });

    $('#btnAdvSearchClose').click(function(e){
        $("#winAdvanceSearch").icsWindow('close');
    });

    $('#cboPODate').change(function(e){
        //EqualTo = 1, NotEqualTo = 2, GreaterThen = 3, SmallerThen = 4, Between = 5, NotBetween = 6
        var nCompareOperator =   parseInt($('#cboPODate').val());
        if(nCompareOperator===0)
        {
            $('#txtPOStartDate').datebox({ disabled : true });
            $('#txtPOEndDate').datebox({ disabled : true });
        }
        else if (nCompareOperator===1 || nCompareOperator===2 || nCompareOperator===3 || nCompareOperator===4)
        {
            $('#txtPOStartDate').datebox({ disabled : false });
            $('#txtPOEndDate').datebox({ disabled : true });
        }
        else
        {
            $('#txtPOStartDate').datebox({ disabled : false });
            $('#txtPOEndDate').datebox({ disabled : false });
        }
        $('#txtPOStartDate').datebox('setValue', icsdateformat(new Date()));
        $('#txtPOEndDate').datebox('setValue', icsdateformat(new Date()));
    });

    $('#cboPOAmount').change(function(e){
        //EqualTo = 1, NotEqualTo = 2, GreaterThen = 3, SmallerThen = 4, Between = 5, NotBetween = 6
        var nCompareOperator =   parseInt($('#cboPOAmount').val());
        if(nCompareOperator===0)
        {
            $('#txtPOAmountStart').prop("disabled", true);
            $('#txtPOAmountEnd').prop("disabled", true);
        }
        else if (nCompareOperator===1 || nCompareOperator===2 || nCompareOperator===3 || nCompareOperator===4)
        {
            $('#txtPOAmountStart').prop("disabled", false);
            $('#txtPOAmountEnd').prop("disabled", true);
        }
        else
        {
            $('#txtPOAmountStart').prop("disabled", false);
            $('#txtPOAmountEnd').prop("disabled", false);
        }
        $('#txtPOAmountStart').val('0.00');
        $('#txtPOAmountEnd').val('0.00');
    });

    $('#btnAdvSearchReset').click(function(e){        
        $("#winAdvanceSearch input").not("input[type='button']").val("");
        $('#cboRefType').val(0);
        $('#cboPODate').val(0);
        $('#txtPOStartDate').datebox({ disabled : true });
        $('#txtPOEndDate').datebox({ disabled : true });
        $('#txtPOStartDate').datebox('setValue', icsdateformat(new Date()));
        $('#txtPOEndDate').datebox('setValue', icsdateformat(new Date()));        
        
        $('#cboPOAmount').val(0);        
        $('#txtPOAmountStart').prop("disabled", true);
        $('#txtPOAmountEnd').prop("disabled", true);     
        $('#txtPOAmountStart').val('0.00');
        $('#txtPOAmountEnd').val('0.00');
        
        $('#cboApprovedBy').val(0);
        $('#txtSupplierName').data('Suppliers', []);
        $("#txtSupplierName").removeClass("fontColorOfPickItem");
        $('#txtProductName').data('Products', []);
        $("#txtProductName").removeClass("fontColorOfPickItem");
    });

    $('#btnSearch').click(function(e){
        if(!ValidateSearch()) return;

        var sPONo =$.trim($('#txtPONo').val());
        var sRefNo =$.trim($('#txtRefNo').val());
        var nRefType = parseInt($('#cboRefType').val());
        
        var nPODate = parseInt($('#cboPODate').val());
        var sPOStartDate   = $('#txtPOStartDate').datebox('getValue');
        var sPOEndDate   = $('#txtPOEndDate').datebox('getValue');
        
        var nPOAmount = parseInt($('#cboPOAmount').val());
        var sPOAmountStart = icsRemoveComma($.trim($('#txtPOAmountStart').val()));
        var sPOAmountEnd = icsRemoveComma($.trim($('#txtPOAmountEnd').val()));

        var nApprovedBy = parseInt($('#cboApprovedBy').val());
        var oSuppliers = $('#txtSupplierName').data('Suppliers');
        var oProducts = $('#txtProductName').data('Products');

        if(oSuppliers===null) {oSuppliers = []; }
        if(oProducts===null) {oProducts = []; }
        var sSupplierIDs =""; var sProductIDs ="";

        for(var i=0; i<oSuppliers.length; i++)
        {
            sSupplierIDs  = sSupplierIDs + oSuppliers[i].ContractorID+ ",";
        }
        if(sSupplierIDs.length>0)
        {
            sSupplierIDs=sSupplierIDs.substring(0, sSupplierIDs.length-1);
        }
        for(var i=0; i<oProducts.length; i++)
        {
            sProductIDs  = sProductIDs + oProducts[i].ProductID+ ",";
        }
        if(sProductIDs.length>0)
        {
            sProductIDs=sProductIDs.substring(0, sProductIDs.length-1);
        }

        var sSearchingData  =  sPONo+'~';
        sSearchingData = sSearchingData + sRefNo+'~';
        sSearchingData = sSearchingData + nRefType+'~';
        sSearchingData = sSearchingData + nPODate+'~';        
        sSearchingData = sSearchingData + sPOStartDate+'~';
        sSearchingData = sSearchingData + sPOEndDate+'~';
        sSearchingData = sSearchingData + nPOAmount+'~';
        sSearchingData = sSearchingData + sPOAmountStart+'~';
        sSearchingData = sSearchingData + sPOAmountEnd+'~';
        sSearchingData = sSearchingData + nApprovedBy+'~';    
        sSearchingData = sSearchingData + sSupplierIDs +'~';
        sSearchingData = sSearchingData + sProductIDs;

        var oPurchaseOrder = {    
            Note : sSearchingData,
            BUID : parseInt(sessionStorage.getItem("BUID"))
        };

        $.ajax({
            type: "POST",
            dataType: "JSON",
            url: sessionStorage.getItem("BaseAddress")+  "/SupplierOrder/AdvanceSearch",             
            data:  JSON.stringify(oPurchaseOrder),
            contentType: "application/json; charset=utf-8",
            success: function (data) {                
                var  oPurchaseOrders = jQuery.parseJSON(data);
                if (oPurchaseOrders != null) {                   
                    if(oPurchaseOrders.length>0)
                    {
                        if(oPurchaseOrders[0].ErrorMessage=="")
                        {
                            RefreshList(oPurchaseOrders);
                            $('#tblPurchaseOrders').data('PurchaseOrders', oPurchaseOrders);
                            $("#winAdvanceSearch").icsWindow('close');
                        }
                        else
                        {
                            alert(oPurchaseOrders[0].ErrorMessage);                            
                        }
                    }    
                    else
                    {
                        alert("Data not found!!");                        
                    }              
                }
            },
            error: function (xhr, status, error) {
                alert(error);
            }
        });
    });

