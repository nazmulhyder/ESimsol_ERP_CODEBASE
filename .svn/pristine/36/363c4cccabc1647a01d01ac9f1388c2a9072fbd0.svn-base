body 
{
    font-family: Arial,Helvetica,sans-serif;
	background-color: #CDCDCD;
	margin:5px 0pt 5px;
	font-size: 9pt;
	width: 100%;
	text-align: left;    
   
}

.PikerHeaderRowStyle
{
    background-color: #e6EEEE;
	border: 1px solid #FFF;
	font-size: 9pt;
	padding: 4px;   
    text-align:center;
    font-weight:bold;
}

.PikerBodyRowStyle
{
    color: #3D3D3D;
	padding: 1px;
	background-color: #FFF;
	vertical-align: top;
	background-color:#F0F0F6;
	font-size: 9pt;
	
}

.PikerCellStyle
{
    border: 1px solid #FFFFFF;
    padding: 0px;
}
            
.normal 
{ 
    background-color:#F0F0F6;   
}

.highlight 
{     
     background-color:#D8D8D8;   
}


