<html>
@{
    ViewBag.Title = "Work Order Registers";
}
<body>
    @model ESimSol.BusinessObjects.WorkOrderRegister
    <div id="progbarParent" style="width:100%;height:100%;margin:0 auto;text-align:center; margin-left: auto;margin-right: auto;left: 0;right: 0;z-index: 1;">
        <div style="margin: 0px auto;margin-top: 251px;text-align:center;width:390px;">
            <label style="font-size:18px;">Please wait</label>
            <div id="progbar" style="width:100%;height:37px;"></div>
        </div>
    </div>
    <div class="menuMainCollectionTable">
        <div class="easyui-panel" title="Work Order Registers" style="font-family:Tahoma; text-align:center; height:88%;">
            <fieldset style="height:95%; text-align:center;">
                <legend style="font-weight:bold"> Serching Criteria : </legend>
                <table border="0" cellspacing="2" cellpadding="2" style="font-size:11px; font-weight:bold; text-align:center; width:100%">
                    <tr>
                        <td colspan="5" style="height:20px;"></td>                        
                    </tr>
                    <tr>
                        <td style="width:15%;text-align:right"> WO No :</td>
                        <td style="width:30%;text-align:left"> 
                            <input type="text" style="width:98%" id="txtWONo" />
                        </td>
                        <td style="width:5%;text-align:right"></td>
                        <td style="width:12%;text-align:right"> WO Date :</td>
                        <td style="width:38%;text-align:right">
                            <table border="0" cellpadding="1" cellspacing="1" width="100%">
                                <tr>
                                    <td style="width:30%; text-align:center">
                                        <select id="cboWorkOrderDate" style="width:100%"> </select>
                                    </td>
                                    <td style="width:30%; text-align:center">
                                        <input id="txtWorkOrderDateStart" type="text" class="easyui-datebox" required="required" data-options="formatter:icsdateformat,parser:icsdateparser" style=" width:100%;" />
                                    </td>
                                    <td style="width:10%; text-align:center">to</td>
                                    <td style="width:30%; text-align:center">
                                        <input id="txtWorkOrderDateEnd" type="text" class="easyui-datebox" required="required" data-options="formatter:icsdateformat,parser:icsdateparser" style=" width:100%;" />
                                    </td>
                                </tr>
                            </table> 
                        </td>
                    </tr>
                    <tr>
                        <td style="width:15%;text-align:right"> Supplier :</td>
                        <td style="width:30%;text-align:left">
                            <table border="0" cellpadding="0" cellspacing="0" style="width:100%">
                                <tr>
                                    <td style="width:70%; text-align:left">
                                        <input type="text" style="width:98%" id="txtSupplier" placeholder="Press enter with supplier name" />
                                    </td>
                                    <td style="width:30%; text-align:right">
                                        <input type="button" value="C" style="width:30%" id="btnClearSupplier" />
                                        <input type="button" value="Pick" style="width:65%" id="btnPickSupplier" />
                                    </td>
                                </tr>
                            </table>
                        </td>
                        <td style="width:5%;text-align:right"></td>
                        <td style="width:12%;text-align:right"> Validity Date:</td>
                        <td style="width:38%;text-align:right">
                            <table border="0" cellpadding="1" cellspacing="1" width="100%">
                                <tr>
                                    <td style="width:30%; text-align:center">
                                        <select id="cboExpectedDeliveryDate" style="width:100%"> </select>
                                    </td>
                                    <td style="width:30%; text-align:center">
                                        <input id="txtExpectedDeliveryDateStart" type="text" class="easyui-datebox" required="required" data-options="formatter:icsdateformat,parser:icsdateparser" style=" width:100%;" />
                                    </td>
                                    <td style="width:10%; text-align:center">to</td>
                                    <td style="width:30%; text-align:center">
                                        <input id="txtExpectedDeliveryDateEnd" type="text" class="easyui-datebox" required="required" data-options="formatter:icsdateformat,parser:icsdateparser" style=" width:100%;" />
                                    </td>
                                </tr>
                            </table>
                        </td>
                    </tr>
                    <tr>
                        <td style="width:15%;text-align:right"> Product :</td>
                        <td style="width:30%;text-align:left">
                            <table border="0" cellpadding="0" cellspacing="0" style="width:100%">
                                <tr>
                                    <td style="width:70%; text-align:left">
                                        <input type="text" style="width:98%" id="txtProduct" placeholder="Press enter with product name" />
                                    </td>
                                    <td style="width:30%; text-align:right">
                                        <input type="button" value="C" style="width:30%" id="btnClearProduct" />
                                        <input type="button" value="PIck" style="width:65%" id="btnPickProduct" />
                                    </td>
                                </tr>
                            </table>
                        </td>
                        <td style="width:5%;text-align:right"></td>
                        <td style="width:12%;text-align:right"> Approved Date:</td>
                        <td style="width:38%;text-align:right">
                            <table border="0" cellpadding="1" cellspacing="1" width="100%">
                                <tr>
                                    <td style="width:30%; text-align:center">
                                        <select id="cboWOApprovedDate" style="width:100%"> </select>
                                    </td>
                                    <td style="width:30%; text-align:center">
                                        <input id="txtWOApprovedDateStart" type="text" class="easyui-datebox" required="required" data-options="formatter:icsdateformat,parser:icsdateparser" style=" width:100%;" />
                                    </td>
                                    <td style="width:10%; text-align:center">to</td>
                                    <td style="width:30%; text-align:center">
                                        <input id="txtWOApprovedDateEnd" type="text" class="easyui-datebox" required="required" data-options="formatter:icsdateformat,parser:icsdateparser" style=" width:100%;" />
                                    </td>
                                </tr>
                            </table>
                        </td>
                    </tr>
                    <tr>
                        <td style="width:15%;text-align:right"> File No :</td>
                        <td style="width:30%;text-align:left">
                            <input type="text" style="width:98%" id="txtFileNo" />
                        </td>
                        <td style="width:5%;text-align:right"></td>
                        <td style="width:12%;text-align:right"> WO Amount :</td>
                        <td style="width:38%;text-align:right">
                            <table border="0" cellpadding="1" cellspacing="1" width="100%">
                                <tr>
                                    <td style="width:30%; text-align:center">
                                        <select id="cboWOAmount" style="width:100%"> </select>
                                    </td>
                                    <td style="width:30%; text-align:center">
                                        <input id="txtWOAmountStart" type="text" style=" width:95%;" />
                                    </td>
                                    <td style="width:10%; text-align:center">to</td>
                                    <td style="width:30%; text-align:center">
                                        <input id="txtWOAmountEnd" type="text" style=" width:95%;" />
                                    </td>
                                </tr>
                            </table>
                        </td>
                    </tr>
                    <tr>
                        <td style="width:15%;text-align:right"> Approved By :</td>
                        <td style="width:30%;text-align:left">
                            <select id="cboApprovedBy" style="width:100%">  </select>
                        </td>
                        <td style="width:5%;text-align:right"></td>
                        <td style="width:12%;text-align:right"> WO Status :</td>
                        <td style="width:38%;text-align:right">
                            <select id="cboWOStatus" style="width:100%">  </select>
                        </td>
                    </tr>
                                  
                    <tr>
                        <td style="width:15%;text-align:right"> Report Layout :</td>
                        <td style="width:30%;text-align:left">
                            <select id="cboReportLayout" style="width:100%">  </select>
                        </td>
                        <td style="width:5%;text-align:right"></td>
                        <td style="width:12%;text-align:right"> Remarks :</td>
                        <td style="width:38%;text-align:right">
                            <input type="text" id="txtRemarks" style="width:99%" />
                        </td>
                    </tr>
                </table>
            </fieldset>
        </div>
        <fieldset>
            <legend style="font-weight:bold"> Action : </legend>
            <table border="0" cellspacing="2" cellpadding="2" style="font-size:11px; font-weight:bold; width:100%;">
                <tr>
                    <td style="width:50%;text-align:right">
                                                 
                    </td>
                    <td style="width:50%;text-align:right">
                        <a id="btnPreview" href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-print" plain="true">Preview</a>
                        <a id="btnExportToExcel" href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-excel" plain="true">Export to Excel</a>
                        <a id="btnClose" href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-cancel" plain="true">Close</a>
                    </td>
                </tr>
            </table>
        </fieldset>
    </div>
</body>
</html>


<script type="text/javascript">
    $(document).ready(function () {
        var nBUID =@Html.Raw(new System.Web.Script.Serialization.JavaScriptSerializer().Serialize(ViewBag.BUID));
        var oApprovalUsers = @Html.Raw(new System.Web.Script.Serialization.JavaScriptSerializer().Serialize(ViewBag.ApprovalUsers));
        var oReportLayouts = @Html.Raw(new System.Web.Script.Serialization.JavaScriptSerializer().Serialize(ViewBag.ReportLayouts));
        var oCompareOperators = @Html.Raw(new System.Web.Script.Serialization.JavaScriptSerializer().Serialize(ViewBag.CompareOperators));
        var oWorkOrderStates = @Html.Raw(new System.Web.Script.Serialization.JavaScriptSerializer().Serialize(ViewBag.WorkOrderStates));
        

        $("#progbar").progressbar({ value: 0 });
        $("#progbarParent").hide();
        $('#txtWOAmountStart,#txtWOAmountEnd').icsCurrencyBox();
        $("#cboWorkOrderDate").icsLoadCombo({ List: oCompareOperators, OptionValue: "id", DisplayText: "Value" });
        $("#cboExpectedDeliveryDate").icsLoadCombo({ List: oCompareOperators, OptionValue: "id", DisplayText: "Value" });
        $("#cboWOApprovedDate").icsLoadCombo({ List: oCompareOperators, OptionValue: "id", DisplayText: "Value" });
        $("#cboWOAmount").icsLoadCombo({ List: oCompareOperators, OptionValue: "id", DisplayText: "Value" });
        $("#cboApprovedBy").icsLoadCombo({ List: oApprovalUsers, OptionValue: "UserID", DisplayText: "UserName" });
        $("#cboWOStatus").icsLoadCombo({ List: oWorkOrderStates, OptionValue: "id", DisplayText: "Value" });
        $("#cboReportLayout").icsLoadCombo({ List: oReportLayouts, OptionValue: "id", DisplayText: "Value" });

        $('#txtWorkOrderDateStart').datebox({ disabled : true });
        $('#txtWorkOrderDateEnd').datebox({ disabled : true });
        $('#txtExpectedDeliveryDateStart').datebox({ disabled : true });
        $('#txtExpectedDeliveryDateEnd').datebox({ disabled : true });
        $('#txtWOApprovedDateStart').datebox({ disabled : true });
        $('#txtWOApprovedDateEnd').datebox({ disabled : true });
        $('#txtWOAmountStart').prop("disabled", true);
        $('#txtWOAmountEnd').prop("disabled", true);

        $('#txtWorkOrderDateStart').datebox('setValue', icsdateformat(new Date()));
        $('#txtWorkOrderDateEnd').datebox('setValue', icsdateformat(new Date()));
        $('#txtExpectedDeliveryDateStart').datebox('setValue', icsdateformat(new Date()));
        $('#txtExpectedDeliveryDateEnd').datebox('setValue', icsdateformat(new Date()));
        $('#txtWOApprovedDateStart').datebox('setValue', icsdateformat(new Date()));
        $('#txtWOApprovedDateEnd').datebox('setValue', icsdateformat(new Date()));
        $('#txtWOAmountStart').val('0.00');
        $('#txtWOAmountEnd').val('0.00');

        $('#txtSupplier').data('Suppliers', []); 
        $('#txtProduct').data('Products', []);                
        $('#txtBankBranch').data('BankBranchs', []);
    });

    function ValidateSearch()
    {
        
        var nWorkOrderDate = parseInt($('#cboWorkOrderDate').val());
        if(nWorkOrderDate===1 || nWorkOrderDate===2 || nWorkOrderDate===3 || nWorkOrderDate===4)
        {
            var sWorkOrderDateStart   = $('#txtWorkOrderDateStart').datebox('getValue');
            if(sWorkOrderDateStart===null || sWorkOrderDateStart==="")
            {
                alert("Please select WO start date!");
                $('#txtWorkOrderDateStart').focus();
                return false;
            }
        }
        if(nWorkOrderDate===5 || nWorkOrderDate===6)
        {
            var sWorkOrderDateStart   = $('#txtWorkOrderDateStart').datebox('getValue');
            var sWorkOrderDateEnd   = $('#txtWorkOrderDateEnd').datebox('getValue');
            if(sWorkOrderDateStart===null || sWorkOrderDateStart==="")
            {
                alert("Please select WO start date!");
                $('#txtWorkOrderDateStart').focus();
                return false;
            }
            if(sWorkOrderDateEnd===null || sWorkOrderDateEnd==="")
            {
                alert("Please select WO end date!");
                $('#txtWorkOrderDateEnd').focus();
                return false;
            }
            if(new Date(sWorkOrderDateStart) > new Date(sWorkOrderDateEnd))
            {
                alert("Start date must be smallar than or equal end date!");
                $('#txtWorkOrderDateStart').focus();
                return false;
            }
        }
        
        var nExpectedDeliveryDate = parseInt($('#cboExpectedDeliveryDate').val());
        if(nExpectedDeliveryDate===1 || nExpectedDeliveryDate===2 || nExpectedDeliveryDate===3 || nExpectedDeliveryDate===4)
        {
            var sExpectedDeliveryDateStart   = $('#txtExpectedDeliveryDateStart').datebox('getValue');
            if(sExpectedDeliveryDateStart===null || sExpectedDeliveryDateStart==="")
            {
                alert("Please select validity start date!");
                $('#txtExpectedDeliveryDateStart').focus();
                return false;
            }
        }
        if(nExpectedDeliveryDate===5 || nExpectedDeliveryDate===6)
        {
            var sExpectedDeliveryDateStart   = $('#txtExpectedDeliveryDateStart').datebox('getValue');
            var sExpectedDeliveryDateEnd   = $('#txtExpectedDeliveryDateEnd').datebox('getValue');
            if(sExpectedDeliveryDateStart===null || sExpectedDeliveryDateStart==="")
            {
                alert("Please select validity start date!");
                $('#txtExpectedDeliveryDateStart').focus();
                return false;
            }
            if(sExpectedDeliveryDateEnd===null || sExpectedDeliveryDateEnd==="")
            {
                alert("Please select validity end date!");
                $('#txtExpectedDeliveryDateEnd').focus();
                return false;
            }
            if(new Date(sExpectedDeliveryDateStart) > new Date(sExpectedDeliveryDateEnd))
            {
                alert("Start date must be smallar than or equal end date!");
                $('#txtExpectedDeliveryDateStart').focus();
                return false;
            }
        }

        var nWOApprovedDate = parseInt($('#cboWOApprovedDate').val());
        if(nWOApprovedDate===1 || nWOApprovedDate===2 || nWOApprovedDate===3 || nWOApprovedDate===4)
        {
            var sWOApprovedDateStart   = $('#txtWOApprovedDateStart').datebox('getValue');
            if(sWOApprovedDateStart===null || sWOApprovedDateStart==="")
            {
                alert("Please select approved start date!");
                $('#txtWOApprovedDateStart').focus();
                return false;
            }
        }
        if(nWOApprovedDate===5 || nWOApprovedDate===6)
        {
            var sWOApprovedDateStart   = $('#txtWOApprovedDateStart').datebox('getValue');
            var sWOApprovedDateEnd   = $('#txtWOApprovedDateEnd').datebox('getValue');
            if(sWOApprovedDateStart===null || sWOApprovedDateStart==="")
            {
                alert("Please select approved start date!");
                $('#txtWOApprovedDateStart').focus();
                return false;
            }
            if(sWOApprovedDateEnd===null || sWOApprovedDateEnd==="")
            {
                alert("Please select approved end date!");
                $('#txtWOApprovedDateEnd').focus();
                return false;
            }
            if(new Date(sWOApprovedDateStart) > new Date(sWOApprovedDateEnd))
            {
                alert("Start date must be smallar than or equal end date!");
                $('#txtWOApprovedDateStart').focus();
                return false;
            }
        }

        var nWOAmount = parseInt($('#cboWOAmount').val());
        if(nWOAmount===1 || nWOAmount===2 || nWOAmount===3 || nWOAmount===4)
        {
            var sWOAmountStart   = $.trim($('#txtWOAmountStart').val());
            if(sWOAmountStart===null || sWOAmountStart==="")
            {
                alert("Please enter invoice start Amount!");
                $('#txtWOAmountStart').focus();
                return false;
            }
            if(icsRemoveComma(sWOAmountStart)<=0)
            {
                alert("Please enter WO start Amount!");
                $('#txtWOAmountStart').focus();
                return false;
            }
        }
        if(nWOAmount===5 || nWOAmount===6)
        {
            var sWOAmountStart = $.trim($('#txtWOAmountStart').val());
            if(sWOAmountStart===null || sWOAmountStart==="")
            {
                alert("Please enter WO start Amount!");
                $('#txtWOAmountStart').focus();
                return false;
            }
            if(icsRemoveComma(sWOAmountStart)<=0)
            {
                alert("Please enter WO start Amount!");
                $('#txtWOAmountStart').focus();
                return false;
            }

            var sWOAmountEnd = $.trim($('#txtWOAmountEnd').val());
            if(sWOAmountEnd===null || sWOAmountEnd==="")
            {
                alert("Please enter WO end Amount!");
                $('#txtWOAmountEnd').focus();
                return false;
            }
            if(icsRemoveComma(sWOAmountEnd)<=0)
            {
                alert("Please enter invoice end Amount!");
                $('#txtWOAmountEnd').focus();
                return false;
            }
            if(icsRemoveComma(sWOAmountStart) >= icsRemoveComma(sWOAmountEnd))
            {
                alert("Start amount must be smallar than end amount!");
                $('#txtWOAmountStart').focus();
                return false;
            }
        }

        var nReportLayout = parseInt($('#cboReportLayout').val());
        if(nReportLayout<=0)
        {
            alert("Please select an Report Layout!");
            return;
        }

        var sWONo =$.trim($('#txtWONo').val());
        var sFileNo =$.trim($('#txtFileNo').val());
        var nApprovedBy = parseInt($('#cboApprovedBy').val());
        var nWOStatus = parseInt($('#cboWOStatus').val());
        var sRemarks =$.trim($('#txtRemarks').val());
        var oSuppliers = $('#txtSupplier').data('Suppliers'); 
        var oProducts = $('#txtProduct').data('Products');

        if(nWorkOrderDate === 0 && nExpectedDeliveryDate === 0 && nWOApprovedDate === 0 && nWOAmount === 0 && sWONo === "" && sFileNo === "" && nApprovedBy === 0 && nWOStatus === 0  && sRemarks === "" && oSuppliers.length<=0 && oProducts.length<=0 )
        {
            alert("Please select atleast one searching criteriea!");
            return false;
        }
        return true;
    }

    function RefreshObject()
    {
        var oSuppliers = $('#txtSupplier').data('Suppliers'); 
        var oProducts = $('#txtProduct').data('Products');
               
        var nWorkOrderDate = parseInt($('#cboWorkOrderDate').val());
        var sWorkOrderDateStart   = $('#txtWorkOrderDateStart').datebox('getValue');
        var sWorkOrderDateEnd   = $('#txtWorkOrderDateEnd').datebox('getValue');

        var nExpectedDeliveryDate = parseInt($('#cboExpectedDeliveryDate').val());
        var sExpectedDeliveryDateStart   = $('#txtExpectedDeliveryDateStart').datebox('getValue');
        var sExpectedDeliveryDateEnd   = $('#txtExpectedDeliveryDateEnd').datebox('getValue');

        var nWOApprovedDate = parseInt($('#cboWOApprovedDate').val());
        var sWOApprovedDateStart   = $('#txtWOApprovedDateStart').datebox('getValue');
        var sWOApprovedDateEnd   = $('#txtWOApprovedDateEnd').datebox('getValue');

        var nWOAmount = parseInt($('#cboWOAmount').val());
        var nWOAmountStart = icsRemoveComma($.trim($('#txtWOAmountStart').val()));
        var nWOAmountEnd = icsRemoveComma($.trim($('#txtWOAmountEnd').val()));

        
        var sSearchingData  =  nWorkOrderDate+'~';
        sSearchingData = sSearchingData + sWorkOrderDateStart+'~';
        sSearchingData = sSearchingData + sWorkOrderDateEnd+'~';
        sSearchingData = sSearchingData + nExpectedDeliveryDate+'~';
        sSearchingData = sSearchingData + sExpectedDeliveryDateStart+'~';
        sSearchingData = sSearchingData + sExpectedDeliveryDateEnd+'~';
        sSearchingData = sSearchingData + nWOApprovedDate+'~';
        sSearchingData = sSearchingData + sWOApprovedDateStart+'~';
        sSearchingData = sSearchingData + sWOApprovedDateEnd+'~';
        sSearchingData = sSearchingData + nWOAmount +'~';
        sSearchingData = sSearchingData + nWOAmountStart +'~';
        sSearchingData = sSearchingData + nWOAmountEnd;


        var oWorkOrderRegister = {
            BUID : parseInt(sessionStorage.getItem("BUID")),
            WorkOrderNo : $.trim($('#txtWONo').val()), 
            FileNo : $.trim($('#txtFileNo').val()), 
            ApproveBy : parseInt($('#cboApprovedBy').val()),
            WorkOrderStatus : parseInt($('#cboWOStatus').val()),
            Remarks : $.trim($('#txtRemarks').val()),                   
            SupplierName : ICS_PropertyConcatation(oSuppliers, 'ContractorID'),
            ProductName : ICS_PropertyConcatation(oProducts, 'ProductID'),
            ReportLayout : parseInt($('#cboReportLayout').val()),
            SearchingData : sSearchingData
        };
        return oWorkOrderRegister;
    }

    $('#btnPreview').click(function(e){
        if(!ValidateSearch()) return;
        var oWorkOrderRegister = RefreshObject();
        $.ajax({
            type: "POST",
            dataType: "json",
            url : _sBaseAddress+  "/WorkOrderRegister/SetSessionSearchCriteria",
            traditional: true,
            data:  JSON.stringify(oWorkOrderRegister),
            contentType: "application/json; charset=utf-8",
            success: function (data) {
                var sFeedBackMessage = jQuery.parseJSON(data);
                if (sFeedBackMessage==="Successful") {
                    var tsv=((new Date()).getTime())/1000;
                    window.open(_sBaseAddress+'/WorkOrderRegister/PrintWorkOrderRegister?ts='+tsv,"_blank");
                }
            },
            error: function (xhr, status, error) {
                alert(error);
            }
        });
    });


    $('#btnExportToExcel').click(function(e){
        if(!ValidateSearch()) return;
        var oWorkOrderRegister = RefreshObject();
        $.ajax({
            type: "POST",
            dataType: "json",
            url : _sBaseAddress+  "/WorkOrderRegister/SetSessionSearchCriteria",
            traditional: true,
            data:  JSON.stringify(oWorkOrderRegister),
            contentType: "application/json; charset=utf-8",
            success: function (data) {
                var sFeedBackMessage = jQuery.parseJSON(data);
                if (sFeedBackMessage==="Successful") {
                    var tsv=((new Date()).getTime())/1000;
                    window.open(_sBaseAddress+'/WorkOrderRegister/ExportToExcelWorkOrderRegister?ts='+tsv,"_blank");
                }
            },
            error: function (xhr, status, error) {
                alert(error);
            }
        });
    });


    $('#cboWorkOrderDate').change(function(e){
        //EqualTo = 1, NotEqualTo = 2, GreaterThen = 3, SmallerThen = 4, Between = 5, NotBetween = 6
        var nCompareOperator =   parseInt($('#cboWorkOrderDate').val());
        if(nCompareOperator===0)
        {
            $('#txtWorkOrderDateStart').datebox({ disabled : true });
            $('#txtWorkOrderDateEnd').datebox({ disabled : true });
        }
        else if (nCompareOperator===1 || nCompareOperator===2 || nCompareOperator===3 || nCompareOperator===4)
        {
            $('#txtWorkOrderDateStart').datebox({ disabled : false });
            $('#txtWorkOrderDateEnd').datebox({ disabled : true });
        }
        else
        {
            $('#txtWorkOrderDateStart').datebox({ disabled : false });
            $('#txtWorkOrderDateEnd').datebox({ disabled : false });
        }
        $('#txtWorkOrderDateStart').datebox('setValue', icsdateformat(new Date()));
        $('#txtWorkOrderDateEnd').datebox('setValue', icsdateformat(new Date()));
    });

    $('#cboExpectedDeliveryDate').change(function(e){
        //EqualTo = 1, NotEqualTo = 2, GreaterThen = 3, SmallerThen = 4, Between = 5, NotBetween = 6
        var nCompareOperator =   parseInt($('#cboExpectedDeliveryDate').val());
        if(nCompareOperator===0)
        {
            $('#txtExpectedDeliveryDateStart').datebox({ disabled : true });
            $('#txtExpectedDeliveryDateEnd').datebox({ disabled : true });
        }
        else if (nCompareOperator===1 || nCompareOperator===2 || nCompareOperator===3 || nCompareOperator===4)
        {
            $('#txtExpectedDeliveryDateStart').datebox({ disabled : false });
            $('#txtExpectedDeliveryDateEnd').datebox({ disabled : true });
        }
        else
        {
            $('#txtExpectedDeliveryDateStart').datebox({ disabled : false });
            $('#txtExpectedDeliveryDateEnd').datebox({ disabled : false });
        }
        $('#txtExpectedDeliveryDateStart').datebox('setValue', icsdateformat(new Date()));
        $('#txtExpectedDeliveryDateEnd').datebox('setValue', icsdateformat(new Date()));
    });

    $('#cboWOApprovedDate').change(function(e){
        //EqualTo = 1, NotEqualTo = 2, GreaterThen = 3, SmallerThen = 4, Between = 5, NotBetween = 6
        var nCompareOperator =   parseInt($('#cboWOApprovedDate').val());
        if(nCompareOperator===0)
        {
            $('#txtWOApprovedDateStart').datebox({ disabled : true });
            $('#txtWOApprovedDateEnd').datebox({ disabled : true });
        }
        else if (nCompareOperator===1 || nCompareOperator===2 || nCompareOperator===3 || nCompareOperator===4)
        {
            $('#txtWOApprovedDateStart').datebox({ disabled : false });
            $('#txtWOApprovedDateEnd').datebox({ disabled : true });
        }
        else
        {
            $('#txtWOApprovedDateStart').datebox({ disabled : false });
            $('#txtWOApprovedDateEnd').datebox({ disabled : false });
        }
        $('#txtWOApprovedDateStart').datebox('setValue', icsdateformat(new Date()));
        $('#txtWOApprovedDateEnd').datebox('setValue', icsdateformat(new Date()));
    });

    $('#cboWOAmount').change(function(e){
        //EqualTo = 1, NotEqualTo = 2, GreaterThen = 3, SmallerThen = 4, Between = 5, NotBetween = 6
        var nCompareOperator =   parseInt($('#cboWOAmount').val());
        if(nCompareOperator===0)
        {
            $('#txtWOAmountStart').prop("disabled", true);
            $('#txtWOAmountEnd').prop("disabled", true);
        }
        else if (nCompareOperator===1 || nCompareOperator===2 || nCompareOperator===3 || nCompareOperator===4)
        {
            $('#txtWOAmountStart').prop("disabled", false);
            $('#txtWOAmountEnd').prop("disabled", true);
        }
        else
        {
            $('#txtWOAmountStart').prop("disabled", false);
            $('#txtWOAmountEnd').prop("disabled", false);
        }
        $('#txtWOAmountStart').val('0.00');
        $('#txtWOAmountEnd').val('0.00');
    });

    $('#btnClose').click(function (e) {
        window.location.href = window.location.href;
    });
    
    ///Supplier WOck
    function PickSupplier()
    {        
        var nBUID = parseInt(sessionStorage.getItem("BUID"));
        var oContractor = { Params: 1 + '~' + $.trim($('#txtSupplier').val())+'~'+ nBUID };//here 1 is Supplier
        var obj = {
            BaseAddress: sessionStorage.getItem("BaseAddress"),
            Object: oContractor,
            ControllerName: "Contractor",
            ActionName: "ContractorSearchByNameType",
            IsWinClose: false
        };
        $("#progbar").progressbar({ value: 0 });
        $("#progbarParent").show();
        var intervalID = setInterval(updateProgress, 250);
        $.icsDataGets(obj, function (response) {
            clearInterval(intervalID);
            $("#progbarParent").hide();
            if (response.status && response.objs.length > 0) {
                if (response.objs[0].ContractorID > 0) {
                    var tblColums = []; var oColumn = { field: "Name", title: "Name", width: 200, align: "left" }; tblColums.push(oColumn);
                    oColumn = { field: "Address", title: "Address", width: 120, align: "left" }; tblColums.push(oColumn);
                    oColumn = { field: "Email", title: "Email", width: 90, align: "left" }; tblColums.push(oColumn);
                    oColumn = { field: "ShortName", title: "Short Name", width: 90, align: "left" }; tblColums.push(oColumn);
                    var oPickerParam = {
                        winid: 'winSuppliers',
                        winclass: 'clsSuppliers',
                        winwidth: 600,
                        winheight: 460,
                        tableid: 'tblSuppliers',
                        tablecolumns: tblColums,
                        datalist: response.objs,
                        multiplereturn: true,
                        searchingbyfieldName: 'Name',
                        windowTittle: 'Supplier List'
                    };
                    $.icsPicker(oPickerParam);
                    IntializePickerbutton(oPickerParam);
                }
                else { alert(response.objs[0].ErrorMessage); }
            }else{
                alert("Data Not Found.");
                return;
            }
        });
    }
    $("#txtSupplier").keydown(function (e) {
        var code = (e.keyCode ? e.keyCode : e.which);
        if (code == 13) // Enter Press
        {
            if($.trim($('#txtSupplier').val())===null || $.trim($('#txtSupplier').val())==="")
            {
                alert("Press enter with supplier name");    
                return;
            }
            PickSupplier();
        }
    });
    $("#btnPickSupplier").click(function () {
        $('#txtSupplier').val('');
        PickSupplier();
    });
    $('#txtSupplier').keydown(function (e) {
        var code = (e.keyCode ? e.keyCode : e.which);
        if (code == 8) //backspace=8
        {
            $("#txtSupplier").removeClass("fontColorOfPickItem");
            $('#txtSupplier').data('Suppliers', []);
        }
    });
    $('#btnClearSupplier').click(function(e){
        $("#txtSupplier").val("");
        $('#txtSupplier').data('Suppliers', []);
        $("#txtSupplier").removeClass("fontColorOfPickItem");
    });    
