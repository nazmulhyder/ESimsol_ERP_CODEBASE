    function IntializePickerbutton(oWOckerobj) {
        $("#" + oWOckerobj.winid).find("#btnOk").click(function () {
            //for Single Select
            SetWOckerValueAssign(oWOckerobj);
        });
        $(document).find('.' + oWOckerobj.winclass).keydown(function (e) {
            if (e.which == 13)//enter=13
            {
                SetWOckerValueAssign(oWOckerobj);
            }
        });
    }

    function SetWOckerValueAssign(oWOckerobj) {
        var oreturnObj = null, oreturnobjs = [];
        if (oWOckerobj.multiplereturn) {
            oreturnobjs = $('#' + oWOckerobj.tableid).datagrid('getChecked');
        } else {
            oreturnObj = $('#' + oWOckerobj.tableid).datagrid('getSelected');
        }
        $("#" + oWOckerobj.winid).icsWindow("close");
        $("#" + oWOckerobj.winid).remove();

        if (oWOckerobj.winid === 'winSuppliers')
        {
            if (oreturnobjs != null && oreturnobjs.length > 0) {
                $('#txtSupplier').val(oreturnobjs.length+"'s Suppliers seleted");
                $('#txtSupplier').addClass('fontColorOfPickItem');
                $('#txtSupplier').data('Suppliers', oreturnobjs);                
                $('#txtSupplier').focus();
            }
        }
        else if (oWOckerobj.winid === 'winProducts')
        {
            if (oreturnobjs != null && oreturnobjs.length > 0) {
                $('#txtProduct').val(oreturnobjs.length+"'s Products seleted");
                $('#txtProduct').addClass('fontColorOfPickItem');
                $('#txtProduct').data('Products', oreturnobjs);                
                $('#txtProduct').focus();
            }
        } 
     
    }

    function updateProgress() {
        var value =$('#progbar').progressbar('getValue');
        if (value < 96){
            value += Math.floor(Math.random() * 10);
            $('#progbar').progressbar('setValue', value);
        }
    }

    function hideShow(miliseconds) {
        $("#progbarParent").hide();
    }
</script>