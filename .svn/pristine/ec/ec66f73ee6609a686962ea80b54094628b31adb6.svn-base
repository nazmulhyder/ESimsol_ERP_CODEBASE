    function LoadSalaryHead(nId,sType)
    {
        debugger;
        var oESStructure = {ErrorMessage:nId+"~"+sType};
        var obj =
        {
            BaseAddress: _sBaseAddress,
            Object: oESStructure,
            ControllerName: "EmployeeSalaryStructure",
