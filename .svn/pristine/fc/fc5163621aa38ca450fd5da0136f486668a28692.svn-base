    .divNewSearch table {
        width:93%;
    }
    .divNewSearch table #btnReset, .divNewSearch table #btnGetList{
        float:right;
        color:#fff;
    }
    .divNewSearch table #btnReset :hover, .divNewSearch table #btnGetList:hover{
       color:#000;
    }

