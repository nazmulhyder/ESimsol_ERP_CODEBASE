    body {
        margin-left: 0px;
        height: 100%;
        width: 100%;
    }
    body label {
        font-family: Tahoma;
        font-size: 12px;
    }
    .actionfieldsetstyle {
        text-align: right;
    }

    .default-background-color{
        background-color:#CFB53B;
    }
    .winstyle {
        padding-top: 2px;
    }
    /* Write by Mahabub start point*/
    
     .winstyle fieldset {
        font-family: Tahoma;
        font-size: 12px;
    }
         
.wincontainerstyle {
        margin-left:0px;
        width: 100%;
    }
      
          .winViewFieldContainer {
        width: 100%;
    }   

         .wininputfieldstyle {
        width: 100%; 
        height: 30px; 
        line-height: 30px;
        clear: both;
    }
          .imageField {
        height: 80px;
    }
         .imageField img {
        border: 1px solid #CCC;
        height: 80px;
        width: 70%;
        margin-left: 5px;
    }
         .winlabelstyle {
        width: 27%; 
        float: left;
        text-align:right;
    }
 .wininputfieldstyle input {
       
        width:70%;
        float: left;
        margin: 4px auto;
        margin-left: 4px;
    }
    .wininputfieldstyle input[type="checkbox"] {
        
         width: 15px;
         height: 15px;
         margin-top: 8px;
         float: none;
    }
    .wininputfieldstyle select {
        width:71%;
        float: left;
        margin: 4px auto;
        margin-left: 4px;
    }
         .bodyfieldsetstyle {
        height: 70%;
    }
    .bodyfieldsetstyle .inputFieldWrapper {
        height: 400px;
        overflow-y: scroll;
    }
    #toolbarBank  input{
        width: 145px;
    }
         .hideItem {
        display: none;
    }
           .selectedItemFontColor {
        color: #3717C6;
    }

#progressbarParent {
        opacity: 0.8;
        background-color: #DCD9D4;
        position: fixed;
        width: 100%;
        height: 100%;
        top: 0px;
        left: 0px;
        z-index: 1000;
    }
        
/**upto last by Mahabub*/  
    .winstyle fieldset legend{
        font-weight: bold;
        font-size: 11px;
        font-family: Tahoma;
    }
    .number {
        text-align: right;
    }
    .menuMainCollectionTable {
        margin-left: 0px; 
        height: 100%; 
        width: 100%;
    }
    .tbl {
        width: 100%;
        font-size: 12px;
        font-family: tahoma;
    }
    .tdLabel {
        width: 30%;
        text-align: right;
    }
    .tdInputDateBox {
        width: 70%;
    }
    .tdInput {
        width: 70%;
    }
    .tdInput input{
        width: 98%;
    }
    .tdInput select{
        width: 100%;
    }
    .tdInput input[type="checkbox"]{
        width: 4%;
    }
    .oneTextTwoButton .tdInput input[type="text"]{
        width: 71%;
    }
    .oneTextTwoButton .tdInput input[type="button"]{
        width: 11%;
        margin-left: 1px;
    }
    .oneRow .tdLabel{
        width:15%;
    }
    .oneRow .tdInput{
        width:100%;
    }

    .oneRow input{
        width: 96%;
    }

    .fontColorOfPickItem {
        color: #264EE0;
        font-style: italic;
    }

    .CaptionStyle {
        font-size:11px; 
        font-weight:bold
    }

    .errorFieldBorder {
        border-color: #c00;
    }
    .PrintList{
        border-style:none;
        margin:6px 0px 0px 0px;
        cursor:pointer;
        white-space:pre,
    }
    .PrintList:hover {
        background: transparent url("icons/print.png") #EAF2FF none repeat 10px center;
        color: #000;
        border: 1px solid #B7D2FF;
        border-radius: 7px;
    }

    .icon-text{

    }

    /*===============Style for One Column Piker===============*/
    .OneColumnTableLabel {
        text-align:right;
        width:30%;
    }
    .OneColumnOneInputField {
        width:70%;
    }
    .OneColumnOneInputField input {
        width:100%;
    }
    .OneColumnOneInputField label {
        width:100%;
    }
    /*===========================End==========================*/
    
    .wrapperForNextPage {
        margin:0 auto;
        width:100%;
        height:100%;
        font-family:Tahoma;
    }
    .wrapperContentFieldSet {
        height:420px;
        overflow-x:hidden;
    }

    .PrintForm {
        float:left;
        width:90px;
        /*margin-top:4px;*/
        margin-right:4px;
    }

    fieldset legend {
        font-weight:bold;
        font-family:Tahoma;
    }

    .lblLoadingMessage {
        color: #c00;
        font-size: 17px;
    }

    .ComboFocusBorderClass {
        border:1px solid #87BAE7;
    }


