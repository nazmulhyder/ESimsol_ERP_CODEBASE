function onClickRow(index)
{  
  
    if (editIndex != index)
    {  
        if (endEditing())
        { 
            _oTrimming = $('#tblTrimmingList').datagrid('getSelected');
            _nRQty = _oTrimming.RcvQty;
            $('#tblTrimmingList').datagrid('selectRow', index).datagrid('beginEdit', index);  
            editIndex = index;
          
        } 
        else
        {  
            $('#tblTrimmingList').datagrid('selectRow', editIndex);  
        }  
    } 
}  

var editIndex = undefined;  

function endEditing()
{  
    if (editIndex == undefined){return true}  
    if ($('#tblTrimmingList').datagrid('validateRow', editIndex))
    { 
        $('#tblTrimmingList').datagrid('endEdit', editIndex);
        if(_bFlag == 0)
        {
            _oTrimming.RcvQty = _nRQty; 
            $('#tblTrimmingList').datagrid('updateRow', { index: editIndex, row: _oTrimming });
        }
        editIndex = undefined; 
        return true;  
    } 
    else
    {  
        return false;  
    } 
    
    
} 

function ValidateInput() 
{
    var oTrimming = $('#tblTrimmingList').datagrid('getSelected'); 
    var SelectedRowIndex = $('#tblTrimmingList').datagrid('getRowIndex', oTrimming);
    
     
    if (oTrimming == null || oTrimming.EPSID <= 0)
    {
        alert("Please select an item from the list!");
        _bFlag = 0;
        return false;
    }

    if(oTrimming.ApproveBy<=0)
    {
        alert("This Item Is Not Approved!");
        _oTrimming.RcvQty = _nRQty;
        $('#tblTrimmingList').datagrid('updateRow', { index: SelectedRowIndex, row: _oTrimming });
        _bFlag = 0;
        return false;
    }

    if(oTrimming.EmpWiseBalanceQty==0)
    {
       alert("Balance Qty Is Zero. You Have Nothing To Receive !");
       _oTrimming.RcvQty = _nRQty;
       $('#tblTrimmingList').datagrid('updateRow', { index: SelectedRowIndex, row: _oTrimming });
       _bFlag = 0;
       return false;
    }

    if(oTrimming.RcvQty=="" || oTrimming.RcvQty== 0)
    {
        alert("Please Enter Receive Qty!");
        $('#tblTrimmingList').datagrid('selectRow', SelectedRowIndex).datagrid('beginEdit', SelectedRowIndex); 
        editIndex = SelectedRowIndex;
        _bFlag = 0;
        return false;
    }
   
 
    if(oTrimming.RcvQty>oTrimming.EmpWiseBalanceQty)
    {
        alert("ReceiveQty Must Not be Greater Than Balance Qty !");
        $('#tblTrimmingList').datagrid('selectRow', SelectedRowIndex).datagrid('beginEdit', SelectedRowIndex); 
        editIndex = SelectedRowIndex;
        _bFlag = 0;
        return false;
    }



    return true;
}

function RefreshObject() 
{
    var oTrimming = $('#tblTrimmingList').datagrid('getSelected');
    var oEmployeeProductionReceiveDetail=
    {
        EPSRDID : 0,
        EPSID : oTrimming.EPSID,
        RcvQty : oTrimming.RcvQty,
        RcvByDate : new Date()
    }
    return oEmployeeProductionReceiveDetail;
}


$('#btnSave').click(function (e)
{
    _bFlag = 1;
    endEditing();
    var oTrimming = $('#tblTrimmingList').datagrid('getSelected'); 
    var SelectedRowIndex = $('#tblTrimmingList').datagrid('getRowIndex', oTrimming);
    
    if(!ValidateInput()) return;

    
    var nNewRcvQty=parseFloat(oTrimming.RcvQty)+parseFloat(_nRQty);

    var oEmployeeProductionReceiveDetail = RefreshObject();
    $.ajax({
            type: "POST",
            dataType: "json",
            url: _sBaseAddress + "/EmployeeProduction/EmployeeProductionReceiveDetail_IU",
            traditional: true,
            data: JSON.stringify({oEmployeeProductionReceiveDetail:oEmployeeProductionReceiveDetail,nNewRcvQty:nNewRcvQty}),
            contentType: "application/json; charset=utf-8",

            success: function(data)
            {
                //debugger
                var oEmployeeProduction = jQuery.parseJSON(data);
                if (oEmployeeProduction.ErrorMessage == "") 
                {
                    alert("Data Saved sucessfully");
                    $('#tblTrimmingList').datagrid('updateRow', { index: SelectedRowIndex, row: oEmployeeProduction });
                    _bFlag = 0;
                } 
                else 
                {
                    alert(oEmployeeProduction.ErrorMessage);
                }
             },
             error: function(xhr, status, error)
             {
                alert(error);
             }
          });

});



</script>