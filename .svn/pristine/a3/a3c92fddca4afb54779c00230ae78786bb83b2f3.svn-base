$('#btnApprove').click(function (e) 
{
    //debugger
    var oTrimming = $('#tblTrimmingList').datagrid('getSelected'); 
    var SelectedRowIndex=$('#tblTrimmingList').datagrid('getRowIndex',oTrimming);
    if (oTrimming == null || oTrimming.EPSID <= 0)
    {
        alert("Please select an item from the list!");
        return ;
    }
    if (oTrimming.ApproveBy > 0)
    {
        alert("Already Approved!");
        return ;
    }

    $.ajax({
            type: "POST",
            dataType: "json",
            url: _sBaseAddress + "/EmployeeProduction/EmployeeProduction_Approve",
            traditional: true,
            data: JSON.stringify({nId:oTrimming.EPSID}),
            contentType: "application/json; charset=utf-8",            
