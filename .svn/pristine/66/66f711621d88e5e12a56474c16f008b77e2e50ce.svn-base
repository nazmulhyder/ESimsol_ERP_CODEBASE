div.tablesorterPager {
	padding: 0 0 0 0;
	background-color: #F0F0F6;
	text-align: center;
}
div.tablesorterPager span {
	padding: 0 0 0 0;
}
div.tablesorterPager input.prev {
	width: auto;
	margin-top:0px;
	margin-right: 10px;
}
div.tablesorterPager input.next {
	width: auto;
	margin-top:0px;
	margin-left: 10px;
}
div.tablesorterPager input {
	font-size:12px;
	width: 50px;
	border: 1px solid #330000;
	text-align: center;
}


  