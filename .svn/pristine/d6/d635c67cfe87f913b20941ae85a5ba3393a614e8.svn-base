    $("#txtStatusAdvSearch").keydown(function (e) {
        var nkeyCode = e.keyCode || e.which;
        if(nkeyCode==13){
            GetStatus();
        }
        else if(nkeyCode==8){
            $("#txtStatusAdvSearch").val("");
            $("#winAdvSearch").data("StatusIDs","-1");
        }
