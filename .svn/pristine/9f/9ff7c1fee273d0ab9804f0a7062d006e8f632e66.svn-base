<script src="~/Views/VehicleChassis/VehicleChassiss.js"></script>
<script src="~/Views/VehicleEngine/VehicleEngine.js"></script>

<div class="modal-header">
    <h4>Vehicle Registration</h4>
</div>
<div class="modal-body ms-custom-control" id="divCC">
    <div class="row">
        <div class="col-md-12">
            <div class="col-md-2 align-right">
                <label class="control-label">File No:</label>
            </div>
            <div class="col-md-4">
                <input type="text" ng-model="VehicleRegistration.FileNo" class="form-control" disabled />
            </div>
            <div class="col-md-2 align-right">
                <label class="control-label">Reg. Type:</label>
            </div>
            <div class="col-md-4">
                <select class="form-control" ng-model="VehicleRegistration.VehicleRegistrationType" ng-options="oItem.id as oItem.Value for oItem in VehicleRegistrationTypes" ng-disabled="disabled">
                    <option value="">---Select Type---</option>
                </select>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-md-12">
            <div class="col-md-2 align-right">
                <label class="control-label">Registration No:</label>
            </div>
            <div class="col-md-4">
                <input type="text" ng-model="VehicleRegistration.VehicleRegNo" class="form-control" ng-disabled="disabled" />
            </div>
            <div class="col-md-2  align-right">
                <label class="control-label">Customer Name:</label>
            </div>
            <div class="col-md-4">
                <div class="input-group">
                    <input ng-model="VehicleRegistration.CustomerName" class="form-control input-group-sm" ng-disabled="disabled" ng-keydown="SearchKeyDownCustomer($event)" placeholder="Type Customer Name & Press Enter" required />
                    <span class="input-group-btn">
                        <button type="button" class="btn btn-sm" aria-label="left Align" ng-click="ClearCustomer()" ng-disabled="disabled" ng-disabled="disabled">
                            <span class="glyphicon glyphicon-trash" aria-hidden="true"></span>
                        </button>
                        <span style="margin-left:0px;"></span>
                        <button type="button" class="btn btn-sm" aria-label="left Align" ng-click="PickCustomer()" ng-disabled="disabled">
                            <span class="glyphicon glyphicon-search" aria-hidden="true"></span>
                        </button>
                    </span>
                </div>
            </div>
            
        </div>
    </div>
    <div class="row">
        <div class="col-md-12">
            <div class="col-md-2  align-right">
                <label class="control-label">Contact Person:</label>
            </div>
            <div class="col-md-4">
                <div class="input-group">
                    <select class="form-control" ng-model="VehicleRegistration.ContactPersonID" ng-options="oItem.ContactPersonnelID as oItem.Name for oItem in ContactPersonnels" ng-disabled="disabled">
                        <option value="">---Select Type---</option>
                    </select>
                    <span class="input-group-btn">
                        <button type="button" class="btn btn-sm" aria-label="left Align" ng-click="NewContactPerson()" ng-disabled="disabled">
                            <span class="glyphicon glyphicon-plus" aria-hidden="true"></span>
                        </button>
                        <button type="button" class="btn btn-sm" aria-label="left Align" ng-click="GetContactPersonel()" ng-disabled="disabled">
                            <span class="glyphicon glyphicon-refresh" aria-hidden="true"></span>
                        </button>
                        <span style="margin-left:2px;"></span>

                    </span>
                </div>
            </div>
            <div class="col-md-2 align-right">
                <label class="control-label"> Engine No:</label>
            </div>
            <div class="col-md-4">
                <div class="input-group">
                    <input ng-model="VehicleRegistration.EngineNo" class="form-control" ng-keydown="SearchKeyDownEngineNo($event)" placeholder="Type Engine No & Press Enter" ng-disabled="disabled" />
                    <span class="input-group-btn">
                        <button type="button" class="btn btn-sm" aria-label="Left Align" ng-click="PickNewEngine()" ng-disabled="disabled"> <span class="glyphicon glyphicon-plus" aria-hidden="true"></span></button>
                        <button type="button" class="btn btn-sm" aria-label="Left Align" ng-click="PickEngineNo()" ng-disabled="disabled"> <span class="glyphicon glyphicon-search" aria-hidden="true"></span></button>
                    </span>
                </div>
            </div>
            
        </div>
    </div>
    <div class="row">
        <div class="col-md-12">
            <div class="col-md-2 align-right">
                <label class="control-label">Chassis/VIN No:</label>
            </div>
            <div class="col-md-4">
                <div class="input-group">
                    <input ng-model="VehicleRegistration.ChassisNo" class="form-control" ng-keydown="SearchKeyDownChassisNo($event)" placeholder="Type Chassis No & Press Enter" ng-disabled="disabled" />
                    <span class="input-group-btn">
                        <button type="button" class="btn btn-sm" aria-label="Left Align" ng-click="PickNewChassis()" ng-disabled="disabled"> <span class="glyphicon glyphicon-plus" aria-hidden="true"></span></button>
                        <button type="button" class="btn btn-sm" aria-label="Left Align" ng-click="PickChassisNo()" ng-disabled="disabled"> <span class="glyphicon glyphicon-search" aria-hidden="true"></span></button>
                    </span>
                </div>
            </div>
            <div class="col-md-2 align-right">
                <label class="control-label"> Vehicle Type:</label>
            </div>
            <div class="col-md-4">
                <select class="form-control" ng-model="VehicleRegistration.VehicleTypeID" ng-options="oItem.VehicleTypeID as oItem.VehicleTypeName for oItem in VehicleTypes" ng-disabled="disabled">
                    <option value="">---Select Type---</option>
                </select>
            </div>
            
        </div>
    </div>
    <div class="row">
        <div class="col-md-12">
            <div class="col-md-2 align-right">
                <label class="control-label">Car Model:</label>
            </div>
            <div class="col-md-4">
                <input type="text" ng-model="VehicleRegistration.VehicleModelNo" class="form-control" ng-disabled="disabled" />
            </div>

            <div class="col-md-2 text-right">
                <label class="control-label">Exterior Color:</label>
            </div>
            <div class="col-md-4 text-left">
                <div class="input-group">
                    <input ng-model="VehicleRegistration.ColorName" class="form-control" ng-keydown="SearchKeyDownColor($event)" placeholder="Type Color Name & Press Enter" required />
                    <span class="input-group-btn">
                        <button type="button" class="btn btn-sm" aria-label="Left Align" ng-click="PickColorName()"> <span class="glyphicon glyphicon-search" aria-hidden="true"></span></button>
                    </span>
                </div>
            </div>
            
        </div>
    </div>
    <div class="row">
        <div class="col-md-12">
            <div class="col-md-2 align-right">
                <label class="control-label">Delivery Date:</label>
            </div>
            <div class="col-md-4">
                <input type="text" ng-model="VehicleRegistration.DeliveryDate" class="form-control" ng-disabled="disabled" />
            </div>
            <div class="col-md-2 align-right">
                <label class="control-label"> Remarks:</label>
            </div>
            <div class="col-md-4">
                <input type="text" ng-model="VehicleRegistration.Remarks" class="form-control" ng-disabled="disabled" />
            </div>
        </div>
    </div>
</div>
<div class="modal-footer">
    <button type="button" id="btnSave" class="btn btn-default btn-xs " aria-label="right Align" ng-click="Save()" ng-disabled="disabled"><span class="glyphicon glyphicon-floppy-disk" aria-hidden="true"> Save</span></button>
    <button type="button" id="btnClose" class="btn btn-default btn-xs " aria-label="right Align" ng-click="Close()"><span class="glyphicon glyphicon-remove" aria-hidden="true"> Close</span></button>
</div>

<style type="text/css">
    .regionVehicleRegistration .form-horizontal .control-label {
        padding-top: 1px;
    }

    .regionVehicleRegistration .form-control {
        height: 26px;
        font-size: 12px;
        width: 100%;
    }
</style>

