    }
    .regionVR .col-md-11 {
        width: 91%;
        padding-right: 2px;
        padding-left: 1px;
        margin-bottom: 3px;
    }
     .regionVR .col-md-10 {
        width: 84%;
        padding-right: 5px;
        padding-left: 5px;
        margin-bottom: 3px;
    }
    .regionVR .col-md-7 {
        width: 58.33%;
        padding-right: 5px;
        padding-left: 5px;
    }
    .regionVR .col-md-2 {
