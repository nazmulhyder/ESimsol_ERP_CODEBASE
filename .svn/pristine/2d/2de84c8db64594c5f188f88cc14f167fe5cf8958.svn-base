
 
@model ESimSol.BusinessObjects.ContractorPersonal
@{
    ViewBag.Title = "View Contractor Personal";
}
@*<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>*@



        <div class="PageHeading">
        <table border="0">
                <tr>
                    
                    <td style="width:900px" class="Headline"> Details of Contractor Personal</td>
                </tr>
            </table></div>    
        <div class="PageBody" style="overflow:auto">
        <table border="0">
            <tr>
                <td style="width:50px"></td>
                <td style="width:800px">                           
                            <fieldset>
                                <legend style="font-weight:bold"> Contractor Personal Info : </legend>
                                <table border="0">
                                     <tr>
                                        <td>Name</td>
                                        <td>:</td>
                                        <td>
                                                <div class="editor-field">
                                                @Html.TextBoxFor(model => model.Name, new { style = "width: 500px;", id = "txtName", disabled = "disabled" })            
                                                </div>
                                        </td>
                                    </tr>
                                    <tr>
                                       <td>Address</td>         
                                        <td>:</td>
                                        <td>
                                            <div class="editor-field">
                                                @Html.TextBoxFor(model => model.Address, new { style = "width: 500px;", id = "txtAddress", disabled = "disabled" })            
                                            </div>
                                        </td>
                                    </tr>
                                    <tr>
                                       <td>Phone Number</td>         
                                        <td>:</td>
                                        <td>
                                            <div class="editor-field">
                                                @Html.TextBoxFor(model => model.Phone, new { style = "width: 500px;", id = "txtPhone", disabled = "disabled" })            
                                            </div>
                                        </td>
                                    </tr>
                                    <tr>
                                       <td>Email Address</td>         
                                        <td>:</td>
                                        <td>
                                            <div class="editor-field">
                                                @Html.TextBoxFor(model => model.Email, new { style = "width: 500px;", id = "txtEmail", disabled = "disabled" })            
                                            </div>
                                        </td>
                                    </tr>
                                    <tr>
                                       <td>Note</td>         
                                        <td>:</td>
                                        <td>
                                            <div class="editor-field">
                                                @Html.TextBoxFor(model => model.Note, new { style = "width: 500px;", id = "txtNote", disabled = "disabled" })            
                                            </div>
                                        </td>
                                    </tr>
                                     
                                    @Html.HiddenFor(model => model.ContractorID) 
                                    @Html.HiddenFor(model => model.ContractorPersonalID) 
                                    <tr>
                                        <td></td>
                                        <td></td>
                                        <td>
                                            
                                        </td>
                                    </tr>
                                </table>
                                </fieldset>                              
                
                    </td>
                <td style="width:50px"></td>
            </tr>
            <tr>
                 <td style="width:50px"></td>
                 <td style="width:800px">
                    <div style="color: red;font-weight:900; margin-top:10px">@TempData["message"]</div>
                 </td>
                 <td style="width:50px"></td>
            </tr>
        </table>     
        </div>

        <div class="PageFooter">
<div style="margin-left:20px;margin-right:20px">
    <table>
        <tr>     
         <td style="width:500px; text-align:right"></td>                   
            <td style="width:400px; text-align:right">
               <div style="margin-left:20px">@Html.ActionLink("Back to Contractor Personal List", "Add", "ContractorPersonal", new { id = Model.ContractorPersonalID }, new { style = "width: 500px;" })</div>    
            </td>
        </tr>
    </table>
    
</div>
</div>  
 
