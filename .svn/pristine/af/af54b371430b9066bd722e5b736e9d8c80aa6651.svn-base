                                    LocationID            : _oMachineLocation.LocationID,
                                    sDateSelection        : $("#cboUpdateDate").val(),
                                    StartTime	          : sDateTime,
                                    EndTime               : eDateTime
                                };
       }

       else
       {

              oProductionSchedule = {   
                                      
                                    sDateSelection        : $("#cboUpdateDate").val(),
                                    StartTime	          : sDateTime,
                                    EndTime               : eDateTime
                                };
       }

       return oProductionSchedule;

 } 

 function Search()
 {

 debugger;



