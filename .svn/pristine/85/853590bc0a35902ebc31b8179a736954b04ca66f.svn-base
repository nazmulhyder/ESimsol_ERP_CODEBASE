                }
                _sCPersonnelIds=_sCPersonnelIds.substring(0,_sCPersonnelIds.length-1);
            }
        }
        $("#" + oPickerobj.winid).icsWindow("close");
        $("#" + oPickerobj.winid).remove();
    }
