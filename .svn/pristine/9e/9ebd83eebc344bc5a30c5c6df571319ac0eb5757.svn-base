@{
    ViewBag.Title = "Export Bill Particular List";
}
@model IEnumerable<ESimSol.BusinessObjects.ExportBillParticular>

<head>
 <title>ExportBillParticular</title>
 <script src="@Url.Content("~/Views/ExportBillParticular/ExportBillParticular.js")" type="text/javascript"></script>
 <script src="@Url.Content("~/Views/ExportBillParticular/ExportBillParticulars.js")" type="text/javascript"></script>
</head>
<body>                
<div id="winExportBillParticular" class="easyui-window winstyle" title="Bank Informations" data-options="modal:true,closed:true,collapsible:false,minimizable:false,maximizable:false,closable:false">
                                  <fieldset>
                                      <table class="tbl">
                                          <tr>
                                              <td class="tdLabel">
                                                  <label style="font-family:Tahoma">Name :</label>
                                              </td>
                                              <td class="tdInput">
                                                  <input id="txtName" type="text" disabled="disabled" />
                                              </td>
                                          </tr>
                                          <tr>
                                              <td class="tdLabel">
                                                  <label>Add/Deduct :</label>
                                              </td>
                                              <td class="tdInput">
                                                  <select id="cboInOutType" class="cboStyle"></select>
                                              </td>
                                          </tr>
                                       
                                      </table>
                                  </fieldset>
                                  <fieldset class="actionfieldsetstyle">
                                      <legend>Actions : </legend>
                                      <a id="btnSaveExportBillParticular" href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-save" plain="true">Save</a>
                                      <a id="btnCloseExportBillParticular" href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-cancel" plain="true">Close</a>
                                  </fieldset>
                              </div>
<div class="menuMainCollectionTable">
                            <table id="tblExportBillParticulars" title="ExportBillParticular List" class="easyui-datagrid" fit="true" fitcolumns="true" rownumbers="true" pagination="fasle" singleselect="true" autorowheight="false" toolbar="#toolbarExportBillParticular">
                                <thead>
                                    <tr>
                                        <th field="Name" width="100">Name</th>
                                        <th field="InOutTypeSt" width="100">Add/Deduct Type</th>
                                        <th field="ActivityInString" width="100">Activity</th>
                                    </tr>
                                </thead>
                            </table>
                                  <div id="toolbarExportBillParticular">
                                      <a id="btnAddExportBillParticular" href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-add" plain="true">Add</a>
                                      <a id="btnEditExportBillParticular" href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-edit" plain="true">Edit</a>
                                      <a id="btnViewExportBillParticular" href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-details" plain="true">View</a>
                                      <a id="btnDeleteExportBillParticular" href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-remove" plain="true">Delete</a>
                                      <a id="btnActiveInactive" href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-ok" plain="true">Active /Inactive</a>
                                  </div>
 </div>
</body>
                      
<style type="text/css">
        #winExportBillParticular {
                 width: 400px;
                          }
</style>

 <script type="text/javascript">
    var _sBaseAddress="";
    var _oExportBillParticular=null;
    var _oExportBillParticulars=[];
    var _oInOutTypes =[];

    $(document).ready(function () {
        _sBaseAddress =@Html.Raw( new System.Web.Script.Serialization.JavaScriptSerializer().Serialize(HttpContext.Current.Session[SessionInfo.BaseAddress]));
        _oExportBillParticulars =@Html.Raw( new System.Web.Script.Serialization.JavaScriptSerializer().Serialize(Model));
        _oInOutTypes=@Html.Raw(new System.Web.Script.Serialization.JavaScriptSerializer().Serialize(ViewBag.EnumInOutTypes));
      
        LoadExportBillParticularsEvents();
        LoadExportBillParticularEvents();
    });

     $(document).keydown(function (e) { if (e.keyCode == 27) { $('#winExportBillParticular').icsWindow('close'); } });

</script>
    
