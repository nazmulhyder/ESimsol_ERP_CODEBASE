});

    function LoadCateGory(oEmployeeCategorys)
    {
        $("#cboCategory").icsLoadCombo({
            List: oEmployeeCategorys,
            OptionValue: "Value",
            DisplayText: "Text"
        });
    }

    //-----search by code or name------

    $('#txtsearchByName').keypress(function (e) {
        
        debugger;
        var oEmps = $('#tblEmployees').datagrid('getRows');
        var sName=document.getElementById("txtsearchByName").value;
        var code = (e.keyCode ? e.keyCode : e.which);
        if (code == 13)//Enter key-13
        {
            debugger;
            //var c = String.fromCharCode(e.which);
            var txtSearchByName = document.getElementById('txtsearchByName').value.toUpperCase();
            if (txtSearchByName  == "")
            {
                alert("Please enter name or code!");
                return;
            }
            // txtSearchByName = txtSearchByName + c;

            var bFlag=false;
            var sTempName="";
            var sTempCode = "";
            var rows = $('#tblEmployees').datagrid('getRows');
            for(i=0;i<rows.length;++i)
            {
                if((rows[i]['Name']).toUpperCase().indexOf(txtSearchByName)>=0 || (rows[i]['Code']).toUpperCase().indexOf(txtSearchByName)>=0 )
                {
                    bFlag=true;
                    break;
                }
            }
            if(bFlag)
            {
                $('#tblEmployees').datagrid('selectRow', i);
            }
            else
            {
                GetByName(sName);
            }
            $('#txtsearchByName').val('');
        }
        
        $('#txtsearchByName').focus();
    });

    function GetByName(sName)
    {
        debugger;
        oEmp={Name:sName};
        $.ajax({
            type: "POST",
            dataType: "json",
            url: _sBaseAddress + "/RosterPlan/SearchByNameOrCode",
            traditional: true,
            data: JSON.stringify(oEmp),
            contentType: "application/json; charset=utf-8",

            success: function(data)
            {
                var oEmps=[];
                oEmps = jQuery.parseJSON(data);
                if (oEmps.length>0 && oEmps[0].ErrorMessage=="")
                {
                    for(var i = 0; i < oEmps.length; i++){
                        $("#tblEmployees").datagrid("appendRow", oEmps[i]);
                    }
                    //DynamicRefreshList(oTPIs, "tblTPIs");
                }
                else
                {
                    alert(oEmps[0].ErrorMessage);
                    oEmps=[];
                    //DynamicRefreshList(oEmps, "tblEmployees");
                    $('#tblEmployees').datagrid('getRows');
                }
            },
        });
    }

    $('#txtsearchByName').click(function (e)
    {
        document.getElementById("txtsearchByName").value="";
        $('#txtsearchByName').focus();
    });



function AdvancedSearch()
{
    var oParameter = new Object();
    oParameter.Name = "RosterPlanEmployee Advance Search";
    oParameter.MultipleItemReturn=true;
    var url = _sBaseAddress + "/Employee/EmployeeHRMPiker";
    var nLeft=(window.screen.width/2)-(1000/2);
    var nHeight=(window.screen.height/2)-(500/2);
    var oEmployees =window.showModalDialog(url, oParameter, 'dialogHeight:500px;dialogWidth:1000px;dialogLeft:'+nLeft+';dialogTop:'+nHeight+';center:yes;resizable:no;status:no;scroll:yes');
    DynamicRefreshList(oEmployees, "tblEmployees");
}

