<div class="modal-header">
    <h4 class="modal-title" id="modal-title" ng-bind="PickerTitle">Advance Search</h4>
</div>
<div class="modal-body regionSearch">
    <fieldset style="margin-bottom: 0px;">
        <legend>Searching criterion</legend>
        <div class="row col-md-12">
                <div class="col-md-3 text-right"><label class="control-label">PI No:</label></div>
                <div class="col-md-9 text-left">
                
                        <input type="text" class="form-control" ng-model="PINo" ng-keydown="SearchKeyDownPI($event)" placeholder="Type PI No ">

            </div>
        </div>
        <div class="row col-md-12">
            <div class="col-md-3 text-right"><label class="control-label">LC No:</label></div>
            <div class="col-md-9 text-left">

                <input type="text" class="form-control" ng-model="LCNo" ng-keydown="SearchKeyDownLCNo($event)" placeholder="Type LC No ">

            </div>
        </div>
       
        
        <div class="row col-md-12">
            <div class="col-md-3 text-right"><label class="control-label">PI Date:</label></div>
            <div class="col-md-9 text-left">
                <div class="col-md-4 text-left">
                    <select class="form-control" ng-model="cboPIDate" ng-change="PIDateChange()" ng-options="item.id as item.Value for item in CompareOperators"></select>
                </div>
                <div class="col-md-8 text-left">
                    <div class="col-md-12">
                        <div class="input-group date date-container" style="width:43%;float:left">
                            <input type="text" class="form-control" ng-model="PIDateStart"><span class="input-group-addon"><i class="glyphicon glyphicon-th"></i></span>
                        </div>
                        <div style="width:14%;float:inherit;">
                            <span class="input-group-addon" style="height:26px;"><span class="label label-primary">To</span></span>
                        </div>
                        <div class="input-group date date-container" style="width:43%;float:left">
                            <input type="text" class="form-control" ng-model="PIDateEnd" ng-disabled="PIDateEndDisabled"><span class="input-group-addon"><i class="glyphicon glyphicon-th"></i></span>
                        </div>
                    </div>
                </div>
            </div>
        </div>
       
        <div class="row col-md-12 ">
            <div class="col-md-3 text-right"><label class="control-label">LC Date:</label></div>
            <div class="col-md-9 text-left">
                <div class="col-md-4 text-left">
                    <select class="form-control" ng-model="cboLCOpenDate" ng-change="LCOpenDateChange()" ng-options="item.id as item.Value for item in CompareOperators"></select>
                </div>
                <div class="col-md-8 text-left">
                    <div class="col-md-12">
                        <div class="input-group date date-container" style="width:43%;float:left">
                            <input type="text" class="form-control" ng-model="LCOpenDateStart"><span class="input-group-addon"><i class="glyphicon glyphicon-th"></i></span>
                        </div>
                        <div style="width:14%;float:inherit;">
                            <span class="input-group-addon" style="height:26px;"><span class="label label-primary">To</span></span>
                        </div>
                        <div class="input-group date date-container" style="width:43%;float:left">
                            <input type="text" class="form-control" ng-model="LCOpenDateEnd" ng-disabled="LCOpenDateEndDisabled"><span class="input-group-addon"><i class="glyphicon glyphicon-th"></i></span>
                        </div>
                    </div>
                </div>

            </div>
        </div>
        <div class="row col-md-12 ">
            <div class="col-md-3 text-right"><label class="control-label">Amendment Date:</label></div>
            <div class="col-md-9 text-left">
                <div class="col-md-4 text-left">
                    <select class="form-control" ng-model="cboAmendmentDate" ng-change="AmmendentDateChange()" ng-options="item.id as item.Value for item in CompareOperators">
                      
                    </select>
                </div>
                <div class="col-md-8 text-left">
                    <div class="col-md-12">
                        <div class="input-group date date-container" style="width:43%;float:left">
                            <input type="text" class="form-control" ng-model="AmendmentDateStart"><span class="input-group-addon"><i class="glyphicon glyphicon-th"></i></span>
                        </div>
                        <div style="width:14%;float:inherit;">
                            <span class="input-group-addon" style="height:26px;"><span class="label label-primary">To</span></span>
                        </div>
                        <div class="input-group date date-container" style="width:43%;float:left">
                            <input type="text" class="form-control" ng-model="AmendmentDateEnd" ng-disabled="AmendmentDateEndDisabled"><span class="input-group-addon"><i class="glyphicon glyphicon-th"></i></span>
                        </div>
                    </div>
                </div>

            </div>
        </div>

        <div class="row col-md-12">
            <div class="col-md-3 text-right"><label class="control-label"> Party Name:</label></div>
            <div class="col-md-9 text-left">
                <div class="input-group">
                    <input type="text" class="form-control" ng-model="ContractorName" ng-keydown="SearchKeyDownContractorName($event)" placeholder="Type Party Name & Press Enter">
                    <span class="input-group-btn">
                        <button type="button" class="btn btn-sm" aria-label="Left Align" ng-click="PickContractor()"> <span class="glyphicon glyphicon-search" aria-hidden="true"></span></button>
                    </span>
                </div>
                
            </div>
        </div>
        <div class="row col-md-12">
            <div class="col-md-3 text-right"><label class="control-label"> Buyer Name:</label></div>
            <div class="col-md-9 text-left">
                <div class="input-group">
                    <input type="text" class="form-control" ng-model="BuyerName" ng-keydown="SearchKeyDownBuyerName($event)" placeholder="Type Buyer Name & Press Enter">
             
            <span class="input-group-btn">
                <button type="button" class="btn btn-sm" aria-label="Left Align" ng-click="PickBuyer()"> <span class="glyphicon glyphicon-search" aria-hidden="true"></span></button>
            </span>
             </div>
            </div>
        </div>

        

        <div class="row col-md-12">
            <div class="col-sm-3  text-right"><input type="checkbox" ng-model="chkYetToDistribute"  /></div>
            <div class="col-sm-6  text-left">
                <label class="control-label"> Waiting for Distribute</label>
            </div>
            
        </div>
        <div class="row col-md-12">
            <div class="col-sm-3 text-right"><input type="checkbox" ng-model="chkYetToPayable"  /></div>
            <div class="col-sm-9 text-left">
                <label class="control-label">Waiting for Payable</label>
            </div>

        </div>
        <div class="row col-md-12">
            <div class="col-sm-3 text-right"><input type="checkbox" ng-model="chkYetToPaid"  /></div>
            <div class="col-sm-9 text-left">
                <label class="control-label">Pending Payment</label>
            </div>

        </div>
        <div class="row col-md-12">
            <div class="col-sm-3 text-right"><input type="checkbox" ng-model="chkMaturityReceived"/></div>
            <div class="col-sm-9 text-left">
                
                    <label class="control-label">Maturity Received But Payable Not Create</label>
            </div>

        </div>
        <div class="row col-md-12">
            <div class="col-sm-3 text-right"><input type="checkbox" ng-model="chkPaymentReceived"/></div>
            <div class="col-sm-9 text-left">
                <label class="control-label">Payment Received But Payable Not Create</label>
            </div>

        </div>
      
        
        
        
        
        
        
    </fieldset>
</div>
<div class="modal-footer">
    <label ng-hide="lblLoadingMessage" style="float:left;color:green; ">Loading Please Wait...</label>
    <button class="btn btn-success" type="button" ng-click="reset()">Reset</button>
    <button class="btn btn-primary" type="button" ng-click="search()">Search</button>
    <button class="btn btn-warning" type="button" ng-click="closeAdvanceSearchModal()">Close</button>
</div>
