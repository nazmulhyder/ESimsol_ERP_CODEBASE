                                                     <input type="text" id="txtFileNo" style="width:100%;"/>
                                                         
                                                     </td>
                                                     <td style="width:32%; text-align:right">
                                                         <label id="lblType"></label>
                                                     </td>
                                                 </tr>
                                                 <tr>
                                                     <td style="width: 30%;text-align:right">
                                                         Request Date:
                                                     </td>
