                                                         <input id="dateOfAskingLCOpen" type="text" class="easyui-datebox" required="required"
                                                                data-options="formatter:icsdateformat,parser:icsdateparser" style="width: 70%;" />
                                                     </td>
                                                 </tr>
                                                 <tr>
                                                     <td style="width: 30%;text-align:right">
