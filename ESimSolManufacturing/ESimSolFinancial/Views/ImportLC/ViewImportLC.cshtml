            MasterLCNo: $.trim($("#txtMasterLCSearch").val()),
            ImportLCID:$('#divImportLC').data('ImportLC').ImportLCID,
            MasterLCObj:oMasterLC
        };

        $.ajax({
            type: "POST",
            dataType: "json",
            url: _sBaseAddress + "/ImportLC/SaveWithMasterLC",
            traditional: true,
            data: JSON.stringify(oImportMasterLC),
            contentType: "application/json; charset=utf-8",
            success: function (data) {
                var oImportMasterLC = jQuery.parseJSON(data);
                debugger;
                if (oImportMasterLC.ErrorMessage=="" || oImportMasterLC.ErrorMessage==null)
                {
                    var oImportMasterLCs = $("#tblMasterLCs").datagrid("getRows");
                    var nIndex = oImportMasterLCs.length;
                    $("#tblMasterLCs").datagrid("appendRow", oImportMasterLC);
                    $("#tblMasterLCs").datagrid("selectRow", nIndex);

                    $("#txtMasterLCDate1").datebox({ disabled: false });
                    $("#txtMasterLCDate1").datebox("setValue", icsdateformat(new Date()));
                    $("#txtMasterLCNo1").attr("placeholder", "Type LC No");
                    $("#txtMasterLCNo1").val("");
                }
                else {
                    alert(oImportMasterLC.ErrorMessage);
                }
            },
            error: function (xhr, status, error) {
                alert(error);
            }
        });

    });

