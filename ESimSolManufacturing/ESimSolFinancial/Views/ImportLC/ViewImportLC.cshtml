                                                         <select id="cboNegotiateBank" onchange="ChangeBankAccount()" style="width: 100%;"></select>
                                                     </td>
                                                 </tr>
                                               

                                                 @*<tr>
                                                     <td style="width: 30%;text-align:right">Supplier Name :</td>
                                                     <td style="width: 65%; padding-left: 3%;"><input type="text" id="txtContractorName_PI" style="width:100%;" /></td>
                                                 </tr>*@
                                                 <tr>
                                                     <td style="width: 30%;">
                                                         <div style="float: right;">Advice Bank :</div>
                                                     </td>
                                                     @*<td style="width: 65%; padding-left: 5%;"><label id="lblAdviceBank" /></td>*@
                                                     <td style="width: 65%; padding-left: 3%;" colspan="2"><input type="text" id="txtAdviceBank_PI" style="width:100%;" disabled="disabled" /></td>
                                                 </tr>
                                                 <tr>
