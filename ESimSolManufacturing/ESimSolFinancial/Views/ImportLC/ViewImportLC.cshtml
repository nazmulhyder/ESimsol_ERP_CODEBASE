    function PickMasterLC_ELC()
    {
        if($('#divImportLC').data('ImportLC').ImportLCID<=0)
        {
            alert("Please, First save L/C");
            return;
        }

        var oImportMasterLC = {
            ImportMasterLCID:0,
            MasterLCID:0,
            BUID:sessionStorage.getItem('BUID'),
            MasterLCNo: $.trim($("#txtMasterLCSearch").val()),
            ImportLCID:$('#divImportLC').data('ImportLC').ImportLCID
        };
        var obj = {
            BaseAddress: _sBaseAddress,
            Object: oImportMasterLC,
            ControllerName: "ImportLC",
            ActionName: "GetsImportMasterLCs",
            IsWinClose: false
        };

        $.icsDataGets(obj, function (response) {
            if (response.status && response.objs.length > 0) {
                _oMasterLC = [];
                if (response.objs[0].MasterLCID > 0) {
                    _oMasterLC = response.objs;
                }
                var tblColums = [];
                var oColumn = { field: "MasterLCNo", title: "L/C No", width: 120, align: "left" }; tblColums.push(oColumn);
                oColumn = { field: "MasterLCDateSt", title: "Date", width: 120, align: "center" }; tblColums.push(oColumn);
                var oPickerParam = {
                    winid: 'winMasterLC_ELC',
                    winclass: 'clsMasterLC_ELC',
                    winwidth: 380,
                    winheight: 460,
                    tableid: 'tblMasterLCPicker',
                    tablecolumns: tblColums,
                    datalist: _oMasterLC,
                    multiplereturn: false,
                    searchingbyfieldName: 'MasterLCNo',
                    windowTittle: 'Master LC Search'
                };
                $.icsPicker(oPickerParam);

                IntializePickerbutton(oPickerParam);
            }
        });
    }
    function SaveMasterLC(oImportMasterLC)
    {

        $.ajax({
            type: "POST",
            dataType: "json",
            url: _sBaseAddress + "/ImportLC/Save_MasterLC",
            traditional: true,
            data: JSON.stringify(oImportMasterLC),
            contentType: "application/json; charset=utf-8",
            success: function (data) {

                var oImportMasterLC = jQuery.parseJSON(data);
                if (parseInt(oImportMasterLC.ImportMasterLCID) > 0) {
                    var oImportMasterLCs = $("#tblMasterLCs").datagrid("getRows");
                    var nIndex = oImportMasterLCs.length;
                    $("#tblMasterLCs").datagrid("appendRow", oImportMasterLC);
                    $("#tblMasterLCs").datagrid("selectRow", nIndex);
                }
                else {
                    alert(oImportMasterLC.ErrorMessage);
                }
            },
            error: function (xhr, status, error) {
                alert(error);
            }
        });
    }

    function ValidationMasterLC1() {
        if ($('#divImportLC').data('ImportLC').ImportLCID <= 0 || $('#divImportLC').data('ImportLC').ImportLCID==null ) {
            alert("Please, First Save L/C, from previous tab ");

            return false;
        }
        if ($.trim($("#txtMasterLCNo1").val()) == "") {
            alert("Please Entry Master/Export LC No");
            $("#txtMasterLCNo1").focus();
            $("#txtMasterLCNo1").addClass("errorFieldBorder");
            return false;
        } else {
            $("#txtMasterLCNo1").removeClass("errorFieldBorder");
        }
        return true;
    }
    $("#btnAddMasterLC1").click(function () {

        debugger;
        if (!ValidationMasterLC1()) return false;

        var oMasterLC = {
            MasterLCID:0,
            MasterLCNo: $("#txtMasterLCNo1").val(),
            MasterLCDate: $('#txtMasterLCDate1').datebox('getValue'),
            MasterLCType:1,
        };

        var oImportMasterLC = {
