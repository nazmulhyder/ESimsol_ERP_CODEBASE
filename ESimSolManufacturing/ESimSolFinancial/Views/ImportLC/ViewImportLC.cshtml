<html>

@{
    ViewBag.Title = "Import LC";
}

<body>
    @model ESimSol.BusinessObjects.ImportLC
    <div class="menuMainCollectionTable" id="divImportLC" class="easyui-panel" title="Request For LC Open" style="font-family:Tahoma;width:98%">
