        //var dtDateFrom = new Date(GenerateStartTimeInString());
        //var dtDateTo = new Date(GenerateEndTimeInString());

        var dtDateFrom=$('#dtStartDate').datebox('getValue');
        var dtDateTo=$('#dtEndDate').datebox('getValue');
        var dtDateFrom = new Date(dtDateFrom);
        var dtDateTo = new Date(dtDateTo);

        if(dtDateFrom>dtDateTo)
        {
            alert("Invalid date range !");
            return;
        }
        //if(_sEmployeeIDs == "")
        //{
        //    alert("Please enter employee !");
        //    return;
        //}
        var EIDs="";
        var oEmpIDs=[];
        EIDs=_sEmployeeIDs+",";
        oEmpIDs = EIDs.split(',');

        //if(oEmpIDs.length>2)//because the list takes one more item !
        //{
        //    alert("Please enter only one employee at a time !");
        //    return;
        //}

        var nMOCID = $('#cboPrintType').val();
        if(nMOCID <= 0) {
            alert("Please select a time card.");
            return;
        }

        var oneDay = 24*60*60*1000; // hours*minutes*seconds*milliseconds
        var nDays = Math.round(Math.abs((dtDateFrom.getTime() - dtDateTo.getTime())/(oneDay)))
        if(nDays>31)
        {
            alert("Date range should not be more then 30!");
            return;
        }
        if(_sBusinessUnitIds.indexOf(',') != -1)
        {
            alert("Select one businessunit");
            return;
        }

        if (!confirm("Confirm to Process?")) return;
        var sTempString = _sEmployeeIDs+'~'+icsdateformat(dtDateFrom)+'~'+icsdateformat(dtDateTo) + '~' + nMOCID + '~' + _sLocationID + '~' + _sBusinessUnitIds;

        $("#progressbar").progressbar({ value: 0 });
        $("#progressbarParent").show();

        var intervalID = setInterval(updateProgress, 250);

        $.ajax({

            type: "POST",
            dataType: "json",
            url: _sBaseAddress + "/ProcessManagement/ProcessAttendanceComplianceAsPerEdit",
            traditional: true,
            data: JSON.stringify({sTemp : sTempString}),
