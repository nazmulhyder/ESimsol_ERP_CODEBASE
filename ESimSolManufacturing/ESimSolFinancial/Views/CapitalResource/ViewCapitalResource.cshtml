    $('#btnSave').click(function (e)
    {
        debugger;
        if (!Validation()) { return; }
        var oCapitalResource = RefreshObject();
        $.ajax({
            type: "POST",
            dataType: "json",
            url : _sBaseAddress+"/CapitalResource/Save",
            traditional: true,
            data:  JSON.stringify(oCapitalResource),
            contentType: "application/json; charset=utf-8",
            success: function (data) {
                //debugger;
                oCR = jQuery.parseJSON(data);
                if (oCR.ErrorMessage=="" || oCR.ErrorMessage==null)
                {
                    debugger;
                    alert("Data Saved sucessfully");
                    var oCapitalResources = sessionStorage.getItem("CapitalResources");
                    var nIndex = parseInt(sessionStorage.getItem("SelectedRowIndex"));
                    if (oCapitalResources != null)
                    {
                        oCapitalResources = jQuery.parseJSON(oCapitalResources);
                    }
                    else {
                        oCapitalResources = [];
                    }
                    if (nIndex != -1) {
                        oCapitalResources[nIndex] = oCR;
                    }
                    else {
                        sessionStorage.setItem("SelectedRowIndex", oCapitalResources.length);
                        oCapitalResources.push(oCR);
                    }
                    sessionStorage.setItem("CapitalResources", JSON.stringify(oCapitalResources));
                    window.location.href = sessionStorage.getItem("BackLink");
                    Close();
                }
                else
                {
                    alert(oCR.ErrorMessage);
                }
            },
            error: function (xhr, status, error) {
