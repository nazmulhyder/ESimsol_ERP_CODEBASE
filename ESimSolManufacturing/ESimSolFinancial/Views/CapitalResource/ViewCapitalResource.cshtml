    function Validation()
    {
        if ($.trim($('#txtName').val()) == '') { alert("Please enter name."); $('#txtName').focus(); return false; }
        else if ($.trim($('#txtCode').val()) == '') { alert("Please enter code."); $('#txtCode').focus(); return false; }
        else if ($('#txtWarrantyPeriod').numberbox('getValue')<=0){ alert("Please enter warranty period.");  return false; }
        else if (new Date($('#txtWarrantyStartDate').numberbox('getValue'))> new Date($('#txtWarrantyEndDate').numberbox('getValue'))){ alert("Start date must be less or equal to end date."); return false; }
        else if ($('#txtInvoiceValue').numberbox('getValue')<=0) { alert("Please enter invoice value.");  return false; }
        else if($("#cboResourceType :selected").text()== "Mold")
        {
            debugger;
            if ($.trim($('#txtLCNo').val()) == '') { alert("Please enter LC No."); $('#txtLCNo').focus(); return false; }
            if (parseFloat($('#txtCavity').val()) <= 0) { alert("Please enter Cavity"); $('#txtCavity').focus(); return false; }
            if ($.trim($('#cboFGWeightUnit').val()) == '') { alert("Please select Finish Good Unit."); $('#cboFGWeightUnit').focus(); return false; }
            if ($.trim($('#txtNaliWeight').val()) == '') { alert("Please enter LC No."); $('#txtNaliWeight').focus(); return false; }
            return true;
        }
        else { return true; }
    }
