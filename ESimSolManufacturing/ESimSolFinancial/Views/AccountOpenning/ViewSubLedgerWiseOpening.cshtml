        }
        else
        {
            for(var i=rowCount;i>=0;i--)
            {
                var indexExplanationID = parseInt($("#tblRefDetail").data('rows')[i].BObjID);
                if($("#tblRefDetail").data('rows')[i].DR_CR == "Subledger" && indexExplanationID !=0 && parseInt(searchItem) != parseInt(currentID))
