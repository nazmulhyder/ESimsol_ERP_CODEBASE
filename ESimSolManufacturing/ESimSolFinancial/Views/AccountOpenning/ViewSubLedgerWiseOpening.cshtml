        $('#tblRefDetail tr').eq(nRowIndex).find(sSelectedClass).focus();
        $('#tblRefDetail tr').eq(nRowIndex).find(sSelectedClass).select();
        InitializeKeyDownEvent(sSelectedClass);
        //GetTotalAmount(sAHOBTStr);
        RefreshSummery();
    }

