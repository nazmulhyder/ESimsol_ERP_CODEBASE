@{
    ViewBag.Title = "Holiday Calendar Define";
}
@model IEnumerable<ESimSol.BusinessObjects.HolidayCalendarDetail>

    <div id="winHolidayCalendarDetail" class="easyui-window" title="Holiday Calendar Define" style="width:800px;height:480px;padding:2px;" data-options="modal:true,closed:true,collapsible:false,minimizable:false,maximizable:false,closable:false">
        <div>
            <fieldset>
                <table border="0" cellspacing="2" cellpadding="2" style="font-size:11px; font-weight:bold">
                    <tr>
                        <td style="width:15%; text-align:right">
                            Calendar Name :
                        </td>
                        <td style="width:20%">
                            <input type="text" style="width: 100%;" id="txtCalendarName" disabled="disabled" />
                        </td>
                        <td style="width:10%; text-align:right">
                            Apply For :
                        </td>
                        <td style="width:20%">
                            <select id="cboCalendarApply" style="width:100%"></select>
                        </td>
                        <td style="width:10%; text-align:right">
                            Holiday :
                        </td>
                        <td style="width:40%">
                            <input type="text" style="width: 70%;" id="txtHolidayName" />
                            <input type="button" id="btnPickHoliday" style="width:13%;" value="P" />
                            <input type="button" id="btnClearHoliday" style="width:13%;" value="C" />
                        </td>
                                         
                    </tr>
                    <tr>
                        <td style="width:15%; text-align:right">
                            Start date :
                        </td>
                        <td style="width:20%">
                            <input type="text" style="width:100%;" id="txtStartDate" class="easyui-datebox" data-options="formatter:icsdateformat,parser:icsdateparser" />
                        </td>

                        <td style="width:10%; text-align:right">
                            End date :
                        </td>
                        <td style="width:20%">
                            <input type="text" style="width:100%;" id="txtEndDate" class="easyui-datebox" data-options="formatter:icsdateformat,parser:icsdateparser" />
                        </td>

                        <td style="width:10%; text-align:right">
                            Remarks :
                        </td>
                        <td style="width:30%" >
                            <input type="text" style="width: 100%;" id="txtRemarks" />
                        </td>
                    </tr>
                </table>
            </fieldset>
            <div class="easyui-tabs" style="width:100%; height:320px;">
                <div title="Assign DRP" style="padding:5px">
                    <table id="tblGeneralWorkingDayDetails" class="easyui-datagrid" style="width: 100%; height: 276px;" data-options="singleSelect: true, fitColumns:false,  rownumbers:true, pagination:false, autoRowHeight:false, toolbar: '#toolbardrp'">
                        <thead>
                            <tr>
                                <th field="BUName" width="250" align="left" style="font-size: 8px"> Business Unit </th>
                                <th field="Location" width="250" align="left" style="font-size: 8px"> Location </th>
                                <th field="Department" width="150" align="left" style="font-size: 8px"> Department </th>
                            </tr>
                        </thead>
                    </table>
                    <div id="toolbardrp">
                        <a id="btnAddDrp" href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-add" plain="true">Add</a>
                        <a id="btnRemoveDrp" href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-remove" plain="true">Remove</a>
                    </div>
                </div>
            </div>
        </div>
        <fieldset style="width:99.9%; vertical-align:top;">
            <table border="0" cellspacing="2" cellpadding="2" style="font-size:11px; font-weight:bold;">
                <tr>
                    <td style="width:400px;text-align:right"></td>
                    <td style="width:408px;text-align:right;">
                        <a id="btnSave" href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-save" plain="true">Save</a>
                        <a id="btnClosePicker" href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-cancel" plain="true">Cancel</a>
                    </td>
                </tr>
            </table>
        </fieldset>
    </div>
   
    <div class="menuMainCollectionTable" style="height:100%">
        <div style="height:500px;width:1015px">
            <table id="tblHolidayCalendarDetails" title="Holiday Calendar Define" class="easyui-datagrid" fit="true" fitcolumns="true" rownumbers="true" pagination="false" singleselect="true" autorowheight="false" toolbar="#toolbar">
                <thead>
                    <tr>
                        <th field="CalendarApplyInString" width="100">Calendar Apply</th>
                        <th field="HolidayName" width="100">Holiday Name</th>
                        <th field="StartDateInString" width="100" align="right">Start Date</th>
                        <th field="EndDateInString" width="100" align="center">End Date</th>
                        <th field="Remarks" width="100">Remarks</th>
                    </tr>
                </thead>
            </table>
            <div id="toolbar">
                <a id="btnAdd" href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-add" plain="true">Add</a>
                <a id="btnEdit" href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-edit" plain="true">Edit</a>
                <a id="btnView" href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-details" plain="true">View</a>
                <a id="btnDelete" href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-remove" plain="true">Delete</a>
                <a id="btnCopy" href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-add" plain="true">Copy</a>
            </div>
        </div>
        <fieldset style="width:1014px; vertical-align:top;">
            <table border="0" cellspacing="2" cellpadding="2" style="font-size:11px; font-weight:bold;">
                <tr>
                    <td style="width:800px;text-align:right"></td>
                    <td style="width:408px;text-align:right;">
                        <a id="btnClose" href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-cancel" plain="true">Close</a>
                    </td>
                </tr>
            </table>
        </fieldset>
    </div>
<script type="text/javascript">

    $(document).ready(function () {

        var oHolidayCalendarDetails =@Html.Raw( new System.Web.Script.Serialization.JavaScriptSerializer().Serialize(Model));
        var oApplyFor = @Html.Raw(new System.Web.Script.Serialization.JavaScriptSerializer().Serialize(@ViewBag.ApplyFor));
        $("#cboCalendarApply").icsLoadCombo({ List: oApplyFor, OptionValue: "id", DisplayText: "Value", InitialValue: "--Select--" });
        sessionStorage.setItem("selectedRowIndex", -1);
        RefreshList(oHolidayCalendarDetails);
    });

    function RefreshList(oHolidayCalendarDetails) {

        $("#cboCalendarApply").change(function () {
            var type = this.value;
            if (type == "1") {
                $("#btnAddDrp").hide();
                $("#btnRemoveDrp").hide();
            }
            else {
                $("#btnAddDrp").show();
                $("#btnRemoveDrp").show();
            }
        });

        DynamicRefreshList(oHolidayCalendarDetails, 'tblHolidayCalendarDetails');

        var nSelectedRowIndex = parseInt(sessionStorage.getItem("selectedRowIndex"));
        if (nSelectedRowIndex != -1) {
            $('#tblHolidayCalendarDetails').datagrid('selectRow', nSelectedRowIndex);
        }
    }

    function RefreshControl(oHolidayCalendarDetail) {

        var oHolidayCalendar = jQuery.parseJSON(sessionStorage.getItem("HolidayCalendar"));        

        $("#txtCalendarName").val(oHolidayCalendar.CalendarName);
        $('#cboCalendarApply').val(parseInt(oHolidayCalendarDetail.CalendarApply));
        $("#txtHolidayName").val(oHolidayCalendarDetail.HolidayName);
        $('#txtStartDate').datebox('setValue', oHolidayCalendarDetail.StartDateInString);
        $('#txtEndDate').datebox('setValue', oHolidayCalendarDetail.EndDateInString);
        $("#txtRemarks").val(oHolidayCalendarDetail.Remarks);
        
        DynamicRefreshList(oHolidayCalendarDetail.HolidayCalendarDRPs, 'tblGeneralWorkingDayDetails');
      
    }

    function ClearwinHolidayCalendar() {

        $("#cboCalendarApply").val("");
        $("#txtHolidayName").val("");
        $('#txtStartDate').datebox('setValue', icsdateformat(new Date()));
        $('#txtEndDate').datebox('setValue', icsdateformat(new Date()));
        $("#txtRemarks").val("");
        
        DynamicRefreshList([], 'tblGeneralWorkingDayDetails');
    }

    $('#btnAdd').click(function () {
        EnableAllfield();
        var oHolidayCalendarDetails = $('#tblHolidayCalendarDetails').datagrid('getRows');
        sessionStorage.setItem("HolidayCalendarDetails", JSON.stringify(oHolidayCalendarDetails));
        sessionStorage.setItem("selectedRowIndex", -1);
        $('#tblHolidayCalendarDetails').data('HolidayCalendarDetailID', 0);
        $("#winHolidayCalendarDetail").icsWindow('open', "HolidayCalendar Define");
        $('#winHolidayCalendarDetail').data('HolidayID', 0);
        var oHolidayCalendar = jQuery.parseJSON(sessionStorage.getItem("HolidayCalendar"));
        $("#txtCalendarName").val(oHolidayCalendar.CalendarName);

        ClearwinHolidayCalendar();
    });

    $('#btnEdit').click(function () {
        var oHolidayCalendarDetail = $('#tblHolidayCalendarDetails').datagrid('getSelected');
        if (oHolidayCalendarDetail == null || oHolidayCalendarDetail.HolidayCalendarID <= 0) {
            alert("Please select an item from list!");
            return;
        }
        $('#tblHolidayCalendarDetails').data('HolidayCalendarDetailID', oHolidayCalendarDetail.HolidayCalendarDetailID);
        $('#winHolidayCalendarDetail').data('HolidayID', oHolidayCalendarDetail.HolidayID);
        $('#tblHolidayCalendarDetails').data('HolidayName', oHolidayCalendarDetail.HolidayName);
        var nSelectedRowIndex = $('#tblHolidayCalendarDetails').datagrid('getRowIndex', oHolidayCalendarDetail);
        var oHolidayCalendarDetails = $('#tblHolidayCalendarDetails').datagrid('getRows');
        sessionStorage.setItem("HolidayCalendarDetails", JSON.stringify(oHolidayCalendarDetails));
        sessionStorage.setItem("selectedRowIndex", nSelectedRowIndex);
        $("#winHolidayCalendarDetail").icsWindow('open', "Edit Holiday Calendar Define");
        RefreshControl(oHolidayCalendarDetail);
        EnableAllfield();
    });

    $('#btnView').click(function () {

        var oHolidayCalendarDetail = $('#tblHolidayCalendarDetails').datagrid('getSelected');
        if (oHolidayCalendarDetail == null || oHolidayCalendarDetail.HolidayCalendarID <= 0) {
            alert("Please select an item from list!");
            return;
        }
        var nSelectedRowIndex = $('#tblHolidayCalendarDetails').datagrid('getRowIndex', oHolidayCalendarDetail);
        var oHolidayCalendarDetails = $('#tblHolidayCalendarDetails').datagrid('getRows');
        sessionStorage.setItem("HolidayCalendarDetails", JSON.stringify(oHolidayCalendarDetails));
        sessionStorage.setItem("selectedRowIndex", nSelectedRowIndex);
        $("#winHolidayCalendarDetail").icsWindow('open', "View Holiday Calendar Define");
        RefreshControl(oHolidayCalendarDetail);
        DisableAllfield();
    });

    $('#btnDelete').click(function () {

        var oHolidayCalendarDetail = $('#tblHolidayCalendarDetails').datagrid('getSelected');
        if (oHolidayCalendarDetail == null || oHolidayCalendarDetail.HolidayCalendarDetailID <= 0) {
            alert("Please select an item from list!");
            return;
        }
        if (!confirm("Confirm to Delete?")) return;

        var SelectedRowIndex = $('#tblHolidayCalendarDetails').datagrid('getRowIndex', oHolidayCalendarDetail);

        if (oHolidayCalendarDetail.HolidayCalendarDetailID > 0) {
            $.ajax
                ({
                    type: "POST",
                    dataType: "json",
                    url: sessionStorage.getItem('BaseAddress') + "/HolidayCalendar/HolidayDetailDelete",
                    data: JSON.stringify(oHolidayCalendarDetail),
                    contentType: "application/json; charset=utf-8",
                    success: function (data) {
                        feedbackmessage = jQuery.parseJSON(data);
                        if (feedbackmessage == "Deleted") {
                            alert("Delete sucessfully");
                            $('#tblHolidayCalendarDetails').datagrid('deleteRow', SelectedRowIndex);
                        }
                        else {
                            alert(feedbackmessage);
                        }
                    },
                    error: function (xhr, status, error) {
                        alert(error);
                    }
                });
        }
    });

    $('#btnCopy').click(function () {
        var oHolidayCalendarDetail = $('#tblHolidayCalendarDetails').datagrid('getSelected');
        if (oHolidayCalendarDetail == null || oHolidayCalendarDetail.HolidayCalendarID <= 0) {
            alert("Please select an item from list!");
            return;
        }
        $('#tblHolidayCalendarDetails').data('HolidayCalendarDetailID', 0);
        $('#winHolidayCalendarDetail').data('HolidayID', oHolidayCalendarDetail.HolidayID);
        $('#tblHolidayCalendarDetails').data('HolidayName', oHolidayCalendarDetail.HolidayName);
        
        var oHolidayCalendarDetails = $('#tblHolidayCalendarDetails').datagrid('getRows');
        sessionStorage.setItem("HolidayCalendarDetails", JSON.stringify(oHolidayCalendarDetails));
        sessionStorage.setItem("selectedRowIndex", -1);
        $("#winHolidayCalendarDetail").icsWindow('open', "Copy Holiday Calendar Define");
        RefreshControl(oHolidayCalendarDetail);
        EnableAllfield();
    });

    $('#btnSave').click(function () {
        if (!ValidateInput()) return;
        var oHolidayCalendar = RefreshObject();
        $.ajax({
            type: "POST",
            dataType: "json",
            url: sessionStorage.getItem('BaseAddress') + "/HolidayCalendar/HolidayDetailSave",
            traditional: true,
            data: JSON.stringify(oHolidayCalendar),
            contentType: "application/json; charset=utf-8",
            success: function (data) {
                oHolidayCalendar = jQuery.parseJSON(data);
                if (oHolidayCalendar.ErrorMessage == "" || oHolidayCalendar.ErrorMessage == null) {
                    alert("Data Saved sucessfully");
                    var oHolidayCalendars = sessionStorage.getItem("HolidayCalendarDetails");
                    var nIndex = parseInt(sessionStorage.getItem("selectedRowIndex"));
                    if (oHolidayCalendars != null) {
                        oHolidayCalendars = jQuery.parseJSON(oHolidayCalendars);
                    }
                    else {
                        oHolidayCalendars = [];
                    }
                    if (nIndex != -1) {
                        oHolidayCalendars[nIndex] = oHolidayCalendar;
                    }
                    else {
                        sessionStorage.setItem("selectedRowIndex", oHolidayCalendars.length);
                        oHolidayCalendars.push(oHolidayCalendar);
                    }
                    sessionStorage.setItem("HolidayCalendarDetails", JSON.stringify(oHolidayCalendars));
                    RefreshList(oHolidayCalendars);
                    $("#winHolidayCalendarDetail").icsWindow('close');
                }
                else {
                    alert(oHolidayCalendar.ErrorMessage);
                }
            },
            error: function (xhr, status, error) {
                alert(error);
            }
        });
    });

    function ValidateInput() {
        var nHolidayID = parseInt($('#winHolidayCalendarDetail').data('HolidayID'));
        if (nHolidayID <= 0)
        {
            alert("Please Pick Holiday First");
            return false;
        }
        var oGeneralWorkingDayDetails = $('#tblGeneralWorkingDayDetails').datagrid('getRows');
        var nApplyType = parseInt($("#cboCalendarApply").val());
        if (nApplyType == null || nApplyType == 0) {
            alert("Please Apply for Calendar!");
            $('#cboCalendarApply').focus();
            return false;
        }
        if (nApplyType == 1) {
            DynamicRefreshList([], 'tblGeneralWorkingDayDetails');
        }
        else if (nApplyType == 2 && oGeneralWorkingDayDetails.length <= 0) {
            alert("Press enter atleast one DRP!");
            return false;
        }
        return true;
    }

    function RefreshObject() {

        var oHolidayCalendar = jQuery.parseJSON(sessionStorage.getItem("HolidayCalendar"));
        
        var oHolidayCalendarDetail = {
            HolidayCalendarDetailID: parseInt($('#tblHolidayCalendarDetails').data('HolidayCalendarDetailID')),
            HolidayCalendarID:parseInt(oHolidayCalendar.HolidayCalendarID),
            HolidayID: $('#winHolidayCalendarDetail').data('HolidayID'),
            CalendarApply: parseInt($("#cboCalendarApply").val()),
            StartDate: $('#txtStartDate').datebox('getValue'),
            EndDate: $('#txtEndDate').datebox('getValue'),
            Remarks: $.trim($('#txtRemarks').val()),
            HolidayCalendarDRPs: $('#tblGeneralWorkingDayDetails').datagrid('getRows')
        }
        return oHolidayCalendarDetail;
    }

    function DisableAllfield() {

        $('#txtCalendarName').attr('disabled', 'disabled');
        $('#cboCalendarApply').attr('disabled', 'disabled');
        $('#txtHolidayName').attr('disabled', 'disabled');
        $('#btnPickHoliday').attr('disabled', 'disabled');
        $('#btnClearHoliday').attr('disabled', 'disabled');
        $('#txtStartDate').textbox('readonly', true);
        $('#txtEndDate').textbox('readonly', true);
        $('#txtRemarks').attr('disabled', 'disabled');
        $('#btnAddDrp').hide();
        $('#btnRemoveDrp').hide();
        $("#btnSave").hide();
    }

    function EnableAllfield() {
        var nApplyType = parseInt($("#cboCalendarApply").val());
        if (nApplyType == 1) {
            $('#btnAddDrp').hide();
            $('#btnRemoveDrp').hide();
        }
        $('#txtCalendarName').attr('disabled', true);
        $('#cboCalendarApply').attr('disabled', false);
        $('#txtHolidayName').attr('disabled', false);
        $('#btnPickHoliday').attr('disabled', false);
        $('#btnClearHoliday').attr('disabled', false);
        $('#txtStartDate').textbox('readonly', false);
        $('#txtEndDate').textbox('readonly', false);
        $('#txtRemarks').attr('disabled', false);
        $("#btnSave").show();
    }

    $('#btnPickHoliday').click(function (e) {
        var obj = {
            BaseAddress: sessionStorage.getItem('BaseAddress'),
            ControllerName: "HolidayCalendar",
            ActionName: "GetsHoliday",
            IsWinClose: false
        };
        var tblColums = [];
        var oColumn = [];
        oColumn = { field: 'Description', title: 'Description', width: '90%' }; tblColums.push(oColumn);

        DynamicPiker('HolidayPicker', obj, tblColums, false, 'Description', 'HolidayID', 350);
    });

    $("#btnAddDrp").click(function () {
        var nApplyType = parseInt($("#cboCalendarApply").val());
        if (nApplyType == null || nApplyType == 0) {
            alert("Please Apply for Calendar!");
            $('#cboCalendarApply').focus();
            return false;
        }

        var obj = {
            BaseAddress: sessionStorage.getItem('BaseAddress'),
            ControllerName: "GeneralWorkingDay",
            ActionName: "GetsGeneralWorkingDayDetail",
            IsWinClose: false
        };
        var tblColums = [];
        var oColumn = [];
        oColumn = { field: 'BUName', title: 'Business Unit', width: '40%' }; tblColums.push(oColumn);
        oColumn = { field: 'LocationName', title: 'Location', width: '30%' }; tblColums.push(oColumn);
        oColumn = { field: 'DepartmentName', title: 'Department', width: '20%' }; tblColums.push(oColumn);

        DynamicPiker('DRPPicker', obj, tblColums, true, 'BUName', 'DepartmentRequirementPolicyID', 650);
    });

    $("#btnRemoveDrp").click(function () {
        var oGeneralWorkingDayDetails = $('#tblGeneralWorkingDayDetails').datagrid('getChecked');
        if (oGeneralWorkingDayDetails.length <= 0) {
            alert("Please select atleast one General Working Day !");
            return;
        }
        var nSelectedIndex = 0;
        for (var i = 0; i < oGeneralWorkingDayDetails.length; i++) {
            nSelectedIndex = $('#tblGeneralWorkingDayDetails').datagrid('getRowIndex', oGeneralWorkingDayDetails[i]);
            $('#tblGeneralWorkingDayDetails').datagrid('deleteRow', nSelectedIndex);
        }
    });

    function DynamicPiker(pickerName, obj, pTblColums, pMultiReturn, pSearchField, pID, nWidth) {
        $.icsDataGets(obj, function (response) {
            if (response.status && response.objs.length > 0) {

                if (response.objs[0][pID] > 0) {

                    var tblColums = pTblColums;
                    var oPickerParam = {
                        winid: 'win' + pickerName,
                        winclass: 'cls' + pickerName,
                        winwidth: nWidth,
                        winheight: 460,
                        tableid: 'tbl' + pickerName + 's',
                        tablecolumns: tblColums,
                        datalist: response.objs,
                        multiplereturn: pMultiReturn,
                        searchingbyfieldName: pSearchField,
                        windowTittle: pickerName + ' List',
                        colsable: true
                    };
                    $.icsPicker(oPickerParam);
                    IntializePickerbutton(oPickerParam);
                }
                else {
                    alert(response.objs[0].ErrorMessage);
                }
            }
            else {
                alert("Data Not Found.");
                return;
            }
        });
    }

    function IntializePickerbutton(oPickerobj) {

        $("#" + oPickerobj.winid).find("#btnOk").click(function () {

            //for Single Select
            SetPickerValueAssign(oPickerobj);
        });

        $(document).find('.' + oPickerobj.winclass).keydown(function (e) {
            if (e.which == 13)//enter=13
            {

                SetPickerValueAssign(oPickerobj);
            }
        });
    }

    function SetPickerValueAssign(oPickerobj) {
        var oResult;
        if (oPickerobj.multiplereturn) {
            oResult = $('#' + oPickerobj.tableid).datagrid('getChecked');
        }
        else {
            oResult = $('#' + oPickerobj.tableid).datagrid('getSelected');
        }
        if (oPickerobj.winid == 'winDRPPicker') {
            SetGeneralWorkingDayDetail(oResult);
        }
        else if (oPickerobj.winid == 'winHolidayPicker') {
            SetHolidayDetail(oResult);
        }
        $("#" + oPickerobj.winid).icsWindow("close");
        $("#" + oPickerobj.winid).remove();
    }

    function SetGeneralWorkingDayDetail(oResult) {

        var oDrps = $('#tblGeneralWorkingDayDetails').datagrid('getRows');
        for (var i = 0; i < oResult.length; i++) {
            var IsExist = false;
            for (var j = 0; j < oDrps.length; j++) {
                if (parseInt(oResult[i].DepartmentRequirementPolicyID) == parseInt(oDrps[j].DRPID)) {
                    IsExist = true;
                }
            }
            if (!IsExist) {
                var oDrp =
                {
                    HolidayCalendarDRPID: 0,
                    HolidayCalendarDetailID: parseInt($('#tblHolidayCalendarDetails').data('HolidayCalendarDetailID')),
                    DRPID: parseInt(oResult[i].DepartmentRequirementPolicyID),
                    BUName: oResult[i].BUName,
                    Location: oResult[i].LocationName,
                    Department: oResult[i].DepartmentName
                }
                $('#tblGeneralWorkingDayDetails').datagrid('appendRow', oDrp);
            }
        }
    }

    function SetHolidayDetail(oResult) {

        $('#txtHolidayName').val(oResult.Description);
        $('#txtHolidayName').focus();

        $('#winHolidayCalendarDetail').data('HolidayID', oResult.HolidayID);
        var txtHolidayName = document.getElementById("txtHolidayName");
        txtHolidayName.style.color = "blue";
        txtHolidayName.style.fontWeight = "bold";
    }

    $("#txtHolidayName").keydown(function (e) {

        var code = (e.keyCode ? e.keyCode : e.which);
        if (code == 13) // Enter Press
        {
            if ($.trim($('#txtHolidayName').val()) === null || $.trim($('#txtHolidayName').val()) === "") {
                alert("Press enter with Holiday Name");
                return;
            }
            PickHoliday();
        }
        if (code == 8) //backspace=8
        {
            $("#txtHolidayName").removeClass("fontColorOfPickItem");
            $('#txtHolidayName').data('Holiday', null);
        }
    });

    $("#btnClearHoliday").click(function () {
        $('#txtHolidayName').val("");
        $("#txtHolidayName").removeClass("fontColorOfPickItem");
        $('#txtHolidayName').data('Holiday', null);
    });

    $('#btnClosePicker').click(function (e) {
        $("#winHolidayCalendarDetail").icsWindow('close');
    });

    $('#btnClose').click(function () {
        window.location.href = sessionStorage.getItem("BackLink");
    });

</script>
