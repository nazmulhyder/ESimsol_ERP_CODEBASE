            alert("Please select a item from list!");
            return;
        }
        //EnumAttchRefType : 1 Production Order;2 Delivery Order;3 Delivery Challan
        window.open(_sBaseAddress + '/AttachDocument/Attachment?id='+oDeliveryOrder.DeliveryOrderID+'&RefType=2&OperationInfo= Attachment of Delivery Order No : '+oDeliveryOrder.DONo, '_blank');
    }


    $('#btnPrintList').click(function(){
        var oDeliveryOrders=$('#tblDeliveryOrders').datagrid('getRows');
        if(oDeliveryOrders.length<=0)
        {
            alert("Sorry, there is no data to Print");
        }
        var ids = ICS_PropertyConcatation(oDeliveryOrders, 'DeliveryOrderID');
        var tsv=((new Date()).getTime())/1000;
        window.open(_sBaseAddress+ "/DeliveryOrder/DeliveryOrderPrintList?sIDs="+ids+"&ts="+tsv);
    });

    $('#btnPreview').click(function(){
        var oDeliveryOrder=$('#tblDeliveryOrders').datagrid('getSelected');
        if(oDeliveryOrder==null || parseInt(oDeliveryOrder.DeliveryOrderID)<=0)
        {
            alert("Please select Delivery Order ");
            return;
        }
        window.open(_sBaseAddress+ "/DeliveryOrder/DeliveryOrderPrintPreview?id="+oDeliveryOrder.DeliveryOrderID);
    });

