    function LoadAccountTypes(Items)
    {
        var listItems="";
        for (i = 0; i < Items.length; i++)
        {
            if(Items[i].id!=0)
            {
                listItems += "<option value='" + Items[i].id+"'>" + Items[i].Value+"</option>";
            }
        }
        $("#cboAccountType").html(listItems);
        $("#cboAccountType").val(5);
    }

