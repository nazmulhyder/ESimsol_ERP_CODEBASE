        var nAccountHead = oChartsOfAccount.AccountHeadID;

        var  sTempString = dDateType + '~' + dStartDate+ '~' + dEndDate+ '~' + parseInt(nAccountType)+'~' + parseInt(nAccountHead);

        var oParameter = new Object();
