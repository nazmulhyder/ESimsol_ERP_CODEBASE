    function FormatStyle(value,row,index){        
        var sAccountHeadName = value;
        var s = "";
        if(nVoucherID > 0)
        {
            s = '<label id="lblAccountHead~'+sAccountHeadName+'" style="color:Blue;cursor:pointer" onclick="Detail('+index+')">'+sAccountHeadName+'</label>';
