                                        <table id="tblBuyingHouse" border="0" cellpadding="0" cellspacing="0" style="width:100%" >
                                            <tr>
                                                <td style="width:40%"><input id="txtBuyingHouse" class="reset-text" style="width:100%" placeholder="Type & Press Enter" disabled /></td>
