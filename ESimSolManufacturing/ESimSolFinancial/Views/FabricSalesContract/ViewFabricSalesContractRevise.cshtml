<html>
<head>
    @{
        ViewBag.Title = "Production Revise";
    }
</head>
<body>
    @model ESimSol.BusinessObjects.FabricSalesContract
    <div id="progressbarParent" style="width:100%;height:100%;margin:0 auto;text-align:center; margin-left: auto;margin-right: auto;left: 0;right: 0;z-index: 1;">
        <div style="margin: 0px auto;margin-top: 251px;text-align:center;width:390px;">
            <label style="font-size:18px;">Please wait</label>
            <div id="progressbar" style="width:100%;height:37px;"></div>
        </div>
    </div>

    <div id="divFSC" class="menuMainCollectionTable">
        <div class="easyui-panel" title="" style="font-family:Tahoma; text-align:center; width:100%; height:100%;">
            <div id="tabFSC" class="easyui-tabs" style="width:100%;">
                <div title="Fabric Info" style="width:100%; height:30%" >
                    <div id="divFSCInfo">
                        <fieldset>
                            <legend style="font-weight:bold"> PO Information : </legend>
                            <table style="width:100%;" border="0" cellspacing="1" cellpadding="1">
                                <tr>
                                    <td style="width:10%; text-align:right;font-weight:bold">
                                        Order Type:
                                    </td>
                                    <td style="width:15%">
                                        <select id="cboOrderType" onchange="ChangeOrderType(true)" style=" width:95%;" disabled></select>
                                    </td>
                                    <td style="width:35%;text-align:left">
                                        <table border="0" cellpadding="0" cellspacing="0" style="width:100%;text-align:left">
                                            <tr>
                                                <td style="width:22%"><input type="checkbox" id="chkIsInHouseYes"  disabled />InHouse </td>
                                                <td style="width:28%;text-align:left"><input type="checkbox" id="chkIsInHouseNo"  disabled />SubContract</td>
                                                <td style="width:25%;text-align:left">Is Print?<input type="checkbox" id="chkIsPrint" disabled /></td>
                                                <td style="width:25%;text-align:left">Is Open PI?<input type="checkbox" id="chkIsOpenPI" disabled /></td>
                                            </tr>
                                        </table>

                                    </td>
                                    <td style="width:10%; text-align:right;font-weight:bold">
                                        PO No:
                                    </td>
                                    <td style="width:15%">
                                        @*<input type="text" style="width:20%;" id="txtSCNoOrderType" />*@
                                        <select id="txtSCNoOrderType" style=" width:30%;" disabled></select>
                                        <input type="text" style="width:65%;" id="txtSCNo" placeholder="Auto Generated" disabled />
                                    </td>
                                    <td style="width:5%; text-align:right;font-weight:bold">
                                        Date:
                                    </td>
                                    <td style="width:10%">
                                        <input type="text" style="width:100%;" id="txtSCDate" class="easyui-datebox" required="required" data-options="formatter:icsdateformat,parser:icsdateparser" disabled />
                                    </td>
                                </tr>
                                <tr>
                                    <td style="width:10%; text-align:right;font-weight:bold">
                                        <span class="lblOrderType"></span>
                                    </td>
                                    <td colspan="2" style="width:50%;text-align:left">
                                        <table border="0" cellpadding="0" cellspacing="0" style="width:100%">
                                            <tr>
                                                <td colspan="3" style="width:56%"><input type="text" id="txtContractorName" class="reset-text" style="width:100%" placeholder="Type & Press Enter" disabled /> </td>
                                                <td style="width:14%;text-align:right;">C.Person: </td>
                                                <td colspan="2" style="width:38%"><select id="cboContractorContactPerson" style=" width:100%;" disabled></select></td>
                                            </tr>
                                        </table>
                                    </td>
                                    <td style="width:10%; text-align:right;font-weight:bold">
                                        PI No:
                                    </td>
                                    <td colspan="3" style="width:30%">
                                        <input type="text" style="width:100%;" id="txtPINo" disabled />
                                    </td>
                                  

                                </tr>
                                <tr id="rtHandLoom">

                                    <td style="width:10%; text-align:right;font-weight:bold">
                                        Buying House :
                                    </td>
