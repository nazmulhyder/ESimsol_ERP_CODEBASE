        sessionStorage.setItem("BackLink", window.location.href);
        var tsv=((new Date()).getTime())/1000;
        window.location.href = _sBaseAddress+ "/FabricDeliveryOrder/View_FabricDeliveryOrder?nID=0&buid="+_nBUID+"&ts="+tsv;
    });



