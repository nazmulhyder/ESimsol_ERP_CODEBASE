    {
        var oFabricDeliveryOrder = $('#tblFabricDeliveryOrder').datagrid('getSelected');
        if (oFabricDeliveryOrder ==null || oFabricDeliveryOrder.FDOID <=0 ) { alert("Please select an item from list."); return ; }

        window.open(_sBaseAddress + '/AttachDocument/Attachment?id='+oFabricDeliveryOrder.FDOID+'&RefType=26&OperationInfo= Attachment of Order No : '+oFabricDeliveryOrder.DONoFull, '_blank');
    });

