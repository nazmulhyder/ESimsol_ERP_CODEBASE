        var obj =
            {
                BaseAddress: _sBaseAddress,
                Object: oFabricDeliveryOrder,
                ObjectId: oFabricDeliveryOrder.FDOID,
                ControllerName: "FabricDeliveryOrder",
                ActionName: "UpdateDONo",
                TableId: "tblFabricDeliveryOrder",
                IsWinClose: true
            };
