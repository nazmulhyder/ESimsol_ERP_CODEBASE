    var _oAuthorizationRolesMapping=[];
    $(document).ready(function () {
        _sBaseAddress = @Html.Raw(new System.Web.Script.Serialization.JavaScriptSerializer().Serialize(HttpContext.Current.Session[SessionInfo.BaseAddress]));
        _oFabricDeliveryOrders =@Html.Raw( new System.Web.Script.Serialization.JavaScriptSerializer().Serialize(Model));
        _oAuthorizationRolesMapping = @Html.Raw(new System.Web.Script.Serialization.JavaScriptSerializer().Serialize(HttpContext.Current.Session[SessionInfo.AuthorizationRolesMapping]));
        _sMenuID =@Html.Raw( new System.Web.Script.Serialization.JavaScriptSerializer().Serialize(HttpContext.Current.Session[SessionInfo.MenuID]));
        _nBUID = @Html.Raw(new System.Web.Script.Serialization.JavaScriptSerializer().Serialize(ViewBag.BUID));
        _oCompareOperators = @Html.Raw(new System.Web.Script.Serialization.JavaScriptSerializer().Serialize(ViewBag.CompareOperators));
        var oFDOTypes =@Html.Raw(new System.Web.Script.Serialization.JavaScriptSerializer().Serialize(ViewBag.FDOTypes));



        sessionStorage.setItem("BUID", _nBUID);
        $("#cboFDOTypes").icsLoadCombo({
            List: oFDOTypes,
            OptionValue: "FDOTypeInt",
            DisplayText: "FDOName",

        });
        if(sessionStorage.getItem("FabricDeliveryOrders")!=null && sessionStorage.getItem("FabricDeliveryOrders").length>0){
            _oFabricDeliveryOrders= jQuery.parseJSON(sessionStorage.getItem('FabricDeliveryOrders'));
            var nIndex= sessionStorage.getItem('SelectedRowIndex');
            DynamicRefreshList(_oFabricDeliveryOrders, 'tblFabricDeliveryOrder');

            if(nIndex>-1)
            {
                $('#tblFabricDeliveryOrder').datagrid('selectRow',nIndex);
            }
        }
        else
        {
            DynamicRefreshList(_oFabricDeliveryOrders, 'tblFabricDeliveryOrder');
        }
        RefreshControlLayout();
        $("#progressbar").progressbar({ value: 0 });
        $("#progressbarParent").hide();
        $(".lblLoadingMessage").hide();
        LoadComboAdv();

        $('#lblMunit').html("Y");
        $('#lblMunitOne').html("In Yard");
        $('#lblMunitTwo').html("In Meter");

        sessionStorage.setItem("QtyFormat",false);//default set
        sessionStorage.setItem("ImageFormat",2);//default set
    });

    $(document).keydown(function (e) { if (e.keyCode == 27) { $('#winFabricDeliveryOrderOrder').icsWindow('close'); } });
    $('#tblFabricDeliveryOrder').datagrid({ onSelect: function (rowIndex, rowData) { OperationPerforms(rowIndex, rowData); } });

    function OperationPerforms(rowIndex, rowData)
    {
        //if (rowData != null && rowData.FDOID > 0)
        //{
        //    if (rowData.DOStatus==0 )  //Initialized = 1,
        //    {
        //        $('#btnEdit,#btnDelete,#btnUpdateDONo,#btnApprove').show();
        //        $('#btnCancel,#btnRevise').hide();
        //    }
        //    else if (rowData.DOStatus==9 )  //CANCEL = 9,
        //    {
        //        $('#btnEdit,#btnDelete,#btnUpdateDONo,#btnCancel,#btnRevise,#btnApprove,#btnAttachment').hide();
        //    }
        //    else if (rowData.DOStatus>0 )  //Initialized = 1,
        //    {
        //        $('#btnEdit,#btnDelete,#btnUpdateDONo').hide();
        //        $('#btnCancel,#btnRevise,#btnApprove').show();
        //    }
        //}

        if (rowData.ApproveBy!=0  && rowData.CheckedBy!=0)  //Initialized = 1,
        {
            $('#btnEdit,#btnDelete,#btnChecked,#btnApprove,#btnRevise').hide();
            if (PermissionChecker('Revise', 'FabricDeliveryOrder',_oAuthorizationRolesMapping)) {$("#btnRevise").show();}
        }
        else if (rowData.ApproveBy==0 && rowData.CheckedBy==0 )
        {
            $('#btnEdit,#btnDelete,#btnApprove,#btnRevise,#btnChecked').hide();
            //$('#btnApprove').show();
            if (PermissionChecker('Verify', 'FabricDeliveryOrder',_oAuthorizationRolesMapping)) {$("#btnChecked").show();}
            if (PermissionChecker('Approved', 'FabricDeliveryOrder',_oAuthorizationRolesMapping)) {$("#btnApprove").show();}
        }
        else if (rowData.ApproveBy==0 && rowData.CheckedBy!=0 )
        {
            $('#btnEdit,#btnDelete,#btnApprove,#btnRevise,#btnCancel').hide();
            //$('#btnApprove').show();
            if (PermissionChecker('Approved', 'FabricDeliveryOrder',_oAuthorizationRolesMapping)) {$("#btnApprove").show();}
            if (PermissionChecker('Cancel', 'FabricDeliveryOrder',_oAuthorizationRolesMapping)) {$("#btnCancel").show();}
            if (PermissionChecker('Revise', 'FabricDeliveryOrder',_oAuthorizationRolesMapping)) {$("#btnRevise").show();}
        }
        else
        {
            $('#btnApprove,#btnRevise,#btnChecked,#btnEdit,#btnDelete').hide();
            if (PermissionChecker('Edit', 'FabricDeliveryOrder',_oAuthorizationRolesMapping)) {$("#btnEdit").show();}
            if (PermissionChecker('Delete', 'FabricDeliveryOrder',_oAuthorizationRolesMapping)) {$("#btnDelete").show();}
        }
            
    }




