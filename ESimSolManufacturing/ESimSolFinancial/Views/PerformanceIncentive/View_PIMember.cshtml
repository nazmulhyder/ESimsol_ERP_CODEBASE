    function Append(oEmployee)
    {
        debugger
        var oEmployees=[];
        oEmployees.push(oEmployee);
        var bAppend=false;
        var oTEmps=$('#tblPIMemberRate').datagrid('getRows');
        
        if(oTEmps.length>0)
        {
            for (var i = 0; i < oEmployees.length; i++)
            {
                var IsAppend=true;
                for (var j = 0; j < oTEmps.length; j++)
                {
                    if(oEmployees[i].EmployeeID==oTEmps[j].EmployeeID)
                    {
                        IsAppend=false;
                        break;
                    }
                }
                if(IsAppend)
                {
                    bAppend=true;
                    $('#tblPIMemberRate').datagrid('appendRow', oEmployees[i]);
                }
            }
        }
        else
        {
            DynamicRefreshList(oEmployees, "tblPIMemberRate");
        }
    }

    var editIndex = undefined;  
    function endEditing()
    {  
        if (editIndex == undefined){return true}  
        if ($('#tblPIMemberRate').datagrid('validateRow', editIndex))
        { 
            $('#tblPIMemberRate').datagrid('endEdit', editIndex);
            editIndex = undefined; 
            return true;  
        } 
        else
        {  
            return false;  
        } 
    } 

    function onClickRow(index)
    {  
        if (editIndex != index)
        {  
            if (endEditing())
            { 
                $('#tblPIMemberRate').datagrid('selectRow', index).datagrid('beginEdit', index);  
                editIndex = index; 
            } 
            else
            {  
                $('#tblPIMemberRate').datagrid('selectRow', editIndex);  
            }  
        } 
    }  

    function Remove() 
    {
        var oPIM = $('#tblPIMemberRate').datagrid('getSelected');
        
