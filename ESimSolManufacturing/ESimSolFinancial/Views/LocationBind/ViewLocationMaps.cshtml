    function RefreshLocationMap()
    {
        var olocation= $('#txtIpAddress').data('Location');
        var oUser = $('#MainDiv').data('User');
        var oLB_UserLocationMap={
            LB_UserLocationMapID:0,
            LB_UserID:parseInt(oUser.UserID),
