    $("#btnAssignMultiLocation").click(function () {
       
        debugger;
        if (!ValidateLocationAssign()) return;
        var oLocationMap=RefreshLocationMap();
        $.ajax({
            type: "POST",
            dataType: "json",
            url : _sBaseAddress+  "/LocationBind/SaveUserLocationMap1",
            traditional: true,
