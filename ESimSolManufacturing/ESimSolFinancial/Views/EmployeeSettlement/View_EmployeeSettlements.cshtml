    $('#btnDiscontinued').click(function (e) {
        debugger
        var oEmployee = $('#tblEmployeeSettlements').datagrid('getSelected');
        var nIndexes = [];
        var sEmpIDs = "";
        //if (oEmployees.length <= 0) {
        //    alert("please select at least one Item!");
        //    return false;
        //}
        //for (var i = 0; i < oEmployees.length; i++) {
        //    if (oEmployees[i].WorkingStatus == 6) {
        //        alert("Some of employees are already discontinued!");
        //        return false;
        //    }
        //    var SelectedRowIndex = $('#tblEmployeeSettlements').datagrid('getRowIndex', oEmployees[i]);
        //    nIndexes.push(SelectedRowIndex);
        //    sEmpIDs += oEmployees[i].EmployeeID + ",";
        //}
        //sEmpIDs = sEmpIDs.substring(0, sEmpIDs.length - 1);

        if (oEmployee.WorkingStatus == 6) {
            alert("Already discontinued!");
            return;
        }
        if (oEmployee.ApproveBy<=0) {
            alert("This item is not approved!");
            return;
        }
        //alert(icsdateformat(new Date(oEmployee.EffectDateInString)));
        //alert(icsdateformat(new Date()))
        if (new Date(oEmployee.EffectDateInString)>  new Date()){
            alert("Please discontinue in the last working day!");
            return;
        }
        var SelectedRowIndex = $('#tblEmployeeSettlements').datagrid('getRowIndex', oEmployee);
        nIndexes.push(SelectedRowIndex);
        sEmpIDs=oEmployee.EmployeeID;
        if (!confirm("Confirm to Discontinue? !!")) return;
        var tsv = ((new Date()).getTime()) / 1000;

        $.ajax
        ({
            type: "POST",
            dataType: "json",
            url: _sBaseAddress + "/Employee/EmployeeWorkingStatusChange",
            traditional: true,
            data: JSON.stringify({ sEmpIDs: sEmpIDs, nts: tsv }),
            contentType: "application/json; charset=utf-8",
            success: function (data) {
                var oEmployees = [];
                oEmployees = jQuery.parseJSON(data);
                if (oEmployees[0].ErrorMessage == "" && oEmployees.length > 0) {
                    for (var i = 0; i < oEmployees.length; i++) {
                        oEmployees[i].WorkingStatus=6;
                        oEmployees[i].IsResigned=true;
                        oEmployees[i].WorkingStatusInString="Resigned";
                        $('#tblEmployeeSettlements').datagrid('updateRow', { index: nIndexes[i], row: oEmployees[i] });
                    }
                    alert("Discontinued Successfully !");
                }
                else {
                    alert(oEmployees[0].ErrorMessage);
                }
            },
            error: function (xhr, status, error) {
                alert(error);
            }
        });
    });

