        ParentConsumptionUnitID : $('#cboGroup').val()
    }

    $("#winMenu").icsWindow("close");
    $("#winMenu input").val("");
    $("#winMenu select").val(0);

    $.ajax({
        type: "POST",
        dataType: "json",
