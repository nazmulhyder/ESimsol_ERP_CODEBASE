                alert("Invalid Machine For Dispo No: "+oRows[i].ExeNo+" !"); return;
            }
            oRows[i].StartTime = new Date(oRows[i].StartTimeSt); //icsdateformat(oFabricSizingPlan.StartTimeSt);
