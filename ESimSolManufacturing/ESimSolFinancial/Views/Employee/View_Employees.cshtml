        function AddEmployeeBranchs()
        {
            //debugger;
            var oEmployee= $('#tblEmployees').datagrid('getSelected');
            if(oEmployee==null || oEmployee.EmployeeID<=0)
            {
                alert("Please select a item from list!");
                return;
            }
