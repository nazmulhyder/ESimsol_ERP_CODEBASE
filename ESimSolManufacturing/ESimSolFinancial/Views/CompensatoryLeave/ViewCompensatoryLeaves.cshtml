<script type="text/javascript">
    $(document).ready(function ()
    {
        debugger;
        var oAURolesMappings=@Html.Raw(new System.Web.Script.Serialization.JavaScriptSerializer().Serialize(ViewBag.AuthorizationRolesMappings));
        var oTimeCards= @Html.Raw(new System.Web.Script.Serialization.JavaScriptSerializer().Serialize(@ViewBag.TimeCards));
        var oEmployeeCategorys=@Html.Raw(new System.Web.Script.Serialization.JavaScriptSerializer().Serialize(@ViewBag.EmployeeCategorys));
        var oHRMShifts=@Html.Raw(new System.Web.Script.Serialization.JavaScriptSerializer().Serialize(@ViewBag.HRMShifts));
        var oLeaveHeads=@Html.Raw(new System.Web.Script.Serialization.JavaScriptSerializer().Serialize(@ViewBag.LeaveHeads));
        $("#progressbar").progressbar({ value: 0 });
        $("#progressbarParent").hide();
        $("#cboLeaveHead").data("LeaveHeads",oLeaveHeads);
        $("#cboEditedShift").data("HRMShifts",oHRMShifts);
        $("#cboPrintFormat").data("TimeCards",oTimeCards);
        $("#cboEmpCategory").data("EmployeeCategorys",oEmployeeCategorys);
        RefreshAdvSearch();
        RefreshControl();
    });





    function updateProgress()
    {
        var value =$('#progressbar').progressbar('getValue');
        if (value < 96){
            value += Math.floor(Math.random() * 5);
            $('#progressbar').progressbar('setValue', value);
        }
    }

    function hideShow(miliseconds)
    {
        $("#progressbarParent").hide();
    }

    $("#cboCompensatoryType").change(function(e){

        if( parseInt($("#cboCompensatoryType").val())==2)
        {
            $("#lblDutyLeave").text("Leave Date");
            $("#dtHideShow").hide();
            $("#cboPrintFormat").val(0);

            DynamicRefreshList([], 'tblAttendanceDaily');
        }
        else
        {
            
            $("#dtHideShow").show();
            $("#lblDutyLeave").text("Duty Date");
            $('#dtCompLeaveDate').datebox('setValue', icsdateformat(new Date()));
            $("#cboLeaveHead").val(0);
            $("#cboPrintFormat").val(0);
 
            DynamicRefreshList([], 'tblAttendanceDaily');
        }
    });

    function RefreshControl()
    {
        $('#dtEndDate').datebox('setValue', icsdateformat(new Date()));

        $('#dtCompLeaveDate').datebox('setValue', icsdateformat(new Date()));

        $("#txtLocation_Colection").data("LocationIDs","");
        $("#txtDepartment_Collection").data("DepartmentIDs","");
        $("#txtEmployee_Collection").data("EmployeeIDs","");
        $("#txtBusinessUnit_Collection").data("BusinessUnitIDs","");
        LoadComboTimeCard();
    }
    function LoadComboTimeCard()
    {
        debugger;
        var oTimeCards= $("#cboPrintFormat").data("TimeCards");
        var oHRMShifts= $("#cboEditedShift").data("HRMShifts");
        var oLeaveHeads=  $("#cboLeaveHead").data("LeaveHeads");
        $("#cboPrintFormat").icsLoadCombo({
            List: oTimeCards,
            OptionValue: "MOCID",
            DisplayText: "TimeCardName",
            InitialValue:"--TimeCard--"
        });

        $("#cboEditedShift").icsLoadCombo({
            List:  oHRMShifts,
            OptionValue: "ShiftID",
            DisplayText: "Name",
        });
        $("#cboLeaveHead").icsLoadCombo({
            List:  oLeaveHeads,
            OptionValue: "LeaveHeadID",
            DisplayText: "Name",
        });
    }

    function RefreshObject()
    {
        var oHCMSearchObj={
            BUIDs : $("#txtBusinessUnit_Collection").data("BusinessUnitIDs"),
            MOCID: parseInt($('#cboPrintFormat').val()),
            EmployeeIDs : $("#txtEmployee_Collection").data("EmployeeIDs"),
            LocationIDs : $("#txtLocation_Colection").data("LocationIDs"),
            DepartmentIDs : $("#txtDepartment_Collection").data("DepartmentIDs"),
            EndDate : $('#dtEndDate').datebox('getValue'),
            CompLeaveDate:$('#dtCompLeaveDate').datebox('getValue'),
            BlockIDs:$("#txtEmployeeBlock").data("BlockIDs"),
            GroupIDs:$("#txtEmployeeGroup").data("GroupIDs"),
            PrintFormatInt:parseInt($("#cboPrintFormat").val()),
            SalarySchemeIDs:$("#txtSalaryScheme").data("SalarySchemeIDs"),
            AttendanceSchemeIDs:$("#txtAttendanceScheme").data("AttendanceSchemeIDs"),
            ShiftIDs: $("#txtShift").data("ShiftIDs"),
            EmployeeTypeIDs: $("#txtEmployeeType").data("EmployeeTypeIDs"),
            DesignationIDs:$("#txtDesignation").data("DesignationIDs"),
            IsJoiningDate:$("#chkJoiningDate").is(':checked'),
            JoiningStartDate:$('#txtJoiningStartDate').datebox('getValue'),
            JoiningEndDate:$('#txtJoiningEndDate').datebox('getValue'),
            AuthenticationNo:$.trim($('#txtAuthenticationNo').val()),
            CategoryID:parseInt($("#cboEmpCategory").val()),
            CompensatoryLeaveType:parseInt($("#cboCompensatoryType").val())
        }
        return oHCMSearchObj;
    }

    function ValidateInput()
    {
        debugger;
        var dtDateTo = $('#dtEndDate').datebox('getValue');
        var dEndDate =  new Date(dtDateTo);

        var bIsJoiningDate=$("#chkJoiningDate").is(':checked');
        if(bIsJoiningDate)
        {
            var sJoiningStDate=$('#txtJoiningStartDate').datebox('getValue');
            var sJoiningEnDate=$('#txtJoiningEndDate').datebox('getValue');
            var dJoiningStDate=new Date(sJoiningStDate);
            var dJoiningEnDate=new Date(sJoiningEnDate);
            if(dJoiningEnDate<dJoiningStDate)
            {
                alert("Start Date Mustbe Smaller than End Date");
                return false;
            }
        }
        if(parseInt($('#cboPrintFormat').val())<=0)
        {
            alert("Please Select One Time Card");
            return false;
        }
        var sBlockIDs=$("#txtEmployeeBlock").data("BlockIDs");
        var sGroupIDs=$("#txtEmployeeGroup").data("GroupIDs");
        var sLocationIDs=$("#txtLocation_Colection").data("LocationIDs");
        var sDepartmentIds=$("#txtDepartment_Collection").data("DepartmentIDs");
        var sEmployeeIDs=$("#txtEmployee_Collection").data("EmployeeIDs");
        var sBusinessUnitIds=$("#txtBusinessUnit_Collection").data("BusinessUnitIDs");
        var sSalarySchemeIDs=$("#txtSalaryScheme").data("SalarySchemeIDs");
        var sAttendanceSchemeIDs=$("#txtAttendanceScheme").data("AttendanceSchemeIDs");
        var sShiftIDs= $("#txtShift").data("ShiftIDs");
        var sEmployeeTypeIDs= $("#txtEmployeeType").data("EmployeeTypeIDs");
        var sDesignationIDs=$("#txtDesignation").data("DesignationIDs");
        var sAuthenticationNo=$.trim($('#txtAuthenticationNo').val());
        var nEmpCategoryID=parseInt($("#cboEmpCategory").val());
        if(sEmployeeIDs == "" && sLocationIDs=="" && sDepartmentIds=="" && sBusinessUnitIds=="" && sBlockIDs=="" && sGroupIDs=="" && sSalarySchemeIDs==""&&sAttendanceSchemeIDs=="" && sShiftIDs=="" && sEmployeeTypeIDs=="" && sDesignationIDs=="" && bIsJoiningDate==false && sAuthenticationNo=="" && nEmpCategoryID<=0)
        {
            alert("Please enter at lest one searching criteria with date !");
            return false;
        }
        return true;
    }


    $("#btnCommit").click(function(e){
        
        if(parseInt($('#cboPrintFormat').val())<=0)
        {
            alert("Please Select One Time Card");
            return;
        }
        if(parseInt($("#cboCompensatoryType").val())==1)
        {
            if(parseInt($('#cboLeaveHead').val())<=0)
            {
                alert("Please Select One Leave Head");
                return;
            }
        }
       

        var oAttendanceDailyV2s=$("#tblAttendanceDaily").datagrid('getChecked');
        if(oAttendanceDailyV2s.length<=0)
        {
            alert("Please Select At least One Employee");
            return;
        }
        var sMOCAIDs="";
        for(var i=0;i<oAttendanceDailyV2s.length;i++)
        {
            sMOCAIDs=sMOCAIDs+oAttendanceDailyV2s[i].MOCAID+",";
        }
        if(sMOCAIDs.length>0)
        {
            sMOCAIDs=sMOCAIDs.substring(0,sMOCAIDs.length-1);
        }
        var dtDutyLeaveDate=$('#dtCompLeaveDate').datebox('getValue');
        if(parseInt($("#cboCompensatoryType").val())==2)
        {
            dtDutyLeaveDate=$('#dtEndDate').datebox('getValue');
        }
        var oAttendanceDailyV2=
            {
                MOCAIDs:sMOCAIDs,
                LeaveHeadID:parseInt($("#cboLeaveHead").val()),
                LeaveAssignDate:dtDutyLeaveDate,
                CompensatoryLeaveType:parseInt($("#cboCompensatoryType").val())
            }

        $.ajax({
            type: "POST",
            dataType: "json",
            url: sessionStorage.getItem("BaseAddress") + "/CompensatoryLeave/AssignLeave",
            traditional: true,
            data: JSON.stringify(oAttendanceDailyV2),
            contentType: "application/json; charset=utf-8",

            success: function (data)
            {
                sFeedBackMessage = jQuery.parseJSON(data);

                if (sFeedBackMessage=="Data Commit successfully")
                {
                  
                    for(var i=0;i<oAttendanceDailyV2s.length;i++)
                    {
                        var SelectedRowIndex=$('#tblAttendanceDaily').datagrid('getRowIndex',oAttendanceDailyV2s[i]);
                        $('#tblAttendanceDaily').datagrid('deleteRow',SelectedRowIndex);
                    }
                    alert("Data Committed Successfully");
                   
                }
                else
                {
                    alert(sFeedBackMessage);
                }
            },

            error: function (xhr, status, error)
            {
                alert(error);
            }
        });
    });

    function Search()
    {
        debugger;
        if(!ValidateInput())return;
        var oHCMSearchObj=RefreshObject();
        $.ajax({
            type: "POST",
            dataType: "json",
            url: sessionStorage.getItem("BaseAddress") + "/CompensatoryLeave/AdvanceSearch",
            traditional: true,
            data: JSON.stringify(oHCMSearchObj),
            contentType: "application/json; charset=utf-8",

            success: function (data)
            {
                var  oAttendanceDailys= [];
                oAttendanceDailys = jQuery.parseJSON(data);

                if (oAttendanceDailys.length>0 && oAttendanceDailys[0].ErrorMessage=="")
                {
                    DynamicRefreshList(oAttendanceDailys, 'tblAttendanceDaily');
                }
                else
                {
                    alert(oAttendanceDailys[0].ErrorMessage);
                    DynamicRefreshList([], 'tblAttendanceDaily');
                }
            },

            error: function (xhr, status, error)
            {
                alert(error);
            }
        });
    }





    /*---------------- emp Search Start click----------------*/


    $("#btnEmployee").click(function () {
        var sCodeName = $.trim($("#txtEmployee_Collection").val());
        if(sCodeName==''){alert("Please enter name or code to search.");$("#txtEmployee_Collection").focus(); return;}
        SearchEmployeeByText(sCodeName);
    });

    /*-------------Business Unit Picker----------------*/
    $("#btnBusinessUnitPicker_Collection").click(function(e){
        BusinessUnitPicker();
    });

    $("#txtBusinessUnit_Collection").keypress(function(e){
        if (e.which == 13)//enter=13
        {
            BusinessUnitPicker();
        }
    });

    function BusinessUnitPicker()
    {
        var oBusinessUnit={
