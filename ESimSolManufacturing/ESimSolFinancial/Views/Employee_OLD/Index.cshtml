@model IEnumerable< ESimSol.BusinessObjects.Employee>
@{
    ViewBag.Title = "Employee List";
}

<h2>Employee List from DB server by ICS WCF server using IVC </h2>

<table border="1" cellpadding="0" cellspacing="0">
    @foreach (ESimSol.BusinessObjects.Employee item in Model)
    {
    <tr>
        <td> @Html.DisplayFor(modelItem => item.Name) </td>
        <td> @Html.DisplayFor(modelItem => item.Address) </td>
        <td> @Html.DisplayFor(modelItem => item.ContactNumber) </td>
    </tr>
    }
    
</table>
