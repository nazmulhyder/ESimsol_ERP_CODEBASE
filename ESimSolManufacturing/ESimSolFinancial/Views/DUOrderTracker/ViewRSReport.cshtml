<script type="text/javascript">
    var _oDUOrderRS = null;
    var _oDUOrderRSs = [];
    var _sBaseAddress = "";
    var _oAuthorizationRolesMapping = [];
    var _sContractorIds="";
    var _sMenuID=0;
    var _sProductIDs="";
    var _sPTUIDs="";
    var _nReportLevel=0;
    $(document).ready(function() 
    {
        debugger;

        _sBaseAddress = @Html.Raw(new System.Web.Script.Serialization.JavaScriptSerializer().Serialize(HttpContext.Current.Session[SessionInfo.BaseAddress]));
       
        _oDUOrderRSs = @Html.Raw(new System.Web.Script.Serialization.JavaScriptSerializer().Serialize(Model));
        _oCompareOperators = @Html.Raw(new System.Web.Script.Serialization.JavaScriptSerializer().Serialize(ViewBag.CompareOperators));
        _nBUID = @Html.Raw(new System.Web.Script.Serialization.JavaScriptSerializer().Serialize(ViewBag.BUID));
        @*_oMktPersons = @Html.Raw(new System.Web.Script.Serialization.JavaScriptSerializer().Serialize(ViewBag.MktPersons));*@
      
        _oOrderTypes = @Html.Raw(new System.Web.Script.Serialization.JavaScriptSerializer().Serialize(ViewBag.OrderTypes));
      


        $("#progressbar").progressbar({ value: 0 });
        $("#progressbarParent").hide();
        $(".lblLoadingMessage").hide();
        //alert("Pick");
        RefreshSearchComboBoxControls();
    });
    
    function updateProgress() {
        var value =$('#progressbar').progressbar('getValue');
        if (value < 96){
            value += Math.floor(Math.random() * 10);
            $('#progressbar').progressbar('setValue', value);
        }
    }
    function hideShow(miliseconds) {
        $("#progressbarParent").hide();
    }
   
    
    $('#btnSearch').click(function(e)
    {
        $(".lblLoadingMessage").show();
        var checkDate = CheckFromAndToDateValidation("cboRSDate", "txtFromRSDate", "txtToRSDate");
        if (!checkDate) {
            alert("Start date must be greater than end date.");
            return false;
        }

        var checkDate = CheckFromAndToDateValidation("cboQCDate", "txtFromQCDate", "txtToRSDate");
        if (!checkDate) {
            alert("Start date must be greater than end date.");
            return false;
        }

        var ncboRSDate = parseInt($('#cboRSDate').val());
        var dFromDODate   = $('#txtFromRSDate').datebox('getValue');
        var dToDODate   = $('#txtToRSDate').datebox('getValue');

        var cboQCDate = parseInt($('#cboQCDate').val());
        var dFromQCDate   = $('#txtFromQCDate').datebox('getValue');
        var dToQCDate   = $('#txtToRSDate').datebox('getValue');
        var nOrderType=parseInt($("#cboOrderType").val());

       
        var sRSNo=$("#txtRSNo").val();

        var sParams =    ncboRSDate + "~" +
                         dFromDODate + "~" +
                         dToDODate + "~" +
                         cboQCDate + "~" +
                         dFromQCDate + "~" +
                         dToQCDate + "~" +
                         _sProductIDs + "~" +
                         nOrderType+ "~" +
                         _sPTUIDs+ "~" +
                         sRSNo+ "~" +
                         _nBUID;

        var oDUOrderRS = {
            ErrorMessage : sParams,
            BUID : parseInt(sessionStorage.getItem("BUID")),
            ReportLevelType : _nReportLevel
        };
        sessionStorage.setItem("Params", sParams);
        $.ajax({
            type: "POST",
            dataType: "JSON",
            url: sessionStorage.getItem("BaseAddress")+  "/DUOrderTracker/AdvanchSearchRS",
            data:  JSON.stringify(oDUOrderRS),
            contentType: "application/json; charset=utf-8",
            success: function (data) {
                var  oDUOrderRSs = data;
             
                if (oDUOrderRSs != null) {
                    if(oDUOrderRSs.length>0)
                    {
                        RefreshList(oDUOrderRSs)
                          RefreshSummary(_nReportLevel);
                        $("#winAdvSearchOrderTracker").icsWindow('close');
                    }
                    else
                    {
                        alert("Data not found!!");
                    }
                }
                $(".lblLoadingMessage").hide();
            },
            error: function (xhr, status, error) {
                alert(error);
            }
        });
    });

    function RefreshList(oDUOrderRSs)
    {
        var nIndex= parseInt(sessionStorage.getItem("SelectedRowIndex"));
      
        if(_nReportLevel==0)
        {
            DynamicRefreshList(oDUOrderRSs, "tblColorView");
            $('#tblColorView').datagrid('selectRow',nIndex);
        }
        else if(_nReportLevel==1)
        {
            DynamicRefreshList(oDUOrderRSs, "tblOrderView");
            $('#tblOrderView').datagrid('selectRow',nIndex);
        }
        else if(_nReportLevel==2)
        {
            DynamicRefreshList(oDUOrderRSs, "tblPIView");
            $('#tblPIView').datagrid('selectRow',nIndex);
        }
        else if(_nReportLevel==3)
        {
            DynamicRefreshList(oDUOrderRSs, "tblProductView");
            $('#tblProductView').datagrid('selectRow',nIndex);
        }
    }
    function RefreshSummary(nReportLevel)
    {
        debugger;
        if(nReportLevel==0)
        {
            var oRows = $('#tblColorView').datagrid('getRows');
        }
        else  if(nReportLevel==3)
        {
            var oRows = $('#tblProductView').datagrid('getRows');
        }
        if(oRows.length>0)
        {
            var nQty_RS = 0,nUnManagedQty=0,nManagedQty=0,nPro_Pipline=0,nFreshDyedYarnQty=0,nGainLoss=0,nBagCount=0,nDeliveryQty=0,nStockInHand=0;
            for(var i=0;i<oRows.length;i++)
            {
                nQty_RS = parseFloat(nQty_RS)+parseFloat(oRows[i].Qty_RS);
                nUnManagedQty = parseFloat(nUnManagedQty)+parseFloat(oRows[i].UnManagedQty);
                nManagedQty = parseFloat(nManagedQty)+parseFloat(oRows[i].ManagedQty);
                nPro_Pipline = parseFloat(nPro_Pipline)+parseFloat(oRows[i].Pro_Pipline);
                nFreshDyedYarnQty = parseFloat(nFreshDyedYarnQty)+parseFloat(oRows[i].FreshDyedYarnQty);
                nGainLoss = parseFloat(nGainLoss)+parseFloat(oRows[i].GainLoss);
                nBagCount = parseFloat(nBagCount)+parseFloat(oRows[i].BagCount);
                nDeliveryQty = parseFloat(nDeliveryQty)+parseFloat(oRows[i].DeliveryQty);
                nStockInHand = parseFloat(nStockInHand)+parseFloat(oRows[i].StockInHand);
               
            }
             
            nQty_RS = parseFloat(nQty_RS).toFixed(2);
            nUnManagedQty = parseFloat(nUnManagedQty).toFixed(2);
            nManagedQty = parseFloat(nManagedQty).toFixed(2);
            nPro_Pipline = parseFloat(nPro_Pipline).toFixed(2);
            nFreshDyedYarnQty = parseFloat(nFreshDyedYarnQty).toFixed(2);
            nGainLoss = parseFloat(nGainLoss).toFixed(2);
            nBagCount = parseFloat(nBagCount).toFixed(2);
            nDeliveryQty = parseFloat(nDeliveryQty).toFixed(2);
            nStockInHand = parseFloat(nStockInHand).toFixed(2);

            debugger
            var oRow =
            {
                ProductName : 'Total',
                Qty_RS:nQty_RS,
                UnManagedQty:nUnManagedQty,
                ManagedQty:nManagedQty,
                Pro_Pipline:nPro_Pipline,
                FreshDyedYarnQty:nFreshDyedYarnQty,
                GainLoss:nGainLoss,
                DeliveryQty:nDeliveryQty,
                BagCount:nBagCount,
                StockInHand:nStockInHand
               
            }
            if(nReportLevel==0)
            {
                $('#tblColorView').datagrid('appendRow',oRow); 
            }
            else  if(nReportLevel==3)
            {
                $('#tblProductView').datagrid('appendRow',oRow); 
            }
        } 
    }


    $("#btnAdvanceSearch_ColorView").click(function ()
    {
        $(".lblLoadingMessage").hide();
        $("#winAdvSearchOrderTracker").icsWindow("open", "Advance Search");
        _nReportLevel=0;
    });
    $("#btnAdvanceSearch_OrderView").click(function () {
        $(".lblLoadingMessage").hide();
        $("#winAdvSearchOrderTracker").icsWindow("open", "Advance Search");
        _nReportLevel=1;
    });
    $("#btnAdvanceSearchLC_PIView").click(function () {
        $(".lblLoadingMessage").hide();
        $("#winAdvSearchOrderTracker").icsWindow("open", "Advance Search");
        _nReportLevel=2;
    });
    $("#btnAdvanceSearch_ProductView").click(function () {
        $(".lblLoadingMessage").hide();
        $("#winAdvSearchOrderTracker").icsWindow("open", "Advance Search");
        _nReportLevel=3;
    });

    $('#btnAdvSearchReset').click(function(e)
    {
        $("#winAdvSearchOrderTracker input").not("input[type='button']").val("");
        RefreshSearchComboBoxControls();
        _sContractorIds ="";
        _sProductIDs="";
        _sPTUIDs="";
        $('#txtFromRSDate').datebox({ disabled : true });
        $('#txtToRSDate').datebox({ disabled : true });
        $('#txtFromRSDate').datebox('setValue', icsdateformat(new Date()));
        $('#txtToRSDate').datebox('setValue', icsdateformat(new Date()));

        $('#txtFromQCDate').datebox({ disabled : true });
        $('#txtToRSDate').datebox({ disabled : true });
        $('#txtFromQCDate').datebox('setValue', icsdateformat(new Date()));
        $('#txtToRSDate').datebox('setValue', icsdateformat(new Date()));
      
        $('#cboRSDate').val(0);
        $('#cboQCDate').val(0);
        $('#cboOrderType').val(0);
        
    });

    $('#btnAdvSearchClose').click(function(e)
    {
        $("#winAdvSearchOrderTracker").icsWindow('close');
    });

    function RefreshSearchComboBoxControls()
    {
        $("#cboRSDate, #cboQCDate").icsLoadCombo({
            List: _oCompareOperators,
            OptionValue: "id",
            DisplayText: "Value"
        });

        $("#cboOrderType").icsLoadCombo({
            List: _oOrderTypes,
            OptionValue: "OrderType",
            DisplayText: "ShortName"
        });

    }

    function CheckFromAndToDateValidation(OperationComboId, FromDateId, ToDateId) {
        $("#" + OperationComboId).parent().parent().parent().find("select").removeClass("errorFieldBorder");
        var nCboVal = $("#" + OperationComboId).val();
        if (parseInt(nCboVal) == 5 || parseInt(nCboVal) == 6) {
            var fromDate = $("#" + FromDateId).datebox("getValue");
            var toDate = $("#" + ToDateId).datebox("getValue");
            if (new Date(fromDate) > new Date(toDate)) {
                $("#" + ToDateId).focus();
                $("#" + OperationComboId).addClass("errorFieldBorder");
                $(".lblLoadingMessage").hide();
                return false;
            } else {
                $("#" + OperationComboId).removeClass("errorFieldBorder");
                return true;
            }
        } else {
            return true;
        }
    }


    function DateActions_DO() {
        DynamicDateActions("cboRSDate", "txtFromRSDate", "txtToRSDate");
    }
    function DateActions_PIDate() {
        DynamicDateActions("cboQCDate", "txtFromQCDate", "txtToRSDate");
    }
    
    //////////////////
    /*..........Pick Order ............. */

    $("#btnPickOrderNo").click(function () {
        debugger;
        if (parseInt($("#cboOrderType").val())<=0 ) {

            alert("Please Select Order Type.");
            $('#cboOrderType').focus();
            $('#cboOrderType').addClass("errorFieldBorder");
            return;
        }
        else{
            $('#cboOrderType').removeClass("errorFieldBorder");
        }

        var oPSDetail = {
            OrderType:$('#cboOrderType').val(),
            OrderNo:$('#txtOrderNo').val(),
            LabDipDetailID:0,
            ContractorName:_sContractorIDs
        };

        GetOrders(oPSDetail);
    });

    $("#txtOrderNoAdv").keydown(function (e) {
        var nkeyCode = e.keyCode || e.which;
        if(nkeyCode==13){
            var sorderNo=$.trim($("#txtOrderNoAdv").val());
            if (parseInt($("#cboOrderType").val())<=0 ) {

                alert("Please Select Order Type.");
                $('#cboOrderType').focus();
                $('#cboOrderType').addClass("errorFieldBorder");
                return;
            }
            else{
                $('#cboOrderType').removeClass("errorFieldBorder");
            }

            var oPSDetail = {
                OrderType:$('#cboOrderType').val(),
                OrderNo:$('#txtOrderNoAdv').val(),
                LabDipDetailID:0
            };
            GetOrders(oPSDetail);
        }
        else if(nkeyCode==8){
            $("#txtOrderNoAdv").val("");

        }
    });
    function GetOrders(oPSDetail)
    {
        var obj = {
            BaseAddress: _sBaseAddress,
            Object: oPSDetail,
            ControllerName: "DUPSchedule",
            ActionName: "GetsDyeingOrder",
            IsWinClose: false
        };
        $("#progressbar").progressbar({ value: 0 });
        $("#progressbarParent").show();
        var intervalID = setInterval(updateProgress, 250);
        $.icsDataGets(obj, function (response) {
            clearInterval(intervalID);
            $("#progressbarParent").hide();
            if (response.status && response.objs.length > 0) {
                if (response.objs[0].DODID > 0) {
                    debugger;
                    var tblColums = [];

                    var oColumn = { field: "OrderNo", title: "OrderNo", width: 100, align: "left" };tblColums.push(oColumn);

                    oColumn = { field: "OrderQty", title: "OrderQty", width: 75, align: "right",formatter:formatPrice };tblColums.push(oColumn);
                    oColumn = { field: "Qty_Pro", title: "Prod Qty", width: 75, align: "right",formatter:formatPrice };tblColums.push(oColumn);
                    oColumn = { field: "Qty_RS", title: "Yet To Pro", width: 75, align: "right" };tblColums.push(oColumn);
                    oColumn = { field: "ProductName", title: "Yarn", width: 225, align: "left" };tblColums.push(oColumn);
