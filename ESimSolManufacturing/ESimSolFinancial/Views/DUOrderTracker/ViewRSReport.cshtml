    function formatPrice(val,row)
    {
   
        val=parseFloat(val);
        var test = val.toFixed(2);
        if (val < 0)
        {
            test=(-1*test);
        }
        var tests = addComma(test);
        if (val < 0)
        {
            return '<span style="color:red;">('+tests+')</span>';
        }
        else 
        {
            return tests;
        }

    }


    function addComma(nStr)
    {

        nStr += '';
        x = nStr.split('.');
        x1 = x[0];
        x2 = x.length > 1 ? '.' + x[1] : '';
        var rgx = /(\d+)(\d{3})/;
        while (rgx.test(x1)) {
            x1 = x1.replace(rgx, '$1' + ',' + '$2');
        }
        return x1 + x2;
    }
