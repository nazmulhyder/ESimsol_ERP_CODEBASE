        var oOrderSheets= $('#tblOrderSheets').datagrid('getRows');
        sessionStorage.setItem("OrderSheets", JSON.stringify(oOrderSheets));
        sessionStorage.setItem("SelectedRowIndex", SelectedRowIndex);
        sessionStorage.setItem("OrderSheetHeader", "Order Sheet Revise Mgt");
        sessionStorage.setItem("BUID", _nBUID);
        sessionStorage.setItem("ORSType", _nORSType);
        sessionStorage.setItem("ProductNature", _nProductNature);
        sessionStorage.setItem("BackLinkForReviseMgt", window.location.href);
        window.location.href = _sBaseAddress+  "/OrderSheet/ViewOrderSheetMgt?id="+oOrderSheet.OrderSheetID;
    }

    function UndoRequest()
    {
        debugger;
        var oOrderSheet = $('#tblOrderSheets').datagrid('getSelected');
        if(oOrderSheet==null || oOrderSheet.OrderSheetID<=0)
        {
            alert("Please select a item from list!");
            return;
        }else if(parseInt(oOrderSheet.OrderSheetStatus)!=1)
        {
            alert("Please select only Requested  item from list");
            return;
        }
        if (!confirm("Confirm to Undo Request?")) return ;
        oOrderSheet.ActionTypeExtra="UndoRequest";
        StatusChange(oOrderSheet,"Undo Request");
    }

    function UndoApprove()
    {
        //debugger;
        var oOrderSheet = $('#tblOrderSheets').datagrid('getSelected');
        if(oOrderSheet==null || oOrderSheet.OrderSheetID<=0)
        {
            alert("Please select a item from list!");
            return;
        }

        else if(oOrderSheet.OrderSheetStatus==2)
        {
            if (!confirm("Confirm to Undo Approved?")) return ;
            oOrderSheet.ActionTypeExtra="UndoApprove";
            StatusChange(oOrderSheet,"UndoApprove");
        }
        else
        {
            alert("Please select only Approved from list");
            return;
        }
    }

    function StatusChange(oOrderSheet,sParamIdentifier)
    {
        var SelectedRowIndex=$('#tblOrderSheets').datagrid('getRowIndex',oOrderSheet);
        $.ajax({
            type: "POST",
            dataType: "json",
            url : _sBaseAddress+"/OrderSheet/ChangeStatus",
            traditional: true,
            data:  JSON.stringify(oOrderSheet),
            contentType: "application/json; charset=utf-8",
            success: function (data) {
                debugger;
                var _oOrderSheet= jQuery.parseJSON(data);
                if (_oOrderSheet.ErrorMessage=="" || _oOrderSheet.ErrorMessage== null)
                {
                    if(sParamIdentifier=="Undo Request")
                    {
                        alert("Successfully Undo the Request !!");
                    }
                    else  if(sParamIdentifier=="UndoApprove")
                    {
                        alert("Successfully Undo Approved.");
                    }
                    else  if(sParamIdentifier=="RequestRevise")
                    {
                        alert("Successfully Request to Revised.");
                    }
                    $('#tblOrderSheets').datagrid('updateRow',{index: SelectedRowIndex,	row: _oOrderSheet});
                }
                else
                {
                    alert(_oOrderSheet.ErrorMessage);
                }
            },
            error: function (xhr, status, error)
            {
                alert(error);
            }
        });


    }

    function WaitForApproval()
    {
        $.ajax
        ({
            type: "GET",
            dataType: "json",
            url : _sBaseAddress+  "/OrderSheet/WaitingSearch",
            data: { nStatusExtra:1, OrderType:_nOrderType },
            contentType: "application/json; charset=utf-8",
            success: function (data) {
                //debugger;
                var _oOrderSheets = jQuery.parseJSON(data);
                if (_oOrderSheets.length>0)
                {

                    var  data=_oOrderSheets;
                    data={"total":""+data.length+"","rows":data};
                    $('#tblOrderSheets').datagrid('loadData',data);

                }
                else
                {
                    alert("Data Not found");
                }
            },
            error: function (xhr, status, error)
            {
                alert(error);
            }
        });
    }


