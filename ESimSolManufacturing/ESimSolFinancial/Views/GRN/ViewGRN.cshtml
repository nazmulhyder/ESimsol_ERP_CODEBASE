 function IsItemExist(nRefObjectID)
 {
     var oGRNDetails = $('#tblGRNDetail').datagrid('getRows');
     if(oGRNDetails!=null)
     {
         for(var i=0; i<oGRNDetails.length; i++)
         {
             if(parseInt(oGRNDetails[i].RefObjectID)===parseInt(nRefObjectID))
             {
                 return true;
             }
         }
     }
     return false;
 }

