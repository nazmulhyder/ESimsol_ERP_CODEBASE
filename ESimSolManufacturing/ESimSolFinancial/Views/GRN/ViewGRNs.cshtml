                                <tr>
                                    <td style="width:40%"><input type="text" style="width: 100%" id="txtInvocieNo" /></td>
                                    <td style="width:20%; text-align:right">Status :</td>
                                    <td style="width:40%"><select id="cboGRNStatus" style="width:100%"></select></td>
