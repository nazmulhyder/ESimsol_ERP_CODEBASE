    $("#btnUpdateMRIR").click(function (){
        debugger;
        var oGRN= $('#tblGRNs').datagrid('getSelected');
        if(oGRN==null || oGRN.GRNID<=0)
        {
            alert("Please select a item from list!");
            return;
        }

        $('#txtMRIRNo').val(oGRN.MRIRNo);
        $('#txtMRIRDate').datebox('setValue',oGRN.MRIRDateSt);
        $("#winMRIRInfoChange").icsWindow('open', "Update MRIR Info");
    })

    $('#btnMRIRInfoChangeSave').click(function(e){
        debugger;
        if($('#txtMRIRNo').val()==null || $('#txtMRIRNo').val()=="")
        {
            alert("Please type MRIR No.");
            return;
        }
        if($('#txtMRIRDate').datebox('getValue')==null ||$('#txtMRIRDate').datebox('getValue')=="")
        {
            alert("Please Select  MRIR Date.");
            return;
        }
        var oGRN= $('#tblGRNs').datagrid('getSelected');
        if(oGRN==null || oGRN.GRNID<=0)
        {
            alert("Please select a item from list!");
            return;
        }
        var SelectedRowIndex=$('#tblGRNs').datagrid('getRowIndex',oGRN);
        oGRN.MRIRNo =$('#txtMRIRNo').val();
        oGRN.MRIRDate =$('#txtMRIRDate').datebox('getValue');

        $.ajax({
            type: "POST",
            dataType: "json",
            url: _sBaseAddress+"/GRN/UpdateMRIRInfo",
            data:  JSON.stringify(oGRN),
            contentType: "application/json; charset=utf-8",
            success: function (data) {
                debugger;
                var oGRN = jQuery.parseJSON(data);
                if (oGRN.ErrorMessage == '' || oGRN.ErrorMessage == null)
                {
                    alert("Successfully Updated MRIR Info.");
                    $('#tblGRNs').datagrid('updateRow',{index: SelectedRowIndex,row: oGRN});
                }
                else
                {
                    alert(oGRN.ErrorMessage);
                }
                $("#winMRIRInfoChange").icsWindow('close');
            },
            error: function (xhr, status, error)
            {
                alert(xhr+'~' +status+'~'+error);
            }
        });


    });

    $('#btnMRIRInfoChangeClose').click(function(e){
        $("#winMRIRInfoChange").icsWindow('close');
    })

</script>