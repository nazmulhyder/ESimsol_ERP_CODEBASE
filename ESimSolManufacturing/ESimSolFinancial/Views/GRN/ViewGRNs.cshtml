    <div id="winMRIRInfoChange" class=" easyui-window winclass" title="Edit MRIR info" style="width:400px" data-options="modal:true,closed:true,collapsible:false,minimizable:false,maximizable:false,closable:false">
        <fieldset>
            <table border="0" cellspacing="2" cellpadding="2" style="width:100%; font-size: 12px">
                <tr>
