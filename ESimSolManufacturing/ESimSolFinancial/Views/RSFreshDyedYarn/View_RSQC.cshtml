                debugger;
                var oResult = data;
                if(oResult.length<=0){
                    alert("No data found");
                }
                else{
                    $('#tblRSWise').data('FDPs',oResult)
                    DynamicRefreshList(oResult,'tblRSWise');
                    $("#winAdvSearch").icsWindow("close");
                    $(".lblLoadingMessage").hide();
