    function IsQtyValid()
    {
        var oLots= $('#tblDUPSLot').datagrid('getRows');
        var oDetails= $('#tblDUPSDetail').datagrid('getRows');
        debugger;

        if(oLots==null || oLots.length<=0)
        {
            return true;
        }
        debugger;
        if(oLots!=null && oDetails!=null && oDetails.length>0)
        {
            for(var i=0; i<oLots.length; i++)
            {
                if(parseFloat(oLots[i].Qty) < 0){
                    alert("Lot Qty Shoulb Be Greater Than Zero! [Lot No: "+ oLots[0].Lot +"]"); return false;}
            }

            for(var i=0; i<oDetails.length; i++)
            {
                
                if(oDetails[i].Qty != GetTotalLotQty(oDetails[i].DODID))
                {
                  
                    alert("Order Qty & Lot Qty Is Not Same [Order No: "+ oDetails[0].OrderNo +"] Qty: "); return false;}
            }
        }

        return true;
    }
    function GetTotalLotQty()
    {
        var nLotQty=0;
        var oLots= $('#tblDUPSLot').datagrid('getRows');
        for(var i=0; i<oLots.length; i++)
        {
            nLotQty= parseFloat(nLotQty) + parseFloat(oLots[i].Qty);
        }
        return nLotQty;
    }
    function GetTotalLotQty(nDODID)
    {
        var nLotQty=0;
        var oLots= $('#tblDUPSLot').datagrid('getRows');
        for(var i=0; i<oLots.length; i++)
        {
            if(oLots[i].DODID==nDODID)
                nLotQty= parseFloat(nLotQty) + parseFloat(oLots[i].Qty);
        }
        return nLotQty;
    }
    

