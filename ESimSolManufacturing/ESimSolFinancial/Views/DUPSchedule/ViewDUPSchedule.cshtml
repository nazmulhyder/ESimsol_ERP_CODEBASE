    function GenerateRowSpan(oDatas)
    {
        var oMCell=null;
        var nIndex=0;
        var nRowSpan=0;
        var oMCells=[];
        for (var i=0; i<oDatas.length;i++)
        {
            nRowSpan=0;
            for(var j=i; j<oDatas.length;j++)
            {
                if(oDatas[i]==oDatas[j]){++nRowSpan;}
            }
            nRowSpan = (nRowSpan >= 0) ? nRowSpan : 0;
            oMCell={
                ParentID : 0,
                Index : nIndex,
                RowSpan : nRowSpan
            };
            oMCells.push(oMCell);
            nIndex = nIndex + nRowSpan;
            i=nIndex-1;
        }
        return oMCells;
    }
