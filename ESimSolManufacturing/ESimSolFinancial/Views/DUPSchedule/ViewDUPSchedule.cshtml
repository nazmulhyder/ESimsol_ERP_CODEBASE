    function isAllSame(field,data)
    {
        var isSame=true;
        for(var i=0; i<data.length;i++)
        {
            for(var j=0; j<data.length;j++)
            {
                if(data[i][field]!=data[j][field])
                    isSame=false;
            }
        }
        return isSame;
    }
