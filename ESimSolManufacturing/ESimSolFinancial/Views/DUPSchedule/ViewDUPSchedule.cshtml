    function SchedulePaste(oDUPScheduleOld)
    {
        if(_oDUPSCopy==null|| _oDUPSCopy.DUPScheduleID<=0){alert("Nothing found for paste."); return;}
        //debugger;
        if(oDUPScheduleOld.DUPScheduleID<=0)
        {
            var dStartDate =$('#txtStartDate').datebox('getValue');
            var startTime= $('#tpStartTime').timespinner('getValue');
        }
        else{
            var dStartDate =oDUPScheduleOld.EndDateSt;
            var startTime=oDUPScheduleOld.EndTimeTS;
        }
        var sTime=startTime.split(':');
        var hStartTime= parseFloat(sTime[0]);
        var mStartTime= parseFloat(sTime[1])+1;
        dStartDate = new Date(dStartDate);
        dStartDate.setHours(hStartTime);
        dStartDate.setMinutes(mStartTime);

        var dEndtDate =new Date();
        dEndtDate= new Date(dEndtDate.setHours(dStartDate.getHours() +oDUPScheduleOld.TimeDiffInHours));
        dEndtDate= new Date(dEndtDate.setMinutes( dEndtDate.getMinutes() +1));

        _oDUPSCopy.MachineID =oDUPScheduleOld.MachineID,
        _oDUPSCopy.LocationID =oDUPScheduleOld.LocationID,
        _oDUPSCopy.StartTime	= dStartDate;
        _oDUPSCopy.EndTime=dEndtDate;

        var nts=(new Date()).getTime()/1000;
        $.ajax({
            type: "POST",
            dataType: "json",
            url: _sBaseAddress + "/DUPSchedule/ScheduleInTimePeriod",
            data: JSON.stringify({oDUPSchedule:_oDUPSCopy, nts: nts}),
            contentType: "application/json; charset=utf-8",
            success: function(data)
            {
                //debugger;
                nCount = jQuery.parseJSON(data);
                if (nCount>0)
                {
                    if(confirm("Are you sure to increase time from next all schedules."))
                    {
                        _oDUPSCopy.IsIncreaseTime=true;
                        SaveProductionSchedule(_oDUPSCopy, false);
                    }
                }
                else if(nCount==0)
                {
                    _oDUPSCopy.IsIncreaseTime=false;
                    SaveProductionSchedule(_oDUPSCopy, false);
                }

            },
            error: function(xhr, status, error)
            {
                alert(error);
            }
        });

    }

