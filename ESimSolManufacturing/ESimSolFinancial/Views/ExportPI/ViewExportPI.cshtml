            {
                oExportPIDetail.Amount =  parseFloat(parseFloat(oExportPIDetail.Qty)/ parseInt($('#txtRateUnit').val())) * parseFloat(oExportPIDetail.UnitPrice);
                oExportPIDetail.AmountSt = formatPrice(parseFloat(parseFloat(oExportPIDetail.Qty)/ parseInt($('#txtRateUnit').val())) * parseFloat(oExportPIDetail.UnitPrice));
                $('#tblExportPIDetail').datagrid('updateRow',{index: editIndex,	row: oExportPIDetail});
            }
            SetTotal();
            editIndex = undefined;
            return true;
        }
        else
        {
            return false;
        }
    }
