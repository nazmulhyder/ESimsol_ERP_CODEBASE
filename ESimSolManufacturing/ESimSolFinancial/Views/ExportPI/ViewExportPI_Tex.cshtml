                        ProductName :oDUDBs[i].ProductName,
                        ModelReferenceID:0,
                        ModelReferenceName:'',
                        Amount:oDUDBs[i].Amount
                    };
                    $('#tblExportPIDetail').datagrid('appendRow',oExportPIDetail);
