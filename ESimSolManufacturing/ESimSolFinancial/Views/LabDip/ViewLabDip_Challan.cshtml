        function SaveChallanNo(oLabDip,nSelectedRowIndex)
        {
            $.ajax({
                type: "POST",
                dataType: "json",
                url : _sBaseAddress+"/LabDip/Save_Challan",
                traditional: true,
                data:  JSON.stringify(oLabDip),
                contentType: "application/json; charset=utf-8",
                success: function (data) {
                    debugger;
                    var oLB= jQuery.parseJSON(data);
                    if (oLB.ErrorMessage=="" || oLB.ErrorMessage==null)
                    {
                        oLabDip=oLB;
                        $('#tblLabDip').datagrid('updateRow', { index: nSelectedRowIndex, row: oLB });
                      
                    }
                    else
                    {
                        alert(oLB.ErrorMessage);
                    }
                },
                error: function (xhr, status, error)
                {
                    alert(error);
                }
            });
        }
