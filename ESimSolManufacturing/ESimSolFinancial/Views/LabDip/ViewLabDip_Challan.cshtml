            <fieldset class="actionfieldsetstyle">
                <legend>Actions : </legend>
                <a id="btnOkPrintFormat2" href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-ok" plain="true">Ok</a>
                <a id="btnClosePrintFormat2" href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-cancel" plain="true">Close</a>
            </fieldset>
        </div>
        <div id="winDivChallan" style="width:260px;" class="easyui-window winstyle" title="Add Challan No" data-options="modal:true,closed:true,collapsible:false,minimizable:false,maximizable:false,closable:false">
            <fieldset>
                <legend>Lab dip Challan</legend>
                <table style="width:100%;">
                    <tr>
                        <td style="text-align:right;">
                            Challan No :
                        </td>
